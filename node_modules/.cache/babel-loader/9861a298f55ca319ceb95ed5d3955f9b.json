lbar\">\n      <el-select v-model=\"query.orderType\" class=\"query-item\" style=\"width: 130px\" placeholder=\"订单类型\" clearable @clear=\"handleQuery\">\n        <el-option value=\"0\" label=\"客户订单\" />\n        <el-option value=\"1\" label=\"客户订单详单\" />\n        <el-option value=\"2\" label=\"采购订单\" />\n        <el-option value=\"3\" label=\"采购订单详单\" />\n        <el-option value=\"4\" label=\"领料单\" />\n        <el-option value=\"5\" label=\"领料单详单\" />\n      </el-select>\n      <el-input v-model.trim=\"query.name\" class=\"query-item\" style=\"width: 180px\" placeholder=\"订单扩展属性名称\" clearable @clear=\"handleQuery\" />\n      <el-select v-model=\"query.valueType\" class=\"query-item\" style=\"width: 180px\" placeholder=\"订单扩展属性类型\" clearable @clear=\"handleQuery\">\n        <el-option value=\"0\" label=\"字符串\" />\n        <el-option value=\"1\" label=\"数值\" />\n        <el-option value=\"2\" label=\"日期\" />\n      </el-select>\n      <el-button class=\"tool tool-query\" type=\"primary\" icon=\"el-icon-search\" @click=\"handleQuery\">查询</el-button>\n      <el-button v-if=\"access.create.allow\" class=\"tool tool-create\" type=\"primary\" icon=\"vue-icon-create\" @click=\"handleCreate\">添加</el-button>\n      <el-button v-if=\"access.delete.allow\" class=\"tool tool-delete\" type=\"danger\" icon=\"vue-icon-delete\" @click=\"handleDeletes\">批量删除</el-button>\n    </div>\n\n    <el-table ref=\"listTable\" v-loading=\"loading.list\" v-adaptive=\"{ bottomOffset: 55 }\" height=\"200px\" :data=\"datas\" :default-sort=\"sort\" border fit highlight-current-row @sort-change=\"handleSort\">\n      <el-table-column type=\"selection\" align=\"center\" width=\"35\" />\n      <el-table-column label=\"序号\" type=\"index\" align=\"center\" width=\"65\" fixed>\n        <template slot-scope=\"scope\">\n          <span>{{ (page.current - 1) * page.size + scope.$index + 1 }}</span>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"订单类型\" prop=\"orderType\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"200\" show-overflow-tooltip>\n        <template slot-scope=\"{row}\">\n          <span v-if=\"row.orderType== 0\">客户订单</span>\n          <span v-if=\"row.orderType== 1\">客户订单详单</span>\n          <span v-if=\"row.orderType== 2\">采购订单</span>\n          <span v-if=\"row.orderType== 3\">采购订单详单</span>\n          <span v-if=\"row.orderType== 4\">领料单</span>\n          <span v-if=\"row.orderType== 5\">领料单详单</span>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"订单扩展属性名称\" prop=\"name\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"200\" show-overflow-tooltip />\n      <el-table-column label=\"订单扩展属性描述\" prop=\"description\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"left\" show-overflow-tooltip />\n      <el-table-column label=\"订单扩展属性值数据类型\" prop=\"valueType\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"200\" show-overflow-tooltip>\n        <template slot-scope=\"{row}\">\n          <span v-if=\"row.valueType== 0\">字符串</span>\n          <span v-if=\"row.valueType== 1\">数值</span>\n          <span v-if=\"row.valueType== 2\">日期</span>\n        </template>\n      </el-table-column>\n      <el-table-column fixed=\"right\" label=\"操作\" align=\"center\" width=\"140\">\n        <template slot-scope=\"{row}\">\n          <el-tooltip v-if=\"access.detail.allow\" class=\"item\" effect=\"dark\" content=\"详情\" placement=\"top-end\">\n            <el-button type=\"primary\" plain class=\"button-operate button-detail\" size=\"mini\" @click=\"handleDetail(row)\"><i class=\"vue-icon-detail\" /></el-button>\n          </el-tooltip>\n          <el-tooltip v-if=\"access.update.allow\" class=\"item\" effect=\"dark\" content=\"编辑\" placement=\"top-end\">\n            <el-button type=\"primary\" plain class=\"button-operate button-update\" size=\"mini\" @click=\"handleUpdate(row)\"><i class=\"vue-icon-update\" /></el-button>\n          </el-tooltip>\n          <el-tooltip v-if=\"access.delete.allow\" class=\"item\" effect=\"dark\" content=\"删除\" placement=\"top-end\">\n            <el-button type=\"danger\" plain class=\"button-operate button-delete\" size=\"mini\" @click=\"handleDelete(row)\"><i class=\"vue-icon-delete\" /></el-button>\n          </el-tooltip>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <pagination :hidden=\"page.total==0\" :total=\"page.total\" :page.sync=\"page.current\" :limit.sync=\"page.size\" @pagination=\"getDatas\" />\n\n    <el-dialog v-loading=\"loading.detail\" custom-class=\"dialog-fullscreen dialog-detail\" :title=\"detail.dialog.title\" :visible.sync=\"detail.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\">\n      <el-form ref=\"formDetail\" label-position=\"right\" :label-width=\"detail.dialog.labelWidth\">\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"订单类型\">\n              <span v-if=\"detail.models.orderType==0\">客户订单</span>\n              <span v-if=\"detail.models.orderType==1\">客户订单详单</span>\n              <span v-if=\"detail.models.orderType==2\">采购订单</span>\n              <span v-if=\"detail.models.orderType==3\">采购订单详单</span>\n              <span v-if=\"detail.models.orderType==4\">领料单</span>\n              <span v-if=\"detail.models.orderType==5\">领料单详单</span>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"订单扩展属性名称\">\n              {{ detail.models.name }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"订单扩展属性描述\">\n              {{ detail.models.description }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"订单扩展属性描述\">\n              <span v-if=\"detail.models.valueType==0\">字符串</span>\n              <span v-if=\"detail.models.valueType==1\">数值</span>\n              <span v-if=\"detail.models.valueType==2\">日期</span>\n            </el-form-item>\n          </el-col>\n        </el-row>\n      </el-form>\n    </el-dialog>\n\n    <el-dialog custom-class=\"dialog-fullscreen dialog-create\" :title=\"create.dialog.title\" :visible.sync=\"create.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\" :close-on-click-modal=\"false\">\n      <el-form ref=\"formCreate\" :rules=\"create.rules\" :model=\"create.models\" label-position=\"right\" :label-width=\"create.dialog.labelWidth\">\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"订单类型\" prop=\"orderType\">\n              <el-select v-model=\"create.models.orderType\" clearable>\n                <el-option label=\"客户订单\" value=\"0\" />\n                <el-option label=\"客户订单详单\" value=\"1\" />\n                <el-option label=\"采购订单\" value=\"2\" />\n                <el-option label=\"采购订单详单\" value=\"3\" />\n                <el-option label=\"领料单\" value=\"4\" />\n                <el-option label=\"领料单详单\" value=\"5\" />\n              </el-select>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"订单扩展属性名称\" prop=\"name\">\n              <el-input v-model=\"create.models.name\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :sl=\"24\">\n            <el-form-item label=\"订单扩展属性描述\" prop=\"description\">\n              <el-input v-model=\"create.models.description\" type=\"textarea\"/>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"扩展属性数据类型\" prop=\"valueType\">\n              <el-radio-group v-model=\"create.models.valueType\">\n                <el-radio :label=\"0\">字符串</el-radio>\n                <el-radio :label=\"1\">数值</el-radio>\n                <el-radio :label=\"2\">日期</el-radio>\n              </el-radio-group>\n            </el-form-item>\n          </el-col>\n        </el-row>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"create.dialog.visible = false\">取消</el-button>\n        <el-button type=\"primary\" @click=\"createData()\">提交</el-button>\n      </div>\n    </el-dialog>\n\n    <el-dialog v-loading=\"loading.update\" custom-class=\"dialog-fullscreen dialog-update\" :title=\"update.dialog.title\" :visible.sync=\"update.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\" :close-on-click-modal=\"false\">\n      <el-form ref=\"formUpdate\" :rules=\"update.rules\" :model=\"update.models\" label-position=\"right\" :label-width=\"update.dialog.labelWidth\">\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"订单类型\" prop=\"orderType\">\n              <el-select v-model=\"update.models.orderType\" clearable>\n                <el-option v-for=\"item in orderType\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n              </el-select>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物扩展属性名称\" prop=\"name\">\n              <el-input v-model=\"update.models.name\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :sl=\"24\">\n            <el-form-item label=\"货物扩展属性描述\" prop=\"description\">\n              <el-input v-model=\"update.models.description\" type=\"textarea\"/>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"扩展属性数据类型\" prop=\"valueType\">\n              <el-radio-group v-model=\"update.models.valueType\">\n                <el-radio :label=\"0\">字符串</el-radio>\n                <el-radio :label=\"1\">数值</el-radio>\n                <el-radio :label=\"2\">日期</el-radio>\n              </el-radio-group>\n            </el-form-item>\n          </el-col>\n        </el-row>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"update.dialog.visible = false\">取消</el-button>\n        <el-button type=\"primary\" @click=\"updateData()\">提交</el-button>\n      </div>\n    </el-dialog>\n  </div>\n\n</template>\n\n<script>\nimport adaptive from '@/directive/el-table'\nimport setRule from '@/utils/form-validate'\nimport Pagination from '@/components/Pagination'\nimport * as ordersProperty from '@/api/order-manage/order-property'\n\nexport default {\n  name: 'OrderProperty',\n  components: { Pagination },\n  directives: { adaptive },\n  data() {\n    return {\n      access: this.$store.getters.access['OrderBusiness']['OrderProperty'],\n      datas: null,\n      orderType: [\n        { id: 0, name: '客户订单' },\n        { id: 1, name: '客户订单详单' },\n        { id: 2, name: '采购订单' },\n        { id: 3, name: '采购订单详单' },\n        { id: 4, name: '领料单' },\n        { id: 5, name: '领料单详单' }\n      ],\n      valueType: [\n        { id: 0, name: '字符串' },\n        { id: 1, name: '数值' },\n        { id: 2, name: '日期' }\n      ],\n      query: { orderType: null, name: null, valueType: null },\n      page: { total: 0, current: 1, size: 20 },\n      sort: { prop: 'sort', order: 'ascending' },\n      detail: {\n        dialog: { title: '订单扩展属性', visible: false, labelWidth: '150px' },\n        models: { orderType: null, name: null, description: null, valueType: null }\n      },\n      create: {\n        dialog: { title: '添加订单扩展属性', visible: false, labelWidth: '150px' },\n        models: { orderType: null, name: null, description: null, valueType: null },\n        rules: {\n          orderType: setRule('订单类型', [{ selected: true }]),\n          name: setRule('订单扩展属性名称', [{ required: true }, { length: [0, 20] }]),\n          description: setRule('货物扩展属性描述', [{ length: [0, 100] }]),\n          valueType: setRule('货物扩展属性值数据类型', [{ selected: true }])\n        }\n      },\n      update: {\n        dialog: { title: '编辑货物颜色信息', visible: false, labelWidth: '150px' },\n        models: { id: null, orderType: null, name: null, description: null, valueType: null },\n        rules: {\n          orderType: setRule('订单类型', [{ selected: true }]),\n          name: setRule('货物扩展属性名称', [{ required: true }, { length: [0, 20] }]),\n          description: setRule('货物扩展属性描述', [{ length: [0, 100] }]),\n          valueType: setRule('货物扩展属性值数据类型', [{ selected: true }])\n        }\n      },\n      loading: { list: true, export: false, detail: false, update: false },\n      sortOrders: ['ascending', 'descending']\n    }\n  },\n  created() {\n    this.getDatas()\n  },\n  methods: {\n    getDatas() {\n      this.loading.list = true\n      ordersProperty.getList(this.query, this.page, this.sort).then(response => {\n        this.datas = response.data.items\n        this.page.total = response.data.total\n        this.loading.list = false\n      }).catch(reject => {\n        this.loading.list = false\n      })\n    },\n    handleQuery() {\n      this.page.current = 1\n      this.getDatas()\n    },\n    handleSort(data) {\n      this.sort.prop = data.prop\n      this.sort.order = data.order\n      this.getDatas()\n    },\n    handleDetail(row) {\n      // 若列表数据展示了全部属性，则可直接拷贝列表数据\n      this.detail.models = Object.assign({}, row)\n      this.detail.dialog.visible = true\n      this.loading.detail = true\n      // api.get(row.id).then(response => {\n      //   this.detail.models = response.data\n      //   this.loading.detail = false\n      // }).catch(reject => {\n      //   this.loading.detail = false\n      // })\n      this.loading.detail = false\n    },\n    handleCreate() {\n      this.create.dialog.visible = true\n    },\n    createData() {\n      this.$refs['formCreate'].validate((valid) => {\n        if (!valid) return false\n        ordersProperty.create(this.create.models).then(response => {\n          // 为方便连续添加，create对话框不关闭\n          // 需要清空的表单，手动清空，至少清空一个必填项，防止点击两遍\n          this.create.models.name = ''\n          this.create.models.description = ''\n          // 重新获取数据\n          this.getDatas()\n        }).catch(reject => {\n        })\n      })\n    },\n    handleRankup(row) {\n      this.loading.list = true\n      ordersProperty.rankup(row.id).then(response => {\n        this.getDatas()\n        this.loading.list = false\n      }).catch(reject => {\n        this.loading.list = false\n      })\n    },\n    handleUpdate(row) {\n      //  若列表数据展示了全部属性，则可直接拷贝列表数据用于编辑\n      this.update.models = Object.assign({}, row)\n      this.update.dialog.visible = true\n      this.loading.update = true\n      // goodsType.get(row.id).then(response => {\n      //   this.update.models = response.data\n      //   this.loading.update = false\n      // }).catch(reject => {\n      //   this.loading.update = false\n      // })\n      this.$nextTick(() => {\n        this.$refs['formUpdate'].clearValidate()\n      })\n      this.loading.update = false\n    },\n    updateData() {\n      this.$refs['formUpdate'].validate((valid) => {\n        if (!valid) return false\n        const tempData = Object.assign({}, this.update.models)\n        ordersProperty.update(tempData).then(response => {\n          // 重新获取数据\n          this.getDatas()\n          this.update.dialog.visible = false\n        }).catch(reject => {\n        })\n      })\n    },\n    handleDelete(row) {\n      this.deleteData([row.id])\n    },\n    handleDeletes(row) {\n      if (this.$refs.listTable.selection.length === 0) {\n        this.$message({ type: 'warning', message: '请在下面表格的左侧勾选需要删除的数据', duration: 4000 })\n        return false\n      }\n      const ids = []\n      for (const item of this.$refs.listTable.selection) {\n        ids.push(item.id)\n      }\n      this.deleteData(ids)\n    },\n    deleteData(ids) {\n      this.$confirm('数据删除后，不可恢复, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        ordersProperty.del(ids).then(response => {\n          // 重新获取数据\n          this.getDatas()\n        }).catch(reject => {\n        })\n      })\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n</style>\n"],"sourceRoot":"src/views/order-manage"}]}