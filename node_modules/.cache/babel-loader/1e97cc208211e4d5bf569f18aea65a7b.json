;;AACO,SAASE,SAAT,CAAmBH,GAAnB,EAAwB;AAC7B,SAAO,sBAAQ;AACbnB,IAAAA,GAAG,EAAE,mCADQ;AAEbC,IAAAA,MAAM,EAAE,MAFK;AAGbC,IAAAA,IAAI,EAAE;AACJiB,MAAAA,GAAG,EAAEA,GADD;AAEJC,MAAAA,OAAO,EAAE;AAFL;AAHO,GAAR,CAAP;AAQD,C,CAED;;;AACO,SAASG,UAAT,CAAoBJ,GAApB,EAAyB;AAC9B,SAAO,sBAAQ;AACbnB,IAAAA,GAAG,EAAE,mCADQ;AAEbC,IAAAA,MAAM,EAAE,MAFK;AAGbC,IAAAA,IAAI,EAAE;AACJiB,MAAAA,GAAG,EAAEA,GADD;AAEJC,MAAAA,OAAO,EAAE;AAFL;AAHO,GAAR,CAAP;AAQD","sourcesContent":["import request from '@/utils/request'\n\n// 获取列表数据（查询、翻页）\nexport function getList(query, page, sort, status) {\n  return request({\n    url: '/residualApplication/list',\n    method: 'post',\n    data: {\n      page: { current: page.current, size: page.size },\n      sort: { property: sort.prop, direction: sort.order },\n      query: { ...query },\n      status: status\n    }\n  })\n}\n\n// 获取列表数据（查询、翻页）\nexport function getListApproval(query, page, sort, status) {\n  return request({\n    url: '/residualApproval/list',\n    method: 'post',\n    data: {\n      page: { current: page.current, size: page.size },\n      sort: { property: sort.prop, direction: sort.order },\n      query: { ...query },\n      status: status\n    }\n  })\n}\n\n// 获取列表数据（查询、翻页）\nexport function getListFinal(query, page, sort, status) {\n  return request({\n    url: '/residualFinalReview/list',\n    method: 'post',\n    data: {\n      page: { current: page.current, size: page.size },\n      sort: { property: sort.prop, direction: sort.order },\n      query: { ...query },\n      status: status\n    }\n  })\n}\n\n// 添加数据\nexport function create(application, goods, quality, unitPriceNew) {\n  return request({\n    url: '/residualApplication/create',\n    method: 'post',\n    data: {\n      application: application,\n      goods: goods,\n      quality: quality,\n      unitPriceNew: unitPriceNew\n    }\n  })\n}\n\n// 导出Excel\nexport function exportExcel(query, page, sort) {\n  return request({\n    url: '/goods/export',\n    method: 'post',\n    responseType: 'blob',\n    data: {\n      page: { current: page.current, size: page.size },\n      sort: { property: sort.prop, direction: sort.order },\n      query: { ...query }\n    }\n  })\n}\n\n// 审批通过\nexport function approvalTrue(ids) {\n  return request({\n    url: '/residualApproval/examine',\n    method: 'post',\n    data: {\n      ids: ids,\n      approve: true\n    }\n  })\n}\n\n// 审批退回\nexport function approvalFalse(ids) {\n  return request({\n    url: '/residualApproval/examine',\n    method: 'post',\n    data: {\n      ids: ids,\n      approve: false\n    }\n  })\n}\n\n// 终审通过\nexport function finalTrue(ids) {\n  return request({\n    url: '/residualFinalReview/finalExamine',\n    method: 'post',\n    data: {\n      ids: ids,\n      approve: true\n    }\n  })\n}\n\n// 终审退回\nexport function finalFalse(ids) {\n  return request({\n    url: '/residualFinalReview/finalExamine',\n    method: 'post',\n    data: {\n      ids: ids,\n      approve: false\n    }\n  })\n}\n"]}]}