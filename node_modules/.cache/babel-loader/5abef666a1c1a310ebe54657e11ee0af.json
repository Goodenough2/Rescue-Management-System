,QAAA,EAAA;AAAA,WAAA,CAAA;AAHA;AAHA,OAFA;AAWA,MAAA,OAAA,EAAA;AAAA,QAAA,IAAA,EAAA,IAAA;AAAA,QAAA,MAAA,EAAA,KAAA;AAAA,QAAA,MAAA,EAAA,KAAA;AAAA,QAAA,MAAA,EAAA;AAAA;AAXA,KAAA;AAaA,GAlBA;AAmBA,EAAA,OAnBA,qBAmBA;AACA,SAAA,oBAAA;AACA,GArBA;AAsBA,EAAA,OAtBA,qBAsBA;AACA,SAAA,OAAA;AACA,GAxBA;AAyBA,EAAA,OAAA,EAAA;AACA,IAAA,OADA,qBACA;AACA,UAAA,SAAA,GAAA,EAAA;AACA,MAAA,SAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,SAAA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,SAAA,GAAA,SAAA;AACA,KALA;AAMA,IAAA,oBANA,kCAMA;AAAA;;AACA,UAAA,MAAA,GAAA,CAAA;AACA,MAAA,gBAAA,CAAA,IAAA,CAAA,MAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,iBAAA,GAAA,QAAA,CAAA,IAAA;AACA,OAFA,EAEA,KAFA,CAEA,UAAA,MAAA,EAAA,CACA,CAHA;AAIA,KAZA;AAaA,IAAA,OAbA,qBAaA;AAAA;;AACA,WAAA,KAAA,CAAA,YAAA,EAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,CAAA,KAAA,EAAA,OAAA,KAAA;AACA,QAAA,KAAA,CAAA,OAAA,CAAA,MAAA,CAAA,MAAA,CAAA,MAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA;AACA;AACA,UAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,SAAA,GAAA,EAAA,CAHA,CAIA;;AACA,UAAA,MAAA,CAAA,QAAA;AACA,SANA,EAMA,KANA,CAMA,UAAA,MAAA,EAAA,CACA,CAPA;AAQA,OAVA;AAWA;AAzBA;AAzBA,C","sourcesContent":["<template>\r\n  <div class=\"app-container list\">\r\n    <el-form ref=\"formCreate\" :rules=\"create.rules\" :model=\"create.models\" label-position=\"right\" :label-width=\"create.dialog.labelWidth\">\r\n      <el-form-item label=\"货物码\" prop=\"goodsCode\">\r\n        <el-input v-model=\"create.models.goodsCode\" />\r\n      </el-form-item>\r\n      <el-form-item label=\"目的位置码\" prop=\"locationCode\">\r\n        <el-input v-model=\"create.models.locationCode\" />\r\n      </el-form-item>\r\n      <el-form-item label=\"操作类型\" prop=\"storeOperateTypeId\">\r\n        <el-select v-model=\"create.models.storeOperateTypeId\" clearable>\r\n          <el-option v-for=\"item in storeOperateTypes\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"operate()\">提交</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport adaptive from '@/directive/el-table'\r\nimport setRule from '@/utils/form-validate'\r\nimport * as storeOperateType from '@/api/log-manage/store-operate-type'\r\nimport * as goods from '@/api/goods-manage/goods'\r\n\r\nexport default {\r\n  name: 'BusinessOperation',\r\n  // components: { Pagination },\r\n  directives: { adaptive },\r\n  data() {\r\n    return {\r\n      storeOperateTypes: null,\r\n      create: {\r\n        dialog: { title: '货物入库', visible: true, labelWidth: '100px' },\r\n        models: { goodsCode: null, locationCode: null, storeOperateTypeId: null },\r\n        rules: {\r\n          goodsCode: setRule('货物编号', [{ required: true }, { length: [0, 32] }]),\r\n          locationCode: setRule('目的地编号', [{ required: true }, { length: [0, 100] }]),\r\n          storeOperateTypeId: setRule('操作类型', [{ selected: true }])\r\n        }\r\n      },\r\n      loading: { list: true, export: false, detail: false, update: false }\r\n    }\r\n  },\r\n  created() {\r\n    this.getStoreOperateTypes()\r\n  },\r\n  mounted() {\r\n    this.getPath()\r\n  },\r\n  methods: {\r\n    getPath() {\r\n      let goodsCode = ''\r\n      goodsCode = this.$route.query.goodsCode\r\n      this.create.models.goodsCode = goodsCode\r\n    },\r\n    getStoreOperateTypes() {\r\n      const ioType = 2\r\n      storeOperateType.list(ioType).then(response => {\r\n        this.storeOperateTypes = response.data\r\n      }).catch(reject => {\r\n      })\r\n    },\r\n    operate() {\r\n      this.$refs['formCreate'].validate((valid) => {\r\n        if (!valid) return false\r\n        goods.operate(this.create.models).then(response => {\r\n          // 为方便连续添加，create对话框不关闭\r\n          // 需要清空的表单，手动清空，至少清空一个必填项，防止点击两遍\r\n          this.create.models.goodsCode = ''\r\n          // 重新获取数据\r\n          this.getDatas()\r\n        }).catch(reject => {\r\n        })\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n  .app-container{\r\n    width: 40%;\r\n    margin: 150px auto;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/views/business-operation"}]}