,CAAA,eAAA,GAAA,EAAA;AACA,KAhCA;AAiCA,IAAA,WAjCA,yBAiCA;AACA,WAAA,IAAA,CAAA,OAAA,GAAA,CAAA;AACA,WAAA,QAAA;AACA,KApCA;AAqCA,IAAA,UArCA,sBAqCA,IArCA,EAqCA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,IAAA,CAAA,IAAA;AACA,WAAA,IAAA,CAAA,KAAA,GAAA,IAAA,CAAA,KAAA;AACA,WAAA,QAAA;AACA,KAzCA;AA0CA,IAAA,YA1CA,wBA0CA,GA1CA,EA0CA;AACA;AACA,WAAA,MAAA,CAAA,MAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,GAAA,CAAA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,OAAA,GAAA,IAAA,CAHA,CAIA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,KArDA;AAsDA,IAAA,WAtDA,uBAsDA,GAtDA,EAsDA;AACA,UAAA,aAAA,GAAA,GAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,qBADA;AAEA,QAAA,KAAA,EAAA;AACA,UAAA,aAAA,EAAA;AADA;AAFA,OAAA;AAMA,KA9DA;AA+DA,IAAA,YA/DA,0BA+DA;AAAA;;AACA,WAAA,OAAA,CAAA,MAAA,GAAA,IAAA;AACA,MAAA,oBAAA,CAAA,WAAA,CAAA,KAAA,KAAA,EAAA,KAAA,IAAA,EAAA,KAAA,IAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,YAAA,IAAA,GAAA,IAAA,IAAA,CAAA,CAAA,QAAA,CAAA,CAAA;AACA,YAAA,QAAA,GAAA,UAAA;AACA,YAAA,KAAA,GAAA,QAAA,CAAA,aAAA,CAAA,GAAA,CAAA;;AACA,QAAA,KAAA,CAAA,MAAA,GAAA,YAAA;AAAA,UAAA,GAAA,CAAA,eAAA,CAAA,KAAA,CAAA,IAAA;AAAA,SAAA;;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,QAAA;AACA,QAAA,KAAA,CAAA,KAAA,CAAA,OAAA,GAAA,MAAA;AACA,QAAA,KAAA,CAAA,IAAA,GAAA,GAAA,CAAA,eAAA,CAAA,IAAA,CAAA;AACA,QAAA,QAAA,CAAA,IAAA,CAAA,WAAA,CAAA,KAAA;AACA,QAAA,KAAA,CAAA,KAAA;AACA,QAAA,QAAA,CAAA,IAAA,CAAA,WAAA,CAAA,KAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,MAAA,GAAA,KAAA;AACA,OAZA,EAYA,KAZA,CAYA,UAAA,MAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,MAAA,GAAA,KAAA;AACA,OAdA;AAeA;AAhFA;AA5BA,C","sourcesContent":["<template>\n  <div class=\"app-container list\">\n    <div class=\"toolbar\">\n      <!--<el-select v-model=\"query.goodsCategoryId\" class=\"query-item\" style=\"width: 120px\" placeholder=\"货物种类\" clearable @clear=\"handleQuery\">-->\n      <!--<el-option v-for=\"item in goodsCategories\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />-->\n      <!--</el-select>-->\n      <el-cascader\n        v-model=\"query.goodsCategoryId\"\n        placeholder=\"请选择货物类别\"\n        :options=\"goodsCategories\"\n        :props=\"{ expandTrigger: 'hover', checkStrictly: true }\"\n        @change=\"handleChange\"\n      />\n      <el-select v-model=\"query.unitId\" class=\"query-item\" style=\"width: 120px\" placeholder=\"货物单位\" clearable @clear=\"handleQuery\">\n        <el-option v-for=\"item in goodsUnits\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n      </el-select>\n      <el-select v-model=\"query.operatorId\" class=\"query-item\" style=\"width: 120px\" placeholder=\"操作人\" clearable @clear=\"handleQuery\">\n        <el-option v-for=\"item in operators\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n      </el-select>\n      <el-button class=\"tool tool-query\" type=\"primary\" icon=\"el-icon-search\" @click=\"handleQuery\">查询</el-button>\n      <el-button v-if=\"access.export.allow\" class=\"tool tool-export\" :loading=\"loading.export\" type=\"primary\" icon=\"vue-icon-excel\" @click=\"handleExport\">导出</el-button>\n    </div>\n\n    <el-table ref=\"listTable\" v-loading=\"loading.list\" v-adaptive=\"{ bottomOffset: 55 }\" height=\"200px\" :data=\"datas\" :default-sort=\"sort\" border fit highlight-current-row @sort-change=\"handleSort\">\n      <el-table-column label=\"序号\" type=\"index\" align=\"center\" width=\"65\" fixed>\n        <template slot-scope=\"scope\">\n          <span>{{ (page.current - 1) * page.size + scope.$index + 1 }}</span>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"货物种类\" prop=\"goodsCategory.name\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />\n      <el-table-column label=\"货物单位\" prop=\"goodsUnit.name\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />\n      <el-table-column label=\"数量\" prop=\"quality\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />\n      <el-table-column label=\"运输设备上数量\" prop=\"qualityOnTemDev\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\n      <el-table-column label=\"单元格中数量\" prop=\"qualityOnTemCell\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\n      <el-table-column label=\"容器中数量\" prop=\"qualityOnTemVes\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\n      <el-table-column label=\"同一种类锁定总数量\" prop=\"qualityLocked\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\n      <el-table-column label=\"未验货数量\" prop=\"inspectionresultNull\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\n      <el-table-column label=\"验货未通过数量\" prop=\"inspectionresultNo\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\n      <el-table-column label=\"验货通过数量\" prop=\"inspectionresultYes\" :sort-orders=\"sortOrders\" align=\"center\" show-overflow-tooltip />\n      <!--<el-table-column label=\"更新时间\" prop=\"updateTime\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" show-overflow-tooltip />-->\n      <!--<el-table-column label=\"操作人\" prop=\"operator.name\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />-->\n      <el-table-column fixed=\"right\" label=\"操作\" align=\"center\" width=\"100\">\n        <template slot-scope=\"{row}\">\n          <el-tooltip v-if=\"access.detail.allow\" class=\"item\" effect=\"dark\" content=\"详情\" placement=\"top-end\">\n            <el-button type=\"primary\" plain class=\"button-operate button-detail\" size=\"mini\" @click=\"handleDetail(row)\"><i class=\"vue-icon-detail\" /></el-button>\n          </el-tooltip>\n          <el-tooltip v-if=\"access.detail.allow\" class=\"item\" effect=\"dark\" content=\"该类货物信息\" placement=\"top-end\">\n            <el-button type=\"primary\" plain class=\"button-operate button-detail\" size=\"mini\" @click=\"handleGoods(row)\"><i class=\"el-icon-zoom-in\" /></el-button>\n          </el-tooltip>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <pagination :hidden=\"page.total==0\" :total=\"page.total\" :page.sync=\"page.current\" :limit.sync=\"page.size\" @pagination=\"getDatas\" />\n\n    <el-dialog v-loading=\"loading.detail\" custom-class=\"dialog-fullscreen dialog-detail\" :title=\"detail.dialog.title\" :visible.sync=\"detail.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\">\n      <el-form ref=\"formDetail\" label-position=\"right\" :label-width=\"detail.dialog.labelWidth\">\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物类别\">\n              {{ detail.models.goodsCategory.name }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物单位\">\n              {{ detail.models.goodsUnit.name }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"数量\">\n              {{ detail.models.quality }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"运输设备上总数量\">\n              {{ detail.models.qualityOnTemDev }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"单元格上总数量\">\n              {{ detail.models.qualityOnTemCell }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"容器中总数量\">\n              {{ detail.models.qualityOnTemVes }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"同一货物锁定总数量\">\n              {{ detail.models.qualityLocked }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"未验货数量\">\n              {{ detail.models.inspectionresultNull }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"验货不通过数量\">\n              {{ detail.models.inspectionresultNo }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"验货通过数量\">\n              {{ detail.models.inspectionresultYes }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"更新时间\">\n              {{ detail.models.updateTime }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"操作人员\">\n              {{ detail.models.operator.userName }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n      </el-form>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport adaptive from '@/directive/el-table'\nimport Pagination from '@/components/Pagination'\nimport * as goodsCategoryClassification from '@/api/goods-manage/goods-category-classification'\nimport * as viewStackBaseongoods from '@/api/data-statistics/view-stack-baseongoods'\nimport * as goodsUnit from '@/api/goods-manage/goods-unit'\nimport * as operator from '@/api/system-manage/user'\n\nexport default {\n  name: 'ViewStockBaseongoods',\n  components: { Pagination },\n  directives: { adaptive },\n  data() {\n    return {\n      access: this.$store.getters.access['DataStatistics']['ViewStockBaseongoods'],\n      datas: null,\n      goodsCategories: null,\n      goodsUnits: null,\n      operators: null,\n      query: { goodsCategoryId: null, unitId: null, operatorId: null },\n      page: { total: 0, current: 1, size: 20 },\n      sort: { prop: 'sort', order: 'ascending' },\n      detail: {\n        dialog: { title: '各类容器现状统计', visible: false, labelWidth: '150px' },\n        models: { goodsCategory: { name: null }, goodsUnit: { name: null }, quality: null, qualityOnTemDev: null, qualityOnTemCel: null, qualityOnTemVes: null, qualityLocked: null, inspectionresultNull: null, inspectionresultNo: null, inspectionresultYes: null, updateTime: null, operator: { userName: null }}\n      },\n      loading: { list: true, export: false, detail: false, update: false },\n      sortOrders: ['ascending', 'descending']\n    }\n  },\n  created() {\n    this.getDatas()\n    this.getGoodsCategoryClassification()\n    this.getGoodsUnits()\n    this.getOperators()\n  },\n  methods: {\n    getDatas() {\n      this.loading.list = true\n      viewStackBaseongoods.getList(this.query, this.page, this.sort).then(response => {\n        this.datas = response.data.items\n        this.page.total = response.data.total\n        this.loading.list = false\n      }).catch(reject => {\n        this.loading.list = false\n      })\n    },\n    getGoodsCategoryClassification() {\n      goodsCategoryClassification.getlist(2).then(response => {\n        this.goodsCategories = response.data\n      }).catch(reject => {\n      })\n    },\n    getGoodsUnits() {\n      goodsUnit.getlist().then(response => {\n        this.goodsUnits = response.data\n      }).catch(reject => {\n      })\n    },\n    getOperators() {\n      operator.getlist().then(response => {\n        this.operators = response.data\n      }).catch(reject => {\n      })\n    },\n    handleChange(value) {\n      const id = value[value.length - 1]\n      this.query.goodsCategoryId = id\n    },\n    handleQuery() {\n      this.page.current = 1\n      this.getDatas()\n    },\n    handleSort(data) {\n      this.sort.prop = data.prop\n      this.sort.order = data.order\n      this.getDatas()\n    },\n    handleDetail(row) {\n      // 若列表数据展示了全部属性，则可直接拷贝列表数据\n      this.detail.models = Object.assign({}, row)\n      this.detail.dialog.visible = true\n      // this.loading.detail = true\n      // viewStackBaseongoods.get(row.id).then(response => {\n      //   this.detail.models = response.data\n      //   this.loading.detail = false\n      // }).catch(reject => {\n      //   this.loading.detail = false\n      // })\n    },\n    handleGoods(row) {\n      const goodsCategory = row\n      this.$router.push({\n        path: '/goods-manage/goods',\n        query: {\n          goodsCategory: goodsCategory\n        }\n      })\n    },\n    handleExport() {\n      this.loading.export = true\n      viewStackBaseongoods.exportExcel(this.query, this.page, this.sort).then(response => {\n        const blob = new Blob([response])\n        const fileName = '仓储现状.xls'\n        const elink = document.createElement('a')\n        elink.onload = () => { URL.revokeObjectURL(elink.href) }\n        elink.download = fileName\n        elink.style.display = 'none'\n        elink.href = URL.createObjectURL(blob)\n        document.body.appendChild(elink)\n        elink.click()\n        document.body.removeChild(elink)\n        this.loading.export = false\n      }).catch(reject => {\n        this.loading.export = false\n      })\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n</style>\n"],"sourceRoot":"src/views/data-statistics"}]}