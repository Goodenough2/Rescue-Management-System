A;AAAA;;AACA,WAAA,OAAA,CAAA,MAAA,GAAA,IAAA;AACA,MAAA,WAAA,CAAA,WAAA,CAAA,KAAA,KAAA,EAAA,KAAA,IAAA,EAAA,KAAA,IAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,YAAA,IAAA,GAAA,IAAA,IAAA,CAAA,CAAA,QAAA,CAAA,CAAA;AACA,YAAA,QAAA,GAAA,cAAA;AACA,YAAA,KAAA,GAAA,QAAA,CAAA,aAAA,CAAA,GAAA,CAAA;;AACA,QAAA,KAAA,CAAA,MAAA,GAAA,YAAA;AAAA,UAAA,GAAA,CAAA,eAAA,CAAA,KAAA,CAAA,IAAA;AAAA,SAAA;;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,QAAA;AACA,QAAA,KAAA,CAAA,KAAA,CAAA,OAAA,GAAA,MAAA;AACA,QAAA,KAAA,CAAA,IAAA,GAAA,GAAA,CAAA,eAAA,CAAA,IAAA,CAAA;AACA,QAAA,QAAA,CAAA,IAAA,CAAA,WAAA,CAAA,KAAA;AACA,QAAA,KAAA,CAAA,KAAA;AACA,QAAA,QAAA,CAAA,IAAA,CAAA,WAAA,CAAA,KAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,MAAA,GAAA,KAAA;AACA,OAZA,EAYA,KAZA,CAYA,UAAA,MAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,MAAA,GAAA,KAAA;AACA,OAdA;AAeA;AA5DA;AAxBA,C","sourcesContent":["<template>\r\n  <div class=\"app-container list\">\r\n    <div class=\"toolbar\">\r\n      <el-select v-model=\"query.elementType\" class=\"query-item\" style=\"width: 120px\" placeholder=\"存储单元\" clearable @clear=\"handleQuery\">\r\n        <el-option label=\"仓库\" value=\"0\" />\r\n        <el-option label=\"区域\" value=\"1\" />\r\n        <el-option label=\"货柜\" value=\"2\" />\r\n        <el-option label=\"单元格\" value=\"3\" />\r\n      </el-select>\r\n      <el-cascader\r\n        v-model=\"query.multiLevelStoreId\"\r\n        style=\"width: 150px\"\r\n        placeholder=\"请选择存储级别\"\r\n        :options=\"fourLevelStores\"\r\n        :props=\"{ expandTrigger: 'hover', checkStrictly: true }\"\r\n        clearable\r\n        @change=\"handleChange\"\r\n        @clear=\"handleQuery\"\r\n      />\r\n      <el-input v-model=\"query.elementCode\" class=\"query-item\" style=\"width: 150px\" placeholder=\"存储单元编号(多级，暂时为空)\" clearable @clear=\"handleQuery\" />\r\n      <el-select v-model=\"query.full\" class=\"query-item\" style=\"width: 120px\" placeholder=\"是否满存\" clearable @clear=\"handleQuery\">\r\n        <el-option label=\"未满存\" value=\"0\" />\r\n        <el-option label=\"满存\" value=\"1\" />\r\n      </el-select>\r\n      <el-input v-model=\"query.rfidCode\" class=\"query-item\" style=\"width: 150px\" placeholder=\"RFID码\" clearable @clear=\"handleQuery\" />\r\n      <el-input v-model=\"query.barCode\" class=\"query-item\" style=\"width: 150px\" placeholder=\"条形码\" clearable @clear=\"handleQuery\" />\r\n      <el-button class=\"tool tool-query\" type=\"primary\" icon=\"el-icon-search\" @click=\"handleQuery\">查询</el-button>\r\n      <el-button v-if=\"access.export.allow\" class=\"tool tool-export\" :loading=\"loading.export\" type=\"primary\" icon=\"vue-icon-excel\" @click=\"handleExport\">导出</el-button>\r\n    </div>\r\n\r\n    <el-table ref=\"listTable\" v-loading=\"loading.list\" v-adaptive=\"{ bottomOffset: 55 }\" height=\"200px\" :data=\"datas\" :default-sort=\"sort\" border fit highlight-current-row @sort-change=\"handleSort\">\r\n      <el-table-column label=\"序号\" type=\"index\" align=\"center\" width=\"65\" fixed>\r\n        <template slot-scope=\"scope\">\r\n          <span>{{ (page.current - 1) * page.size + scope.$index + 1 }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"存储单元类型\" prop=\"elementType\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip>\r\n        <template slot-scope=\"{row}\">\r\n          <span v-if=\"row.elementType==0\">仓库</span>\r\n          <span v-if=\"row.elementType==1\">区域</span>\r\n          <span v-if=\"row.elementType==2\">货柜</span>\r\n          <span v-if=\"row.elementType==3\">单元格</span>\r\n          <span v-if=\"row.elementType==4 || row.elementType == 5\">容器</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"存储单元\" prop=\"element.name\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />\r\n      <el-table-column label=\"父存储单元\" prop=\"parent.name\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"当前承重值\" prop=\"supportingCur\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"当前容积\" prop=\"volumeRestCur\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"当前下级存储单元数\" prop=\"count\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"是否满存\" prop=\"full\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip>\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.full===0 ? '未满存' : '满存' }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"RFID码\" prop=\"rfidCode\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"条形码\" prop=\"barCode\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" show-overflow-tooltip />\r\n      <el-table-column fixed=\"right\" label=\"操作\" align=\"center\" width=\"60\">\r\n        <template slot-scope=\"{row}\">\r\n          <el-tooltip v-if=\"access.detail.allow\" class=\"item\" effect=\"dark\" content=\"详情\" placement=\"top-end\">\r\n            <el-button type=\"primary\" plain class=\"button-operate button-detail\" size=\"mini\" @click=\"handleDetail(row)\"><i class=\"vue-icon-detail\" /></el-button>\r\n          </el-tooltip>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <pagination :hidden=\"page.total==0\" :total=\"page.total\" :page.sync=\"page.current\" :limit.sync=\"page.size\" @pagination=\"getDatas\" />\r\n\r\n    <el-dialog v-loading=\"loading.detail\" custom-class=\"dialog-fullscreen dialog-detail\" :title=\"detail.dialog.title\" :visible.sync=\"detail.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\">\r\n      <el-form ref=\"formDetail\" label-position=\"right\" :label-width=\"detail.dialog.labelWidth\">\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"存储单元类型\">\r\n              <span v-if=\"detail.models.elementType==0\">仓库</span>\r\n              <span v-if=\"detail.models.elementType==1\">区域</span>\r\n              <span v-if=\"detail.models.elementType==2\">货柜</span>\r\n              <span v-if=\"detail.models.elementType==3\">单元格</span>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"存储单元编号\">\r\n              {{ detail.models.element.code }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"存储单元名称\">\r\n              {{ detail.models.element.name }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"父存储单元编号\">\r\n              {{ detail.models.parent === null ? '无' : detail.models.parent.code }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"父存储单元名称\">\r\n              {{ detail.models.parent === null ? '无' : detail.models.parent.name }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"当前承重值\">\r\n              {{ detail.models.supportingCur }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"当前容积\">\r\n              {{ detail.models.volumeRestCur }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"含下级存储单元数\">\r\n              {{ detail.models.count }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"是否满存\">\r\n              {{ detail.models.full==0? '未满存':'满存' }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"RFID码\">\r\n              {{ detail.models.rfidCode }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"条形码\">\r\n              {{ detail.models.barCode }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n    </el-dialog>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport adaptive from '@/directive/el-table'\r\nimport Pagination from '@/components/Pagination'\r\nimport * as storageinfo from '@/api/data-statistics/storageinfo'\r\nimport * as store from '@/api/store-manage/store'\r\n\r\nexport default {\r\n  name: 'Storageinfo',\r\n  components: { Pagination },\r\n  directives: { adaptive },\r\n  data() {\r\n    return {\r\n      access: this.$store.getters.access['DataStatistics']['Storageinfo'],\r\n      datas: null,\r\n      fourLevelStores: null,\r\n      query: { elementType: null, multiLevelStoreId: null, full: null, rfidCode: null, barCode: null },\r\n      page: { total: 0, current: 1, size: 20 },\r\n      sort: { prop: 'sort', order: 'ascending' },\r\n      detail: {\r\n        dialog: { title: '各类容器现状统计', visible: false, labelWidth: '150px' },\r\n        models: { elementType: null, element: { code: null, name: null }, parent: { code: null, name: null }, supportingCur: null, volumeRestCur: null, count: null, full: null, rfidCode: null, barCode: null }\r\n      },\r\n      loading: { list: true, export: false, detail: false, update: false },\r\n      sortOrders: ['ascending', 'descending']\r\n    }\r\n  },\r\n  created() {\r\n    this.getDatas()\r\n    this.getFourLevelStores()\r\n  },\r\n  methods: {\r\n    getDatas() {\r\n      this.loading.list = true\r\n      storageinfo.getList(this.query, this.page, this.sort).then(response => {\r\n        this.datas = response.data.items\r\n        this.page.total = response.data.total\r\n        this.loading.list = false\r\n      }).catch(reject => {\r\n        this.loading.list = false\r\n      })\r\n    },\r\n    // 获取多级存储模式：仓库-区域-货柜-单元格\r\n    getFourLevelStores() {\r\n      store.getFourLevelStores().then(response => {\r\n        this.fourLevelStores = response.data\r\n      }).catch(reject => {\r\n      })\r\n    },\r\n    handleQuery() {\r\n      this.page.current = 1\r\n      this.getDatas()\r\n    },\r\n    handleSort(data) {\r\n      this.sort.prop = data.prop\r\n      this.sort.order = data.order\r\n      this.getDatas()\r\n    },\r\n    handleChange(value) {\r\n      const id = value[value.length - 1]\r\n      this.query.multiLevelStoreId = id\r\n    },\r\n    handleDetail(row) {\r\n      // 若列表数据展示了全部属性，则可直接拷贝列表数据\r\n      this.detail.models = Object.assign({}, row)\r\n      this.detail.dialog.visible = true\r\n      // this.loading.detail = true\r\n      // storageinfo.get(row.id).then(response => {\r\n      //   this.detail.models = response.data\r\n      //   this.loading.detail = false\r\n      // }).catch(reject => {\r\n      //   this.loading.detail = false\r\n      // })\r\n    },\r\n    handleExport() {\r\n      this.loading.export = true\r\n      storageinfo.exportExcel(this.query, this.page, this.sort).then(response => {\r\n        const blob = new Blob([response])\r\n        const fileName = '各类容器现状统计.xls'\r\n        const elink = document.createElement('a')\r\n        elink.onload = () => { URL.revokeObjectURL(elink.href) }\r\n        elink.download = fileName\r\n        elink.style.display = 'none'\r\n        elink.href = URL.createObjectURL(blob)\r\n        document.body.appendChild(elink)\r\n        elink.click()\r\n        document.body.removeChild(elink)\r\n        this.loading.export = false\r\n      }).catch(reject => {\r\n        this.loading.export = false\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n</style>\r\n"],"sourceRoot":"src/views/data-statistics"}]}