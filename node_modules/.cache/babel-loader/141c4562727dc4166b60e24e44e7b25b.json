n}\r\n\r\n// 添加数据\r\nexport function create(data) {\r\n  return request({\r\n    url: '/request/record',\r\n    method: 'post',\r\n    data\r\n  })\r\n}\r\n\r\n// 排序上移\r\nexport function rankup(id) {\r\n  return request({\r\n    url: `/request/${id}/rankup`,\r\n    method: 'patch'\r\n  })\r\n}\r\n\r\n// 更新数据\r\nexport function update(data) {\r\n  return request({\r\n    url: '/request/update',\r\n    method: 'put',\r\n    data\r\n  })\r\n}\r\n\r\n// 删除、批量删除数据\r\nexport function del(data) {\r\n  return request({\r\n    url: '/request/delete',\r\n    method: 'delete',\r\n    data: data\r\n  })\r\n}\r\n\r\n// 导出Excel\r\nexport function exportExcel(query, page, sort) {\r\n  return request({\r\n    url: '/request/export',\r\n    method: 'post',\r\n    responseType: 'blob',\r\n    data: {\r\n      page: { current: page.current, size: page.size },\r\n      sort: { property: sort.prop, direction: sort.order },\r\n      query: { ...query }\r\n    }\r\n  })\r\n}\r\n"]}]}