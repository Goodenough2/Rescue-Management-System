    <el-form-item label=\"目的位置码\" prop=\"locationCode\">\n            <el-input v-model=\"create.models.locationCode\" />\n          </el-form-item>\n        </el-col>\n      </el-row>\n      <el-row>\n        <el-col :xl=\"24\">\n          <el-form-item label=\"操作意图\" prop=\"storeOperateTypeId\">\n            <el-select v-model=\"create.models.storeOperateTypeId\" clearable filterable>\n              <el-option v-for=\"item in storeOperateTypes\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n            </el-select>\n          </el-form-item>\n        </el-col>\n      </el-row>\n      <el-row>\n        <el-col :xl=\"24\">\n          <el-form-item>\n            <el-button type=\"primary\" @click=\"operate()\">提交</el-button>\n          </el-form-item>\n        </el-col>\n      </el-row>\n    </el-form>\n  </div>\n</template>\n\n<script>\n\nimport adaptive from '@/directive/el-table'\nimport setRule from '@/utils/form-validate'\nimport * as storeOperateType from '@/api/log-manage/store-operate-type'\nimport * as goods from '@/api/goods-manage/goods'\n\nexport default {\n  name: 'BusinessOperation',\n  // components: { Pagination },\n  directives: { adaptive },\n  data() {\n    return {\n      storeOperateTypes: null,\n      create: {\n        dialog: { title: '货物入库', visible: true, labelWidth: '100px' },\n        models: { originalLocationCode: null, goodsCode: null, locationCode: null, storeOperateTypeId: null },\n        rules: {\n          goodsCode: setRule('货物编号', [{ required: true }, { length: [0, 32] }]),\n          locationCode: setRule('目的地编号', [{ required: true }, { length: [0, 100] }]),\n          originalLocationCode: setRule('源地址编号', [{ required: true }, { length: [0, 100] }]),\n          storeOperateTypeId: setRule('操作类型', [{ selected: true }])\n        }\n      },\n      loading: { list: true, export: false, detail: false, update: false }\n    }\n  },\n  created() {\n    this.getStoreOperateTypes()\n  },\n  mounted() {\n    this.getPath()\n  },\n  methods: {\n    getPath() {\n      let goodsCode = ''\n      goodsCode = this.$route.query.goodsCode\n      this.create.models.goodsCode = goodsCode\n    },\n    getStoreOperateTypes() {\n      const id = 3\n      storeOperateType.getlist(id).then(response => {\n        this.storeOperateTypes = response.data\n      }).catch(reject => {\n      })\n    },\n    operate() {\n      this.$refs['formCreate'].validate((valid) => {\n        if (!valid) return false\n        goods.operate(this.create.models).then(response => {\n          // 为方便连续添加，create对话框不关闭\n          // 需要清空的表单，手动清空，至少清空一个必填项，防止点击两遍\n          this.create.models.goodsCode = ''\n          // 重新获取数据\n          this.getDatas()\n        }).catch(reject => {\n        })\n      })\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n  .app-container{\n    width: 40%;\n    margin: 150px auto;\n  }\n</style>\n"],"sourceRoot":"src/views/business-operation"}]}