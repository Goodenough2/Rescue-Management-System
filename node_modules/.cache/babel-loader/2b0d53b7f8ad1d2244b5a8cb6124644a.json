;AAEbC,IAAAA,MAAM,EAAE,MAFK;AAGbC,IAAAA,IAAI,EAAJA;AAHa,GAAR,CAAP;AAKD,C,CAED;;;AACO,SAASW,GAAT,CAAaX,IAAb,EAAmB;AACxB,SAAO,sBAAQ;AACbF,IAAAA,GAAG,EAAE,eADQ;AAEbC,IAAAA,MAAM,EAAE,MAFK;AAGbC,IAAAA,IAAI,EAAJA;AAHa,GAAR,CAAP;AAKD,C,CAED;;;AACO,SAASY,KAAT,CAAeZ,IAAf,EAAqB;AAC1B,SAAO,sBAAQ;AACbF,IAAAA,GAAG,EAAE,gBADQ;AAEbC,IAAAA,MAAM,EAAE,MAFK;AAGbC,IAAAA,IAAI,EAAJA;AAHa,GAAR,CAAP;AAKD,C,CAED;;;AACO,SAASa,MAAT,CAAgBL,EAAhB,EAAoB;AACzB,SAAO,sBAAQ;AACbV,IAAAA,GAAG,mBAAYU,EAAZ,YADU;AAEbT,IAAAA,MAAM,EAAE;AAFK,GAAR,CAAP;AAID,C,CAED;;;AACO,SAASe,WAAT,CAAqBnB,KAArB,EAA4BC,IAA5B,EAAkCC,IAAlC,EAAwC;AAC7C,SAAO,sBAAQ;AACbC,IAAAA,GAAG,EAAE,eADQ;AAEbC,IAAAA,MAAM,EAAE,MAFK;AAGbgB,IAAAA,YAAY,EAAE,MAHD;AAIbf,IAAAA,IAAI,EAAE;AACJJ,MAAAA,IAAI,EAAE;AAAEK,QAAAA,OAAO,EAAEL,IAAI,CAACK,OAAhB;AAAyBC,QAAAA,IAAI,EAAEN,IAAI,CAACM;AAApC,OADF;AAEJL,MAAAA,IAAI,EAAE;AAAEM,QAAAA,QAAQ,EAAEN,IAAI,CAACO,IAAjB;AAAuBC,QAAAA,SAAS,EAAER,IAAI,CAACS;AAAvC,OAFF;AAGJX,MAAAA,KAAK,kCAAOA,KAAP;AAHD;AAJO,GAAR,CAAP;AAUD,C,CAED;;;AACO,SAASqB,MAAT,CAAgBhB,IAAhB,EAAsB;AAC3B,SAAO,sBAAQ;AACbF,IAAAA,GAAG,EAAE,eADQ;AAEbC,IAAAA,MAAM,EAAE,KAFK;AAGbC,IAAAA,IAAI,EAAJA;AAHa,GAAR,CAAP;AAKD,C,CAED;;;AACO,SAASiB,GAAT,CAAajB,IAAb,EAAmB;AACxB,SAAO,sBAAQ;AACbF,IAAAA,GAAG,EAAE,eADQ;AAEbC,IAAAA,MAAM,EAAE,QAFK;AAGbC,IAAAA,IAAI,EAAE;AAAEkB,MAAAA,GAAG,EAAElB;AAAP;AAHO,GAAR,CAAP;AAKD,C,CAED;;;AACO,SAASmB,OAAT,CAAiBnB,IAAjB,EAAuB;AAC5B,SAAO,sBAAQ;AACbF,IAAAA,GAAG,EAAE,gBADQ;AAEbC,IAAAA,MAAM,EAAE,MAFK;AAGbC,IAAAA,IAAI,EAAJA;AAHa,GAAR,CAAP;AAKD","sourcesContent":["import request from '@/utils/request'\r\n\r\n// 获取列表数据（查询、翻页）\r\nexport function getList(query, page, sort) {\r\n  return request({\r\n    url: '/goods/list',\r\n    method: 'post',\r\n    data: {\r\n      page: { current: page.current, size: page.size },\r\n      sort: { property: sort.prop, direction: sort.order },\r\n      query: { ...query }\r\n    }\r\n  })\r\n}\r\n\r\n// 根据id获取数据\r\nexport function get(id) {\r\n  return request({\r\n    url: `/goods/${id}/detail`,\r\n    method: 'get'\r\n  })\r\n}\r\n\r\n// 获得下拉列表的数据\r\nexport function getlist() {\r\n  return request({\r\n    url: '/goods/selectlist',\r\n    method: 'get'\r\n  })\r\n}\r\n\r\n// 添加数据\r\nexport function create(data) {\r\n  return request({\r\n    url: '/goods/store',\r\n    method: 'post',\r\n    data\r\n  })\r\n}\r\n\r\n// 货物出库\r\nexport function out(data) {\r\n  return request({\r\n    url: '/goods/output',\r\n    method: 'post',\r\n    data\r\n  })\r\n}\r\n\r\n// 货物验货\r\nexport function check(data) {\r\n  return request({\r\n    url: '/goods/inspect',\r\n    method: 'post',\r\n    data\r\n  })\r\n}\r\n\r\n// 排序上移\r\nexport function rankup(id) {\r\n  return request({\r\n    url: `/goods/${id}/rankup`,\r\n    method: 'patch'\r\n  })\r\n}\r\n\r\n// 导出Excel\r\nexport function exportExcel(query, page, sort) {\r\n  return request({\r\n    url: '/goods/export',\r\n    method: 'post',\r\n    responseType: 'blob',\r\n    data: {\r\n      page: { current: page.current, size: page.size },\r\n      sort: { property: sort.prop, direction: sort.order },\r\n      query: { ...query }\r\n    }\r\n  })\r\n}\r\n\r\n// 更新数据\r\nexport function update(data) {\r\n  return request({\r\n    url: '/goods/update',\r\n    method: 'put',\r\n    data\r\n  })\r\n}\r\n\r\n// 删除、批量删除数据\r\nexport function del(data) {\r\n  return request({\r\n    url: '/goods/delete',\r\n    method: 'delete',\r\n    data: { ids: data }\r\n  })\r\n}\r\n\r\n// 货物操作\r\nexport function operate(data) {\r\n  return request({\r\n    url: '/goods/operate',\r\n    method: 'post',\r\n    data\r\n  })\r\n}\r\n"]}]}