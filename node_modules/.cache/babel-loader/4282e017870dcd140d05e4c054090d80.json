 getList(query, page, sort, status) {\n  return request({\n    url: '/profitApplication/list',\n    method: 'post',\n    data: {\n      page: { current: page.current, size: page.size },\n      sort: { property: sort.prop, direction: sort.order },\n      query: { ...query },\n      status: status\n    }\n  })\n}\n\n// 获取列表数据（查询、翻页）\nexport function getListApproval(query, page, sort, status) {\n  return request({\n    url: '/profitApproval/list',\n    method: 'post',\n    data: {\n      page: { current: page.current, size: page.size },\n      sort: { property: sort.prop, direction: sort.order },\n      query: { ...query },\n      status: status\n    }\n  })\n}\n\n// 获取列表数据（查询、翻页）\nexport function getListFinal(query, page, sort, status) {\n  return request({\n    url: '/profitFinalReview/list',\n    method: 'post',\n    data: {\n      page: { current: page.current, size: page.size },\n      sort: { property: sort.prop, direction: sort.order },\n      query: { ...query },\n      status: status\n    }\n  })\n}\n\n// 添加数据\nexport function create(application, goods, unitPriceNew) {\n  return request({\n    url: '/profitApplication/create',\n    method: 'post',\n    data: {\n      application: application,\n      goods: goods,\n      quality: null,\n      unitPriceNew: unitPriceNew\n    }\n  })\n}\n\n// 审批通过\nexport function approvalTrue(ids) {\n  return request({\n    url: '/profitApproval/examine',\n    method: 'post',\n    data: {\n      ids: ids,\n      approve: true\n    }\n  })\n}\n\n// 审批退回\nexport function approvalFalse(ids) {\n  return request({\n    url: '/profitApproval/examine',\n    method: 'post',\n    data: {\n      ids: ids,\n      approve: false\n    }\n  })\n}\n\n// 终审通过\nexport function finalTrue(ids) {\n  return request({\n    url: '/profitFinalReview/finalExamine',\n    method: 'post',\n    data: {\n      ids: ids,\n      approve: true\n    }\n  })\n}\n\n// 终审退回\nexport function finalFalse(ids) {\n  return request({\n    url: '/residualFinalReview/finalExamine',\n    method: 'post',\n    data: {\n      ids: ids,\n      approve: false\n    }\n  })\n}\n"]}]}