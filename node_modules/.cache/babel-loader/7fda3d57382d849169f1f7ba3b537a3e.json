nge[0]\r\n          //     item.message = rule.message || `${name}的范围应大于数字${min}`\r\n          //   }\r\n          //   if (max) {\r\n          //     item.max = rule.range[1]\r\n          //     item.message = rule.message || `${name}的范围应小于数字${max}`\r\n          //   }\r\n          //   if (min && max) {\r\n          //     item.message = rule.message || `${name}的范围应介于${min}-${max}之间`\r\n          //   }\r\n          // }\r\n          break\r\n        case 'max':\r\n          break\r\n        case 'min':\r\n          break\r\n        case 'date':\r\n          break\r\n        case 'datetime':\r\n          break\r\n        default:\r\n          item[key] = rule[key]\r\n          break\r\n      }\r\n      item.trigger = item.trigger || 'blur'\r\n    }\r\n    items.push(item)\r\n  }\r\n  return items\r\n}\r\n"]}]}