earable @clear=\"handleQuery\">\r\n        <el-option v-for=\"item in goodsUnits\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\r\n      </el-select>\r\n      <el-input v-model.trim=\"query.name\" class=\"query-item\" style=\"width: 150px\" placeholder=\"存储模式名称\" clearable @clear=\"handleQuery\" />\r\n      <el-button class=\"tool tool-query\" type=\"primary\" icon=\"el-icon-search\" @click=\"handleQuery\">查询</el-button>\r\n      <el-button v-if=\"access.delete.allow\" class=\"tool tool-delete\" type=\"danger\" icon=\"vue-icon-delete\" @click=\"handleDeletes\">批量删除</el-button>\r\n    </div>\r\n\r\n    <el-table ref=\"listTable\" v-loading=\"loading.list\" v-adaptive=\"{ bottomOffset: 55 }\" height=\"200px\" :data=\"datas\" :default-sort=\"sort\" border fit highlight-current-row @sort-change=\"handleSort\">\r\n      <el-table-column type=\"selection\" align=\"center\" width=\"35\" />\r\n      <el-table-column label=\"序号\" type=\"index\" align=\"center\" width=\"65\" fixed>\r\n        <template slot-scope=\"scope\">\r\n          <span>{{ (page.current - 1) * page.size + scope.$index + 1 }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"存储模式名称\" prop=\"name\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"货物种类\" prop=\"goodsCategory.name\" :sort-orders=\"sortOrders\" width=\"150\" align=\"center\" show-overflow-tooltip />\r\n      <el-table-column label=\"单位换算组别\" prop=\"groupId\" :sort-orders=\"sortOrders\" width=\"300\" align=\"center\" show-overflow-tooltip />\r\n      <el-table-column label=\"存储单位\" prop=\"goodsUnit.name\" :sort-orders=\"sortOrders\" width=\"150\" align=\"center\" show-overflow-tooltip />\r\n      <el-table-column label=\"换算级别\" prop=\"level\" :sort-orders=\"sortOrders\" width=\"150\" align=\"center\" show-overflow-tooltip />\r\n      <el-table-column label=\"换算系数\" prop=\"ratio\" :sort-orders=\"sortOrders\" width=\"150\" align=\"center\" show-overflow-tooltip />\r\n      <el-table-column label=\"模式满存重量\" prop=\"weight\" :sort-orders=\"sortOrders\" width=\"150\" align=\"center\" show-overflow-tooltip />\r\n      <el-table-column label=\"模式存储体积\" prop=\"volume\" :sort-orders=\"sortOrders\" align=\"center\" show-overflow-tooltip />\r\n\r\n      <el-table-column fixed=\"right\" label=\"操作\" align=\"center\" width=\"140\">\r\n        <template slot-scope=\"{row}\">\r\n          <el-tooltip v-if=\"access.update.allow\" class=\"item\" effect=\"dark\" content=\"排序上移\" placement=\"top-end\">\r\n            <el-button type=\"primary\" plain class=\"button-operate button-rankup\" size=\"mini\" @click=\"handleRankup(row)\"><i class=\"vue-icon-rankup\" /></el-button>\r\n          </el-tooltip>\r\n          <el-tooltip v-if=\"access.detail.allow\" class=\"item\" effect=\"dark\" content=\"详情\" placement=\"top-end\">\r\n            <el-button type=\"primary\" plain class=\"button-operate button-detail\" size=\"mini\" @click=\"handleDetail(row)\"><i class=\"vue-icon-detail\" /></el-button>\r\n          </el-tooltip>\r\n          <el-tooltip v-if=\"access.delete.allow\" class=\"item\" effect=\"dark\" content=\"删除\" placement=\"top-end\">\r\n            <el-button type=\"danger\" plain class=\"button-operate button-delete\" size=\"mini\" @click=\"handleDelete(row)\"><i class=\"vue-icon-delete\" /></el-button>\r\n          </el-tooltip>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <pagination :hidden=\"page.total==0\" :total=\"page.total\" :page.sync=\"page.current\" :limit.sync=\"page.size\" @pagination=\"getDatas\" />\r\n\r\n    <el-dialog v-loading=\"loading.detail\" custom-class=\"dialog-fullscreen\" :title=\"detail.dialog.title\" :visible.sync=\"detail.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\">\r\n      <el-form ref=\"formDetail\" label-position=\"right\" :label-width=\"detail.dialog.labelWidth\">\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"存储模式名称\">\r\n              {{ detail.models.name }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"货物种类\">\r\n              {{ detail.models.goodsCategory.name }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"当前换算组别\">\r\n              {{ detail.models.groupId }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"存储单位名称\">\r\n              {{ detail.models.goodsUnit.name }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"计量单位换算级别\">\r\n              {{ detail.models.level }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"换算系数\">\r\n              {{ detail.models.ratio }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"满存重量\">\r\n              {{ detail.models.weight }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"存储的长\">\r\n              {{ detail.models.length }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item labe