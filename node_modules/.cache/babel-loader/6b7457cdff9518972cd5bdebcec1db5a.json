AA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,GAAA,CAAA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,OAAA,GAAA,IAAA;AACA,WAAA,OAAA,CAAA,MAAA,GAAA,IAAA,CAJA,CAKA;AACA;AACA;AACA;AACA;AACA;;AACA,WAAA,OAAA,CAAA,MAAA,GAAA,KAAA;AACA,KAhCA;AAiCA,IAAA,YAjCA,0BAiCA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,OAAA,GAAA,IAAA;AACA,KAnCA;AAoCA,IAAA,UApCA,wBAoCA;AAAA;;AACA,WAAA,KAAA,CAAA,YAAA,EAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,CAAA,KAAA,EAAA,OAAA,KAAA;AACA,QAAA,UAAA,CAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,MAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA;AACA;AACA,UAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,IAAA,GAAA,EAAA,CAHA,CAIA;;AACA,UAAA,MAAA,CAAA,QAAA;AACA,SANA,EAMA,KANA,CAMA,UAAA,MAAA,EAAA,CACA,CAPA;AAQA,OAVA;AAWA,KAhDA;AAiDA,IAAA,YAjDA,wBAiDA,GAjDA,EAiDA;AAAA;;AACA,WAAA,OAAA,CAAA,IAAA,GAAA,IAAA;AACA,MAAA,UAAA,CAAA,MAAA,CAAA,GAAA,CAAA,EAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,IAAA,GAAA,KAAA;AACA,OAHA,EAGA,KAHA,CAGA,UAAA,MAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,IAAA,GAAA,KAAA;AACA,OALA;AAMA,KAzDA;AA0DA,IAAA,YA1DA,wBA0DA,GA1DA,EA0DA;AAAA;;AACA;AACA,WAAA,MAAA,CAAA,MAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,GAAA,CAAA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,OAAA,GAAA,IAAA;AACA,WAAA,OAAA,CAAA,MAAA,GAAA,IAAA,CAJA,CAKA;AACA;AACA;AACA;AACA;AACA;;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,YAAA,EAAA,aAAA;AACA,OAFA;AAGA,WAAA,OAAA,CAAA,MAAA,GAAA,KAAA;AACA,KAzEA;AA0EA,IAAA,UA1EA,wBA0EA;AAAA;;AACA,WAAA,KAAA,CAAA,YAAA,EAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,CAAA,KAAA,EAAA,OAAA,KAAA;AACA,YAAA,QAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA;AACA,QAAA,UAAA,CAAA,MAAA,CAAA,QAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA;AACA,UAAA,MAAA,CAAA,QAAA;;AACA,UAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,SAJA,EAIA,KAJA,CAIA,UAAA,MAAA,EAAA,CACA,CALA;AAMA,OATA;AAUA,KArFA;AAsFA,IAAA,YAtFA,wBAsFA,GAtFA,EAsFA;AACA,WAAA,UAAA,CAAA,CAAA,GAAA,CAAA,EAAA,CAAA;AACA,KAxFA;AAyFA,IAAA,aAzFA,yBAyFA,GAzFA,EAyFA;AACA,UAAA,KAAA,KAAA,CAAA,SAAA,CAAA,SAAA,CAAA,MAAA,KAAA,CAAA,EAAA;AACA,aAAA,QAAA,CAAA;AAAA,UAAA,IAAA,EAAA,SAAA;AAAA,UAAA,OAAA,EAAA,oBAAA;AAAA,UAAA,QAAA,EAAA;AAAA,SAAA;AACA,eAAA,KAAA;AACA;;AACA,UAAA,GAAA,GAAA,EAAA;;AALA,+DAMA,KAAA,KAAA,CAAA,SAAA,CAAA,SANA;AAAA;;AAAA;AAMA,4DAAA;AAAA,cAAA,IAAA;AACA,UAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,EAAA;AACA;AARA;AAAA;AAAA;AAAA;AAAA;;AASA,WAAA,UAAA,CAAA,GAAA;AACA,KAnGA;AAoGA,IAAA,UApGA,sBAoGA,GApGA,EAoGA;AAAA;;AACA,WAAA,QAAA,CAAA,mBAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,UAAA,CAAA,GAAA,CAAA,GAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA;AACA,UAAA,MAAA,CAAA,QAAA;AACA,SAHA,EAGA,KAHA,CAGA,UAAA,MAAA,EAAA,CACA,CAJA;AAKA,OAVA;AAWA;AAhHA;AAtCA,C","sourcesContent":["<template>\r\n  <div class=\"app-container list\">\r\n    <div class=\"toolbar\">\r\n      <el-input v-model.trim=\"query.name\" class=\"query-item\" style=\"width: 120px\" placeholder=\"组别名称\" clearable @clear=\"handleQuery\" />\r\n      <el-button class=\"tool tool-query\" type=\"primary\" icon=\"el-icon-search\" @click=\"handleQuery\">查询</el-button>\r\n      <el-button v-if=\"access.create.allow\" class=\"tool tool-create\" type=\"primary\" icon=\"vue-icon-create\" @click=\"handleCreate\">添加</el-button>\r\n      <el-button v-if=\"access.delete.allow\" class=\"tool tool-delete\" type=\"danger\" icon=\"vue-icon-delete\" @click=\"handleDeletes\">批量删除</el-button>\r\n    </div>\r\n\r\n    <el-table ref=\"listTable\" v-loading=\"loading.list\" v-adaptive=\"{ bottomOffset: 55 }\" height=\"200px\" :data=\"datas\" :default-sort=\"sort\" border fit highlight-current-row @sort-change=\"handleSort\">\r\n      <el-table-column type=\"selection\" align=\"center\" width=\"35\" />\r\n      <el-table-column label=\"序号\" type=\"index\" align=\"center\" width=\"65\" fixed>\r\n        <template slot-scope=\"scope\">\r\n          <span>{{ (page.current - 1) * page.size + scope.$index + 1 }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"组别名称\" prop=\"name\" :sort-orders=\"sortOrders\" align=\"center\" width=\"200\" show-overflow-tooltip />\r\n      <el-table-column label=\"组别描述\" prop=\"description\" :sort-orders=\"sortOrders\" align=\"center\" show-overflow-tooltip />\r\n      <el-table-column fixed=\"right\" label=\"操作\" align=\"center\" width=\"180\">\r\n        <template slot-scope=\"{row}\">\r\n          <el-tooltip v-if=\"access.update.allow\" class=\"item\" effect=\"dark\" content=\"排序上移\" placement=\"top-end\">\r\n            <el-button type=\"primary\" plain class=\"button-operate button-rankup\" size=\"mini\" @click=\"handleRankup(row)\"><i class=\"vue-icon-rankup\" /></el-button>\r\n          </el-tooltip>\r\n          <el-tooltip v-if=\"access.detail.allow\" class=\"item\" effect=\"dark\" content=\"详情\" placement=\"top-end\">\r\n            <el-button type=\"primary\" plain class=\"button-operate button-detail\" size=\"mini\" @click=\"handleDetail(row)\"><i class=\"vue-icon-detail\" /></el-button>\r\n          </el-tooltip>\r\n          <el-tooltip v-if=\"access.update.allow\" class=\"item\" effect=\"dark\" content=\"编辑\" placement=\"top-end\">\r\n            <el-button type=\"primary\" plain class=\"button-operate button-update\" size=\"mini\" @click=\"handleUpdate(row)\"><i class=\"vue-icon-update\" /></el-button>\r\n          </el-tooltip>\r\n          <el-tooltip v-if=\"access.delete.allow\" class=\"item\" effect=\"dark\" content=\"删除\" placement=\"top-end\">\r\n            <el-button type=\"danger\" plain class=\"button-operate button-delete\" size=\"mini\" @click=\"handleDelete(row)\"><i class=\"vue-icon-delete\" /></el-button>\r\n          </el-tooltip>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <pagination :hidden=\"page.total==0\" :total=\"page.total\" :page.sync=\"page.current\" :limit.sync=\"page.size\" @pagination=\"getDatas\" />\r\n\r\n    <el-dialog v-loading=\"loading.detail\" custom-class=\"dialog-detail\" :title=\"detail.dialog.title\" :visible.sync=\"detail.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\">\r\n      <el-form ref=\"formDetail\" label-position=\"right\" :label-width=\"detail.dialog.labelWidth\">\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"报警组别名称\">\r\n              {{ detail.models.name }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"报警组别描述\">\r\n              {{ detail.models.description }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n    </el-dialog>\r\n\r\n    <el-dialog custom-class=\"dialog-create\" :title=\"create.dialog.title\" :visible.sync=\"create.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\" :close-on-click-modal=\"false\">\r\n      <el-form ref=\"formCreate\" :rules=\"create.rules\" :model=\"create.models\" label-position=\"right\" :label-width=\"create.dialog.labelWidth\">\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"报警组别名称\" prop=\"name\">\r\n              <el-input v-model=\"create.models.name\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"报警组别描述\" prop=\"description\">\r\n              <el-input v-model=\"create.models.description\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"create.dialog.visible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"createData()\">提交</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n    <el-dialog v-loading=\"loading.update\" custom-class=\"dialog-update\" :title=\"update.dialog.title\" :visible.sync=\"update.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\" :close-on-click-modal=\"false\">\r\n      <el-form ref=\"formUpdate\" :rules=\"update.rules\" :model=\"update.models\" label-position=\"right\" :label-width=\"update.dialog.labelWidth\">\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"报警组别名称\" prop=\"name\">\r\n              <el-input v-model=\"update.models.name\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"报警组别描述\" prop=\"description\">\r\n              <el-input v-model=\"update.models.description\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"update.dialog.visible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"updateData()\">提交</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport adaptive from '@/directive/el-table'\r\nimport setRule from '@/utils/form-validate'\r\nimport Pagination from '@/components/Pagination'\r\nimport * as alarmGroup from '@/api/alarm-manage/alarm-group'\r\n\r\nexport default {\r\n  name: 'AlarmGroup',\r\n  components: { Pagination },\r\n  directives: { adaptive },\r\n  data() {\r\n    return {\r\n      access: this.$store.getters.access['AlarmManage']['AlarmGroup'],\r\n      datas: null,\r\n      query: { name: null },\r\n      page: { total: 0, current: 1, size: 20 },\r\n      sort: { prop: 'sort', order: 'ascending' },\r\n      detail: {\r\n        dialog: { title: '报警分组', visible: false, labelWidth: '120px' },\r\n        models: { code: null, name: null, description: null }\r\n      },\r\n      create: {\r\n        dialog: { title: '添加报警分组', visible: false, labelWidth: '120px' },\r\n        models: { name: null, description: null },\r\n        rules: {\r\n          name: setRule('组别名称', [{ required: true }, { length: [0, 20] }]),\r\n          description: setRule('组别描述', [{ length: [0, 50] }])\r\n        }\r\n      },\r\n      update: {\r\n        dialog: { title: '编辑报警分组', visible: false, labelWidth: '120px' },\r\n        models: { id: null, name: null, description: null },\r\n        rules: {\r\n          name: setRule('组别名称', [{ required: true }, { length: [0, 20] }]),\r\n          description: setRule('组别描述', [{ length: [0, 50] }])\r\n        }\r\n      },\r\n      loading: { list: true, export: false, detail: false, update: false },\r\n      sortOrders: ['ascending', 'descending']\r\n    }\r\n  },\r\n  created() {\r\n    this.getDatas()\r\n  },\r\n  methods: {\r\n    getDatas() {\r\n      this.loading.list = true\r\n      alarmGroup.getList(this.query, this.page, this.sort).then(response => {\r\n        this.datas = response.data.items\r\n        this.page.total = response.data.total\r\n        this.loading.list = false\r\n      }).catch(reject => {\r\n        this.loading.list = false\r\n      })\r\n    },\r\n    handleQuery() {\r\n      this.page.current = 1\r\n      this.getDatas()\r\n    },\r\n    handleSort(data) {\r\n      this.sort.prop = data.prop\r\n      this.sort.order = data.order\r\n      this.getDatas()\r\n    },\r\n    handleDetail(row) {\r\n      // 若列表数据展示了全部属性，则可直接拷贝列表数据\r\n      this.detail.models = Object.assign({}, row)\r\n      this.detail.dialog.visible = true\r\n      this.loading.detail = true\r\n      // api.get(row.id).then(response => {\r\n      //   this.detail.models = response.data\r\n      //   this.loading.detail = false\r\n      // }).catch(reject => {\r\n      //   this.loading.detail = false\r\n      // })\r\n      this.loading.detail = false\r\n    },\r\n    handleCreate() {\r\n      this.create.dialog.visible = true\r\n    },\r\n    createData() {\r\n      this.$refs['formCreate'].validate((valid) => {\r\n        if (!valid) return false\r\n        alarmGroup.create(this.create.models).then(response => {\r\n          // 为方便连续添加，create对话框不关闭\r\n          // 需要清空的表单，手动清空，至少清空一个必填项，防止点击两遍\r\n          this.create.models.name = ''\r\n          // 重新获取数据\r\n          this.getDatas()\r\n        }).catch(reject => {\r\n        })\r\n      })\r\n    },\r\n    handleRankup(row) {\r\n      this.loading.list = true\r\n      alarmGroup.rankup(row.id).then(response => {\r\n        this.getDatas()\r\n        this.loading.list = false\r\n      }).catch(reject => {\r\n        this.loading.list = false\r\n      })\r\n    },\r\n    handleUpdate(row) {\r\n      //  若列表数据展示了全部属性，则可直接拷贝列表数据用于编辑\r\n      this.update.models = Object.assign({}, row)\r\n      this.update.dialog.visible = true\r\n      this.loading.update = true\r\n      // goodsType.get(row.id).then(response => {\r\n      //   this.update.models = response.data\r\n      //   this.loading.update = false\r\n      // }).catch(reject => {\r\n      //   this.loading.update = false\r\n      // })\r\n      this.$nextTick(() => {\r\n        this.$refs['formUpdate'].clearValidate()\r\n      })\r\n      this.loading.update = false\r\n    },\r\n    updateData() {\r\n      this.$refs['formUpdate'].validate((valid) => {\r\n        if (!valid) return false\r\n        const tempData = Object.assign({}, this.update.models)\r\n        alarmGroup.update(tempData).then(response => {\r\n          // 重新获取数据\r\n          this.getDatas()\r\n          this.update.dialog.visible = false\r\n        }).catch(reject => {\r\n        })\r\n      })\r\n    },\r\n    handleDelete(row) {\r\n      this.deleteData([row.id])\r\n    },\r\n    handleDeletes(row) {\r\n      if (this.$refs.listTable.selection.length === 0) {\r\n        this.$message({ type: 'warning', message: '请在下面表格的左侧勾选需要删除的数据', duration: 4000 })\r\n        return false\r\n      }\r\n      const ids = []\r\n      for (const item of this.$refs.listTable.selection) {\r\n        ids.push(item.id)\r\n      }\r\n      this.deleteData(ids)\r\n    },\r\n    deleteData(ids) {\r\n      this.$confirm('数据删除后，不可恢复, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        alarmGroup.del(ids).then(response => {\r\n          // 重新获取数据\r\n          this.getDatas()\r\n        }).catch(reject => {\r\n        })\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n</style>\r\n"],"sourceRoot":"src/views/alarm-manage"}]}