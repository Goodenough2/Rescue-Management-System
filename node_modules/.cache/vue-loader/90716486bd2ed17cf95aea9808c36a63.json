AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"order-material.vue","sourceRoot":"src/views/order-business","sourcesContent":["<template>\r\n  <div class=\"app-container list\">\r\n    <div class=\"toolbar\">\r\n      <el-input v-model.trim=\"query.orderDetailCode\" class=\"query-item\" style=\"width: 150px\" placeholder=\"订单详单编号\" clearable @clear=\"handleQuery\" />\r\n      <el-select v-model=\"query.goodsCategoryId\" class=\"query-item\" style=\"width: 200px\" placeholder=\"货物种类\" clearable @clear=\"handleQuery\">\r\n        <el-option v-for=\"item in goodsCategories\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\r\n      </el-select>\r\n      <el-select v-model=\"query.operatorId\" class=\"query-item\" style=\"width: 200px\" placeholder=\"操作人员\" clearable @clear=\"handleQuery\">\r\n        <el-option v-for=\"item in operators\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\r\n      </el-select>\r\n      <el-select v-model=\"query.status\" class=\"query-item\" style=\"width: 120px\" placeholder=\"锁定状态\" clearable @clear=\"handleQuery\">\r\n        <el-option label=\"未锁定\" value=\"0\" />\r\n        <el-option label=\"部分锁定\" value=\"1\" />\r\n        <el-option label=\"锁定\" value=\"2\" />\r\n      </el-select>\r\n      <el-button class=\"tool tool-query\" type=\"primary\" icon=\"el-icon-search\" @click=\"handleQuery\">查询</el-button>\r\n      <el-button v-if=\"access.record.allow\" class=\"tool tool-create\" type=\"primary\" icon=\"vue-icon-create\" @click=\"handleCreate\">添加</el-button>\r\n      <el-button v-if=\"access.delete.allow\" class=\"tool tool-delete\" type=\"danger\" icon=\"vue-icon-delete\" @click=\"handleDeletes\">批量删除</el-button>\r\n      <el-button v-if=\"access.export.allow\" class=\"tool tool-export\" :loading=\"loading.export\" type=\"primary\" icon=\"vue-icon-excel\" @click=\"handleExport\">导出</el-button>\r\n    </div>\r\n\r\n    <el-table ref=\"listTable\" v-loading=\"loading.list\" v-adaptive=\"{ bottomOffset: 55 }\" height=\"200px\" :data=\"datas\" :default-sort=\"sort\" border fit highlight-current-row @sort-change=\"handleSort\">\r\n      <el-table-column type=\"selection\" align=\"center\" width=\"35\" />\r\n      <el-table-column label=\"序号\" type=\"index\" align=\"center\" width=\"65\" fixed>\r\n        <template slot-scope=\"scope\">\r\n          <span>{{ (page.current - 1) * page.size + scope.$index + 1 }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"物料单编号\" prop=\"code\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"货物种类\" prop=\"goodsCategory.name\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"数量\" prop=\"quality\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"锁定数量\" prop=\"qualityLocked\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"状态改变时间\" prop=\"statusTime\" :sort-orders=\"sortOrders\" align=\"center\" width=\"200\" show-overflow-tooltip />\r\n      <el-table-column label=\"锁定状态\" prop=\"status\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip>\r\n        <template slot-scope=\"{row}\">\r\n          <span v-if=\"row.status==0\">未锁定</span>\r\n          <span v-if=\"row.status==1\">部分锁定</span>\r\n          <span v-if=\"row.status==2\">锁定</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作人员\" prop=\"operator.userName\" :sort-orders=\"sortOrders\" align=\"center\" show-overflow-tooltip />\r\n      <el-table-column fixed=\"right\" label=\"操作\" align=\"center\" width=\"140\">\r\n        <template slot-scope=\"{row}\">\r\n          <el-tooltip v-if=\"access.detail.allow\" class=\"item\" effect=\"dark\" content=\"详情\" placement=\"top-end\">\r\n            <el-button type=\"primary\" plain class=\"button-operate button-detail\" size=\"mini\" @click=\"handleDetail(row)\"><i class=\"vue-icon-detail\" /></el-button>\r\n          </el-tooltip>\r\n          <el-tooltip v-if=\"access.update.allow\" class=\"item\" effect=\"dark\" content=\"编辑\" placement=\"top-end\">\r\n            <el-button type=\"primary\" plain class=\"button-operate button-update\" size=\"mini\" @click=\"handleUpdate(row)\"><i class=\"vue-icon-update\" /></el-button>\r\n          </el-tooltip>\r\n          <el-tooltip v-if=\"access.delete.allow\" class=\"item\" effect=\"dark\" content=\"删除\" placement=\"top-end\">\r\n            <el-button type=\"danger\" plain class=\"button-operate button-delete\" size=\"mini\" @click=\"handleDelete(row)\"><i class=\"vue-icon-delete\" /></el-button>\r\n          </el-tooltip>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <pagination :hidden=\"page.total==0\" :total=\"page.total\" :page.sync=\"page.current\" :limit.sync=\"page.size\" @pagination=\"getDatas\" />\r\n\r\n    <el-dialog v-loading=\"loading.detail\" custom-class=\"dialog-detail\" :title=\"detail.dialog.title\" :visible.sync=\"detail.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\">\r\n      <el-form ref=\"formDetail\" label-position=\"right\" :label-width=\"detail.dialog.labelWidth\">\r\n        <!--<el-row>-->\r\n        <!--<el-col :xl=\"6\" :md=\"12\" :sm=\"24\">-->\r\n        <!--<el-form-item label=\"客户订单号\">-->\r\n        <!--{{ detail.models.orderDetail.order.id }}-->\r\n        <!--</el-form-item>-->\r\n        <!--</el-col>-->\r\n        <!--</el-row>-->\r\n        <!--<el-row>