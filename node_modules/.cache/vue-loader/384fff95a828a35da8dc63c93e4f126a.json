ACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"goods-output.vue","sourceRoot":"src/views/business-operation","sourcesContent":["<template>\n  <div class=\"app-container list\">\n    <div class=\"toolbar\">\n      <el-cascader\n        v-model=\"query.multiLevelStoreId\"\n        placeholder=\"请选择存储级别\"\n        :options=\"fiveLevelStores\"\n        :props=\"{ expandTrigger: 'hover', checkStrictly: true }\"\n        clearable\n        @change=\"handleStoreChange\"\n        @clear=\"handleQuery\"\n      />\n      <el-cascader\n        v-model=\"query.goodsCategoryId\"\n        placeholder=\"请选择货物类别\"\n        :options=\"goodsCategoryClassificationWithCategory\"\n        :props=\"{ expandTrigger: 'hover', checkStrictly: true }\"\n        clearable\n        @change=\"handleCategoryChange\"\n        @clear=\"handleQuery\"\n      />\n      <el-select v-model=\"query.storageModeId\" class=\"query-item\" style=\"width: 120px\" placeholder=\"存储模式\" clearable @clear=\"handleQuery\">\n        <el-option v-for=\"item in goodsStorageModes\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n      </el-select>\n      <el-select v-model=\"query.inspectorId\" class=\"query-item\" style=\"width: 120px\" placeholder=\"验货人员\" clearable @clear=\"handleQuery\">\n        <el-option v-for=\"item in operators\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n      </el-select>\n      <el-select v-model=\"query.locationType\" class=\"query-item\" style=\"width: 150px\" placeholder=\"正常存储位置\" clearable @clear=\"handleQuery\">\n        <el-option value=\"0\" label=\"运输设备\" />\n        <el-option value=\"1\" label=\"单元格\" />\n        <el-option value=\"2\" label=\"容器\" />\n      </el-select>\n      <el-select v-model=\"query.locked\" class=\"query-item\" style=\"width: 120px\" placeholder=\"锁定状态\" clearable @clear=\"handleQuery\">\n        <el-option value=\"0\" label=\"未锁定\" />\n        <el-option value=\"1\" label=\"部分锁定\" />\n        <el-option value=\"2\" label=\"锁定\" />\n      </el-select>\n      <el-select v-model=\"query.inspectionResult\" class=\"query-item\" style=\"width: 120px\" placeholder=\"验货结果\" clearable @clear=\"handleQuery\">\n        <el-option value=\"0\" label=\"未通过\" />\n        <el-option value=\"1\" label=\"通过\" />\n      </el-select>\n      <el-button class=\"tool tool-query\" type=\"primary\" icon=\"el-icon-search\" @click=\"handleQuery\">查询</el-button>\n      <el-button v-if=\"access.create.allow\" class=\"tool tool-create\" type=\"primary\" icon=\"el-icon-folder-checked\" @click=\"handleOutGoods\">批量确认出库</el-button>\n      <el-button v-if=\"access.delete.allow\" class=\"tool tool-delete\" type=\"danger\" icon=\"el-icon-folder-remove\" @click=\"handleDismiss\">批量解除预出库</el-button>\n    </div>\n\n    <el-table ref=\"listTable\" v-loading=\"loading.list\" v-adaptive=\"{ bottomOffset: 55 }\" height=\"200px\" :data=\"datas\" :default-sort=\"sort\" border fit highlight-current-row @sort-change=\"handleSort\">\n      <el-table-column type=\"selection\" align=\"center\" width=\"35\" />\n      <el-table-column label=\"序号\" type=\"index\" align=\"center\" width=\"65\" fixed>\n        <template slot-scope=\"scope\">\n          <span>{{ (page.current - 1) * page.size + scope.$index + 1 }}</span>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"最近更新时间\" prop=\"updateTime\" sortable align=\"center\" width=\"160\" show-overflow-tooltip />\n      <el-table-column label=\"货物RFID\" prop=\"goodsRfidCode\" sortable align=\"center\" width=\"150\" show-overflow-tooltip />\n      <el-table-column label=\"货物条形码\" prop=\"goodsBarCode\" sortable align=\"center\" width=\"150\" show-overflow-tooltip />\n      <el-table-column label=\"货物类别\" prop=\"goodsCategory.name\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />\n      <el-table-column label=\"货物实际总数量\" prop=\"quality\" sortable align=\"center\" width=\"100\" show-overflow-tooltip />\n      <el-table-column label=\"被锁定的数量\" prop=\"qualityLocked\" sortable align=\"center\" show-overflow-tooltip />\n      <el-table-column label=\"存储模式名称\" prop=\"goodsStoragemode.name\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />\n      <el-table-column label=\"货物存储位置类型\" prop=\"locationType\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />\n\n      <el-table-column fixed=\"right\" label=\"操作\" align=\"center\" width=\"120\">\n        <template slot-scope=\"{row}\">\n          <el-tooltip class=\"item\" effect=\"dark\" content=\"详情\" placement=\"top-end\">\n            <el-button type=\"primary\" plain class=\"button-operate button-detail\" size=\"mini\" @click=\"handleDetail(row)\"><i class=\"vue-icon-detail\" /></el-button>\n          </el-tooltip>\n          <el-tooltip class=\"item\" effect=\"dark\" content=\"确认出库\" placement=\"top-end\">\n            <el-button type=\"primary\" plain class=\"button-operate button-detail\" size=\"mini\" @click=\"handleOutGood(row)\"><i class=\"el-icon-folder-checked\" /></el-button>\n          </el-tooltip>\n          <el-tooltip class=\"item\" effect=\"dark\" content=\"取消预出库\" placement=\"top-end\">\n            <el-button type=\"danger\" plain class=\"button-operate button-detail\" size=\"mini\" @click=\"handleDismissone(row)\"><i class=\"el-icon-folder-remove\" /></el-button>\n          </el-tooltip>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <pagination :hidden=\"page.total==0\" :total=\"page.total\" :page.sync=\"page.current\" :limit.sync=\"page.size\" @pagination=\"getDatas\" />\n\n    <el-dialog v-loading=\"loading.detail\" custom-class=\"dialog-fullscreen dialog-detail\" :title=\"detail.dialog.title\" :visible.sync=\"detail.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\">\n      <el-form ref=\"formDetail\" label-position=\"right\" :label-width=\"detail.dialog.labelWidth\">\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物RFID\">\n              {{ detail.models.goodsRfidCode }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物条形码\">\n              {{ detail.models.goodsBarCode }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物种类\">\n              {{ detail.models.goodsCategory.name }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"存储模式名称\">\n              {{ detail.models.goodsStoragemode.name }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"实际总数量\">\n              {{ detail.models.quality }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"锁定总数量\">\n              {{ detail.models.qualityLocked }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"存储位置类型\">\n              <span v-if=\"detail.models.locationType==0\">运输设备</span>\n              <span v-if=\"detail.models.locationType==1\">单元格</span>\n              <span v-if=\"detail.models.locationType==2\">容器</span>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <!--<el-row>-->\n        <!--<el-col :xl=\"6\" :md=\"12\" :sm=\"24\">-->\n        <!--<el-form-item v-if=\"detail.models.locationType ==0\" label=\"存储的运输设备名称\">-->\n        <!--{{ detail.models.device.name}}}-->\n        <!--</el-form-item>-->\n        <!--</el-col>-->\n        <!--</el-row>-->\n        <!--<el-row>-->\n        <!--<el-col :xl=\"6\" :md=\"12\" :sm=\"24\">-->\n        <!--<el-form-item v-if=\"detail.models.locationType ==1\" label=\"存储的单元格名称\">-->\n        <!--{{ detail.models.cell.name}}}-->\n        <!--</el-form-item>-->\n        <!--</el-col>-->\n        <!--</el-row>-->\n        <!--<el-row>-->\n        <!--<el-col :xl=\"6\" :md=\"12\" :sm=\"24\">-->\n        <!--<el-form-item v-if=\"detail.models.locationType ==2\" label=\"存储的容器名称\">-->\n        <!--{{ detail.models.vessel.name}}}-->\n        <!--</el-form-item>-->\n        <!--</el-col>-->\n        <!--</el-row>-->\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"存储位置条形码\">\n              {{ detail.models.locationBarCode }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"存储位置RFID\">\n              {{ detail.models.locationRfidCode }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"锁定状态\">\n              <span v-if=\"detail.models.locked==0\">未锁定</span>\n              <span v-if=\"detail.models.locked==1\">部分锁定</span>\n              <span v-if=\"detail.models.locked==2\">锁定</span>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"验货结果\">\n              <span v-if=\"detail.models.inspectionResult==0\">未通过</span>\n              <span v-if=\"detail.models.inspectionResult==1\">通过</span>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"验货时间\">\n              {{ detail.models.inspectionTime }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"验货人员\">\n              {{ detail.models.inspector===null?'无':detail.models.inspector.userName }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"最后更新时间\">\n              {{ detail.models.updateTime }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"最后更新操作人员\">\n              {{ detail.models.operator.userName }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n      </el-form>\n    </el-dialog>\n\n    <el-dialog :title=\"out.dialog.title\" :visible.sync=\"out.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\" :close-on-click-modal=\"false\">\n      <el-form ref=\"formCreate\" :rules=\"out.rules\" :model=\"out.models\" label-position=\"right\" :label-width=\"out.dialog.labelWidth\">\n        <el-row>\n          <el-col :xl=\"24\">\n            <el-form-item label=\"领料详单编号\" prop=\"requestDetailCode\">\n              <el-input v-model=\"out.models.requestDetailCode\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"24\">\n            <el-form-item label=\"类型\" prop=\"storeOperateTypeId\">\n              <el-select v-model=\"out.models.storeOperateTypeId\" clearable filterable>\n                <el-option v-for=\"item in storeOperateTypes\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n              </el-select>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"24\">\n            <el-form-item label=\"提货人员信息\" prop=\"deliveryman\">\n              <el-input v-model=\"out.models.deliveryman\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"24\">\n            <el-form-item label=\"出库备注描述\" prop=\"description\">\n              <el-input v-model=\"out.models.description\" type=\"textarea\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"out.dialog.visible = false\">取消</el-button>\n        <el-button type=\"primary\" @click=\"outGood()\">提交</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport adaptive from '@/directive/el-table'\nimport setRule from '@/utils/form-validate'\nimport Pagination from '@/components/Pagination'\nimport * as storeOperateType from '@/api/log-manage/store-operate-type'\nimport * as goods from '@/api/goods-manage/goods'\nimport * as goodsUnit from '@/api/goods-manage/goods-unit'\nimport * as operator from '@/api/system-manage/user'\nimport * as goodsCategoryClassification from '@/api/goods-manage/goods-category-classification'\nimport * as store from '@/api/store-manage/store'\nimport * as goodsStorageMode from '@/api/goods-manage/goods-storageMode'\n\nexport default {\n  name: 'GoodsOutput',\n  components: { Pagination },\n  directives: { adaptive },\n  data() {\n    return {\n      access: this.$store.getters.access['BusinessOperation']['StoreOperateType'],\n      datas: null,\n      fiveLevelStores: null,\n      operators: null,\n      goodsCategoryClassificationWithCategory: null,\n      storeOperateTypes: null,\n      goodsStorageModes: null,\n      query: { multiLevelStoreId: null, goodsCategoryId: null, storageModeId: null, locationType: null, locked: null, inspectionResult: null, inspectorId: null },\n      page: { total: 0, current: 1, size: 20 },\n      sort: { prop: 'sort', order: 'ascending' },\n      detail: {\n        dialog: { title: '货物信息', visible: false, labelWidth: '150px' },\n        models: { goodsBarCode: null, goodsRfidCode: null, goodsCategory: { name: null, groupId: null }, goodsStoragemode: { name: null }, quality: null, qualityLocked: null, locationType: null, locationBarCode: null, locationRfidCode: null, locked: null, inspectionResult: null, inspectionTime: null, inspector: { userName: null }, updateTime: null, operator: { userName: null }}\n      },\n      out: {\n        dialog: { title: '出库信息', visible: false, labelWidth: '120px' },\n        models: { requestDetailCode: null, deliveryman: null, description: null, ids: [], storeOperateTypeId: null },\n        rules: {\n          requestDetailCode: setRule('领料详单编号', [{ required: true }, { length: [0, 50] }]),\n          storeOperateTypeId: setRule('类型', [{ required: true }, { selected: true }]),\n          deliveryman: setRule('提货人员信息', [{ length: [0, 255] }]),\n          description: setRule('出库备注描述', [{ length: [0, 255] }])\n        }\n      },\n      loading: { list: true, export: false, detail: false, update: false },\n      sortOrders: ['ascending', 'descending']\n    }\n  },\n  created() {\n    this.getDatas()\n    this.getStoreOperateTypes()\n    this.getOperators()\n    this.getGoodsCategoryClassificationWithCategory()\n    this.getFiveLevelStores()\n    this.getGoodsStorageModes()\n  },\n  methods: {\n    getDatas() {\n      this.loading.list = true\n      const status = 1\n      goods.getList(this.query, this.page, this.sort, status).then(response => {\n        this.datas = response.data.items\n        this.page.total = response.data.total\n        this.loading.list = false\n      }).catch(reject => {\n        this.loading.list = false\n      })\n    },\n    getStoreOperateTypes() {\n      const id = 0\n      storeOperateType.getlist(id).then(response => {\n        this.storeOperateTypes = response.data\n      }).catch(reject => {\n        this.loading.list = false\n      })\n    },\n    getGoodsUnits() {\n      goodsUnit.getlist().then(response => {\n        this.goodsUnits = response.data\n      }).catch(reject => {\n      })\n    },\n    getOperators() {\n      operator.getlist().then(response => {\n        this.operators = response.data\n      }).catch(reject => {\n      })\n    },\n    getGoodsStorageModes() {\n      goodsStorageMode.getlist().then(response => {\n        this.goodsStorageModes = response.data\n      }).catch(reject => {\n      })\n    },\n    getFiveLevelStores() {\n      store.getFiveLevelStores().then(response => {\n        this.fiveLevelStores = response.data\n      }).catch(reject => {\n      })\n    },\n    getGoodsCategoryClassificationWithCategory() {\n      goodsCategoryClassification.getlist(2).then(response => {\n        this.goodsCategoryClassificationWithCategory = response.data\n      }).catch(reject => {\n      })\n    },\n    handleQuery() {\n      this.page.current = 1\n      this.getDatas()\n    },\n    handleSort(data) {\n      this.sort.prop = data.prop\n      this.sort.order = data.order\n      this.getDatas()\n    },\n    handleDetail(row) {\n      // 若列表数据展示了全部属性，则可直接拷贝列表数据\n      // this.detail.models = Object.assign({}, row)\n      this.detail.dialog.visible = true\n      this.loading.detail = true\n      goods.get(row.id).then(response => {\n        this.detail.models = response.data\n        this.loading.detail = false\n      }).catch(reject => {\n        this.loading.detail = false\n      })\n    },\n    handleOutGoods() {\n      if (this.$refs.listTable.selection.length === 0) {\n        this.$message({ type: 'warning', message: '请在下面表格的左侧勾选需要出库的货物', duration: 4000 })\n        return false\n      }\n      // const ids = []\n      for (const item of this.$refs.listTable.selection) {\n        this.out.models.ids.push(item.id)\n      }\n      this.out.dialog.visible = true\n    },\n    outGood() {\n      goods.out(this.out.models).then(response => {\n        // 重新获取数据\n        this.getDatas()\n        this.out.dialog.visible = false\n        this.out.models.ids.splice(0, this.out.models.ids.length)\n        this.out.models.requestDetailCode = ''\n      }).catch(reject => {\n        this.out.models.ids.splice(0, this.out.models.ids.length)\n        this.out.dialog.visible = false\n      })\n    },\n    handleOutGood(row) {\n      // console.log(this.out.models.ids)\n      this.out.models.ids = [row.id]\n      // this.out.model.ids.push(row.id)\n      this.out.dialog.visible = true\n      // this.outGood([row.id])\n    },\n    handleDismissone(row) {\n      this.dismissData([row.id])\n    },\n    handleDismiss(row) {\n      if (this.$refs.listTable.selection.length === 0) {\n        this.$message({ type: 'warning', message: '请在下面表格的左侧勾选需要解除预出库的货物', duration: 4000 })\n        return false\n      }\n      const ids = []\n      for (const item of this.$refs.listTable.selection) {\n        ids.push(item.id)\n      }\n      this.dismissData(ids)\n    },\n    dismissData(ids) {\n      // console.log(ids)\n      this.$confirm('是否确定解除预出库?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        goods.dismissPreOutput(ids).then(response => {\n          // 重新获取数据\n          this.getDatas()\n        }).catch(reject => {\n        })\n      })\n    },\n    handleStoreChange(value) {\n      const id = value[value.length - 1]\n      this.query.multiLevelStoreId = id\n    },\n    handleCategoryChange(value) {\n      const id = value[value.length - 1]\n      this.query.goodsCategoryId = id\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n</style>\n"]}]}