   <el-option label=\"全未入库\" value=\"0\" />\r\n        <el-option label=\"部分出库\" value=\"1\" />\r\n        <el-option label=\"完全出库\" value=\"2\" />\r\n        <el-option label=\"订单完成\" value=\"3\" />\r\n      </el-select>\r\n    </div>\r\n    <div class=\"toolbar\">\r\n      <el-date-picker\r\n        v-model=\"value1\"\r\n        align=\"right\"\r\n        end-placeholder=\"结束日期\"\r\n        range-separator=\"至\"\r\n        start-placeholder=\"开始日期\"\r\n        type=\"datetimerange\"\r\n        value-format=\"yyyy-MM-dd HH:mm:ss\"\r\n        @change=\"getTime\"\r\n      />\r\n      <el-button class=\"tool tool-query\" type=\"primary\" icon=\"el-icon-search\" @click=\"handleQuery\">查询</el-button>\r\n      <el-button v-if=\"access.record.allow\" class=\"tool tool-create\" type=\"primary\" icon=\"vue-icon-create\" @click=\"handleCreate\">添加</el-button>\r\n      <el-button v-if=\"access.delete.allow\" class=\"tool tool-delete\" type=\"danger\" icon=\"vue-icon-delete\" @click=\"handleDeletes\">批量删除</el-button>\r\n      <el-button v-if=\"access.export.allow\" class=\"tool tool-export\" :loading=\"loading.export\" type=\"primary\" icon=\"vue-icon-excel\" @click=\"handleExport\">导出</el-button>\r\n    </div>\r\n\r\n    <el-table ref=\"listTable\" v-loading=\"loading.list\" v-adaptive=\"{ bottomOffset: 55 }\" height=\"200px\" :data=\"datas\" :default-sort=\"sort\" border fit highlight-current-row @sort-change=\"handleSort\">\r\n      <el-table-column type=\"selection\" align=\"center\" width=\"35\" />\r\n      <el-table-column label=\"序号\" type=\"index\" align=\"center\" width=\"65\" fixed>\r\n        <template slot-scope=\"scope\">\r\n          <span>{{ (page.current - 1) * page.size + scope.$index + 1 }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"订单编号\" prop=\"code\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />\r\n      <el-table-column label=\"订单名称\" prop=\"name\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />\r\n      <el-table-column label=\"客户名称\" prop=\"customer.name\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />\r\n      <el-table-column label=\"制单时间\" prop=\"createTime\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />\r\n      <el-table-column label=\"录入人员名称\" prop=\"creater.userName\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />\r\n      <el-table-column label=\"订购总金额\" prop=\"amount\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />\r\n      <el-table-column label=\"订单成交金额\" prop=\"amountDone\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />\r\n      <el-table-column label=\"订单预付金额\" prop=\"amountPre\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />\r\n      <el-table-column label=\"订单最终支付金额\" prop=\"amountReal\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />\r\n      <el-table-column label=\"订单交货时间\" prop=\"deliveryTime\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"200\" show-overflow-tooltip />\r\n      <el-table-column label=\"订单状态\" prop=\"status\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip>\r\n        <template slot-scope=\"{row}\">\r\n          <span v-if=\"row.status== 0\">全未入库</span>\r\n          <span v-if=\"row.status== 1\">部分入库</span>\r\n          <span v-if=\"row.status== 2\">完全出库</span>\r\n          <span v-if=\"row.status== 3\">订单完成</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"订单状态改变时间\" prop=\"statusTime\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" show-overflow-tooltip />\r\n      <el-table-column fixed=\"right\" label=\"操作\" align=\"center\" width=\"140\">\r\n        <template slot-scope=\"{row}\">\r\n          <!-- todo===页面跳转问题-->\r\n          <!--<el-tooltip class=\"item\" effect=\"dark\" content=\"领料单管理\" placement=\"top-end\">-->\r\n          <!--<el-button type=\"primary\" plain class=\"button-operate button-detail\" size=\"mini\" @click=\"handleCreateRequest(row)\"><i class=\"el-icon-circle-plus\" /></el-button>-->\r\n          <!--</el-tooltip>-->\r\n          <!--<el-tooltip class=\"item\" effect=\"dark\" content=\"详单管理\" placement=\"top-end\">-->\r\n          <!--<el-button type=\"primary\" plain class=\"button-operate button-detail\" size=\"mini\" @click=\"handleCreateDetail(row)\"><i class=\"el-icon-circle-plus\" /></el-button>-->\r\n          <!--</el-tooltip>-->\r\n          <el-tooltip v-if=\"access.detail.allow\" class=\"item\" effect=\"dark\" content=\"详情\" placement=\"top-end\">\r\n            <el-button type=\"primary\" plain class=\"button-operate button-detail\" size=\"mini\" @click=\"handleDetail(row)\"><i class=\"vue-icon-detail\" /></el-button>\r\n          </el-tooltip>\r\n          <el-tooltip v-if=\"access.update.allow\" class=\"item\" effect=\"dark\" content=\"编辑\" placement=\"top-end\">\r\n            <el-button type=\"primary\" plain class=\"button-operate button-update\" size=\"mini\" @click=\"handleUpdate(row)\"><i class=\"vue-icon-update\" /></el-button>\r\n          </el-tooltip>\r\n          <el-tooltip v-if=\"access.delete.allow\" class=\"item\" effect=\"dark\" content=\"删除\" placement=\"top-end\">\r\n            <el-button type=\"danger\" plain class=\"button-operate button-delete\" size=\"mini\" @click=\"handleDelete(row)\"><i class=\"vue-icon-delete\" /></el-button>\r\n          </el-tooltip>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <pagination :hidden=\"page.total==0\" :total=\"page.total\" :page.sync=\"page.current\" :limit.sync=\"page.size\" @pagination=\"getDatas\" />\r\n\r\n    <el-dialog v-loading=\"loading.detail\" custom-class=\"dialog-detail\" :title=\"detail.dialog.title\" :visible.sync=\"detail.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\">\r\n      <el-form ref=\"formDetail\" label-position=\"right\" :label-width=\"detail.dialog.labelWidth\">\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单编号\">\r\n              {{ detail.models.code }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单名称\">\r\n              {{ detail.models.name }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"客户姓名\">\r\n              {{ detail.models.customer.name }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"制单时间\">\r\n              {{ detail.models.createTime }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"录入人员名称\">\r\n              {{ detail.models.creater.userName }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订购总金额\">\r\n              {{ detail.models.amount }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单成交金额\">\r\n              {{ detail.models.amountDone }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单预付金额\">\r\n              {{ detail.models.amountPre }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单最终支付金额\">\r\n              {{ detail.models.amountReal }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单交货时间\">\r\n              {{ detail.models.deliveryTime }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单实际交货时间\">\r\n              {{ detail.models.deliveryTimeReal }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单描述\">\r\n              {{ detail.models.description }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单状态\">\r\n              <span v-if=\"detail.models.status==0\">全未出库</span>\r\n              <span v-if=\"detail.models.status==1\">部分出库</span>\r\n              <span v-if=\"detail.models.status==2\">完全出库</span>\r\n              <span v-if=\"detail.models.status==3\">订单完成</span>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单状态改变时间\">\r\n              {{ detail.models.statusTime }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n    </el-dialog>\r\n\r\n    <el-dialog custom-class=\"dialog-create\" :title=\"create.dialog.title\" :visible.sync=\"create.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\" :close-on-click-modal=\"false\">\r\n      <el-form ref=\"formCreate\" :rules=\"create.rules\" :model=\"create.models\" label-position=\"right\" :label-width=\"create.dialog.labelWidth\">\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单编号\" prop=\"order.code\">\r\n              <el-input v-model=\"create.models.order.code\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单名称\" prop=\"order.name\">\r\n              <el-input v-model=\"create.models.order.name\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"选择客户\" prop=\"order.customerId\">\r\n              <el-select v-model=\"create.models.order.customerId\" clearable>\r\n                <el-option v-for=\"item in customers\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\r\n              </el-select>\r\n              <el-tooltip class=\"item\" effect=\"dark\" content=\"添加用户\" placement=\"top-end\">\r\n                <el-button type=\"primary\" plain class=\"button-operate button-detail\" circle icon=\"el-icon-edit\" @click=\"handleCreateCustomer()\" />\r\n              </el-tooltip>\r\n              <!--<el-button type=\"primary\" icon=\"el-icon-edit\" content=\"添加用户\" circle show-overflow-tooltip />-->\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单成交金额\" prop=\"order.amount\">\r\n              <el-input v-model=\"create.models.order.amount\" type=\"number\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单预付金额\" prop=\"order.amountPre\">\r\n              <el-input v-model=\"create.models.order.amountPre\" type=\"number\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单交货时间\" prop=\"order.deliveryTime\">\r\n              <el-date-picker\r\n                v-model=\"create.models.order.deliveryTime \"\r\n                type=\"datetime\"\r\n                placeholder=\"选择日期时间\"\r\n                default-time=\"12:00:00\"\r\n              />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"备注\" prop=\"order.description\">\r\n              <el-input v-model=\"create.models.order.description\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <!-- 书写扩展属性值 -->\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item v-for=\"(item,index) in orderProperties\" :key=\"item.id\" :label=\"item.name\">\r\n              <el-input v-model=\"OrderPropertyValue[index].value\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <div v-for=\"(orderPropertyValue, index) in create.models.orderPropertyValue\" :key=\"index\">\r\n          <el-row>\r\n            <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n              <el-form-item\r\n                v-if=\"orderPropertyValue.orderProperty\"\r\n                :key=\"orderPropertyValue.key\"\r\n                :label=\"'新增属性' + (index + 1)\"\r\n                :prop=\"'orderPropertyValue.' + index + '.orderProperty.name'\"\r\n                :rules=\"{\r\n                  required: true, message: '请输入扩展属性名称', trigger: 'blur',length: [0, 20]\r\n                }\"\r\n              >\r\n                <span>扩展属性名称</span><el-input v-model=\"orderPropertyValue.orderProperty.name\" />\r\n                <span>扩展属性描述(可空)</span><el-input v-model=\"orderPropertyValue.orderProperty.description\" />\r\n                <span>扩展属性值(可空)</span><el-input v-model=\"orderPropertyValue.value\" />\r\n                <el-button @click.prevent=\"removeDomain(orderPropertyValue)\">删除</el-button>\r\n              </el-form-item>\r\n            </el-col>\r\n          </el-row>\r\n        </div>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"create.dialog.visible = false\">取消</el-button>\r\n        <el-button @click=\"addDomain\">新增扩展属性</el-button>\r\n        <el-button type=\"primary\" @click=\"createData()\">提交</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n    <el-dialog v-loading=\"loading.update\" custom-class=\"dialog-update\" :title=\"update.dialog.title\" :visible.sync=\"update.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\" :close-on-click-modal=\"false\">\r\n      <el-form ref=\"formUpdate\" :rules=\"update.rules\" :model=\"update.models\" label-position=\"right\" :label-width=\"update.dialog.labelWidth\">\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单编号\" prop=\"order.code\">\r\n              <el-input v-model=\"update.models.order.code\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"选择客户\" prop=\"order.customerId\">\r\n              <el-select v-model=\"update.models.order.customerId\" clearable>\r\n                <el-option v-for=\"item in customers\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\r\n              </el-select>\r\n              <el-tooltip class=\"item\" effect=\"dark\" content=\"添加用户\" placement=\"top-end\">\r\n                <el-button type=\"primary\" plain class=\"button-operate button-detail\" circle icon=\"el-icon-edit\" @click=\"handleCreateCustomer()\" />\r\n              </el-tooltip>\r\n              <!--<el-button type=\"primary\" icon=\"el-icon-edit\" content=\"添加用户\" circle show-overflow-tooltip />-->\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单成交金额\" prop=\"order.amount\">\r\n              <el-input v-model=\"update.models.order.amount\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单预付金额\" prop=\"order.amountPre\">\r\n              <el-input v-model=\"update.models.order.amountPre\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单交货时间\" prop=\"order.deliveryTime\">\r\n              <el-date-picker\r\n                v-model=\"update.models.order.deliveryTime \"\r\n                type=\"datetime\"\r\n                placeholder=\"选择日期时间\"\r\n                default-time=\"12:00:00\"\r\n              />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"备注\" prop=\"order.description\">\r\n              <el-input v-model=\"update.models.order.description\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"已有扩展属性\">\r\n              <el-tag v-for=\"item in orderProperties\" :key=\"item.id\">{{ item.name }}</el-tag>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <!--<el-row>-->\r\n        <!--<el-col :xl=\"6\" :md=\"12\" :sm=\"24\">-->\r\n        <!--<el-form-item-->\r\n        <!--v-for=\"(orderPropertyValue, index) in update.models.orderPropertyValue\"-->\r\n        <!--:key=\"orderPropertyValue.key\"-->\r\n        <!--:label=\"'新增属性' + (index + 1)\"-->\r\n        <!--:prop=\"'orderPropertyValue.' + index + '.orderProperty.name'\"-->\r\n        <!--:rules=\"{-->\r\n        <!--required: true, message: '请输入扩展属性名称', trigger: 'blur',length: [0, 20]-->\r\n        <!--}\"-->\r\n        <!--&gt;-->\r\n        <!--<span>扩展属性名称</span><el-input v-model=\"orderPropertyValue.orderProperty.name\" />-->\r\n        <!--<span>扩展属性描述(可空)</span><el-input v-model=\"orderPropertyValue.orderProperty.description\" />-->\r\n        <!--<span>扩展属性值(可空)</span><el-input v-model=\"orderPropertyValue.value\" />-->\r\n        <!--<el-button @click.prevent=\"removeDomain(orderPropertyValue)\">删除</el-button>-->\r\n        <!--</el-form-item>-->\r\n        <!--</el-col>-->\r\n        <!--</el-row>-->\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"update.dialog.visible = false\">取消</el-button>\r\n        <!--<el-button @click=\"adddomain\">新增扩展属性</el-button>-->\r\n        <el-button type=\"primary\" @click=\"updateData()\">提交</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport adaptive from '@/directive/el-table'\r\nimport setRule from '@/utils/form-validate'\r\nimport Pagination from '@/components/Pagination'\r\nimport * as order from '@/api/order-manage/order'\r\nimport * as customer from '@/api/order-manage/customer'\r\nimport * as user from '@/api/system-manage/user'\r\nimport * as orderProperty from '@/api/order-manage/order-property'\r\n\r\nexport default {\r\n  name: 'Order',\r\n  components: { Pagination },\r\n  directives: { adaptive },\r\n  data() {\r\n    return {\r\n      access: this.$store.getters.access['OrderBusiness']['Order'],\r\n      datas: null,\r\n      customers: null,\r\n      creaters: null,\r\n      orderProperties: null,\r\n      OrderPropertyValue: [],\r\n      value1: [],\r\n      query: { code: null, name: null, customerId: null, createrId: null, status: null },\r\n      page: { total: 0, current: 1, size: 20 },\r\n      sort: { prop: 'sort', order: 'ascending' },\r\n      detail: {\r\n        dialog: { title: '订单录入信息', visible: false, labelWidth: '150px' },\r\n        models: {\r\n          code: null,\r\n          name: null,\r\n          customer: { name: null },\r\n          creater: { userName: null },\r\n          amount: null,\r\n          amountDone: null,\r\n          amountPre: null,\r\n          amountReal: null,\r\n          deliveryTime: null,\r\n          deliveryTImeReal: null,\r\n          description: null,\r\n          status: null,\r\n          statusTime: null\r\n        }\r\n      },\r\n      create: {\r\n        dialog: { title: '添加订单', visible: false, labelWidth: '150px' },\r\n        models: {\r\n          order: { code: null, name: null, customerId: null, amount: null, amountPre: null, deliveryTime: null, description: null },\r\n          orderPropertyValue: []\r\n        },\r\n        rules: {\r\n          order: {\r\n            code: setRule('订单编号', [{ required: true }, { length: [0, 32] }]),\r\n            name: setRule('订单名称', [{ required: true }, { length: [0, 255] }]),\r\n            customerId: setRule('客户', [{ selected: true }]),\r\n            amount: setRule('订单成交金额', [{ required: true }]),\r\n            amountPre: setRule('订单预付金额', [{ required: true }]),\r\n            deliveryTime: setRule('订单交货时间', [{ required: true }])\r\n          }\r\n        }\r\n      },\r\n      update: {\r\n        dialog: { title: '编辑订单信息', visible: false, labelWidth: '120px' },\r\n        models: {\r\n          order: { code: null, customerId: null, createrId: null, amount: null, amountPre: null, deliveryTime: null, description: null },\r\n          orderPropertyValue: []\r\n        },\r\n        rules: {\r\n          order: {\r\n            code: setRule('订单类型编号', [{ required: true }, { length: [0, 32] }]),\r\n            customerId: setRule('客户', [{ selected: true }]),\r\n            amount: setRule('订单成交金额', [{ required: true }]),\r\n            amountPre: setRule('订单预付金额', [{ required: true }]),\r\n            deliveryTime: setRule('订单交货时间', [{ required: true }])\r\n          }\r\n        }\r\n      },\r\n      loading: { list: true, export: false, detail: false, update: false },\r\n      sortOrders: ['ascending', 'descending']\r\n    }\r\n  },\r\n  created() {\r\n    this.getDatas()\r\n    this.getCustomers()\r\n    this.getUsers()\r\n    this.getOrderProperties()\r\n  },\r\n  methods: {\r\n    removeDomain(item) {\r\n      var index = this.create.models.orderPropertyValue.indexOf(item)\r\n      if (index !== -1) {\r\n        this.create.models.orderPropertyValue.splice(index, 1)\r\n      }\r\n    },\r\n    addDomain() {\r\n      this.create.models.orderPropertyValue.push({\r\n        orderProperty: {\r\n          name: null,\r\n          description: null\r\n        },\r\n        value: null,\r\n        propertyId: null\r\n      })\r\n    },\r\n    getCustomers() {\r\n      customer.getlist().then(response => {\r\n        this.customers = response.data\r\n        this.loading.detail = false\r\n      }).catch(reject => {\r\n        this.loading.detail = false\r\n      })\r\n    },\r\n    getUsers() {\r\n      user.getlist().then(response => {\r\n        this.creaters = response.data\r\n      }).catch(reject => {\r\n      })\r\n    },\r\n    getOrderProperties() {\r\n      const orderType = 0\r\n      orderProperty.getlist(orderType).then(response => {\r\n        this.orderProperties = response.data\r\n        // 初始化OrderPropertyValue 数组，每条数据依次赋上propertyId的值\r\n        for (let i = 0; i < response.data.length; i++) {\r\n          this.OrderPropertyValue.push({\r\n            value: null,\r\n            propertyId: null\r\n          })\r\n          this.OrderPropertyValue[i].propertyId = response.data[i].id\r\n        }\r\n        this.loading.detail = false\r\n      }).catch(reject => {\r\n        this.loading.detail = false\r\n      })\r\n    },\r\n    getDatas() {\r\n      this.loading.list = true\r\n      order.getList(this.query, this.page, this.sort).then(response => {\r\n        this.datas = response.data.items\r\n        this.page.total = response.data.total\r\n        this.loading.list = false\r\n      }).catch(reject => {\r\n        this.loading.list = false\r\n      })\r\n    },\r\n    getTime(datetimerange) {\r\n      this.query.loginTime = this.value1[0]\r\n      this.query.loginOutTime = this.value1[1]\r\n    },\r\n    handleQuery() {\r\n      this.page.current = 1\r\n      this.getDatas()\r\n    },\r\n    handleSort(data) {\r\n      this.sort.prop = data.prop\r\n      this.sort.order = data.order\r\n      this.getDatas()\r\n    },\r\n    handleClean() {\r\n      this.create.models.order.propertyId = null\r\n    },\r\n    handleDetail(row) {\r\n      // 若列表数据展示了全部属性，则可直接拷贝列表数据\r\n      // this.detail.models = Object.assign({}, row)\r\n      this.detail.dialog.visible = true\r\n      this.loading.detail = true\r\n      order.get(row.id).then(response => {\r\n        this.detail.models = response.data\r\n        console.log(this.detail.models)\r\n        this.loading.detail = false\r\n      }).catch(reject => {\r\n        this.loading.detail = false\r\n      })\r\n    },\r\n    handleCreateCustomer() {\r\n      this.$router.push({\r\n        path: '/order-manage/customer',\r\n        query: { flag: true }\r\n      })\r\n    },\r\n    handleCreateDetail(row) {\r\n      const orderId = row.id\r\n      const orderCode = row.code\r\n      this.$router.push({\r\n        path: '/order-manage/order-detail',\r\n        query: {\r\n          orderCode: orderCode,\r\n          orderId: orderId\r\n        }\r\n      })\r\n    },\r\n    handleCreateRequest(row) {\r\n      const orderId = row.id\r\n      const orderCode = row.code\r\n      this.$router.push({\r\n        path: '/goods-manage/request',\r\n        query: {\r\n          orderId: orderId,\r\n          orderCode: orderCode\r\n        }\r\n      })\r\n    },\r\n    handleCreate() {\r\n      this.create.dialog.visible = true\r\n    },\r\n    createData() {\r\n      this.$refs['formCreate'].validate((valid) => {\r\n        if (!valid) return false\r\n        this.create.models.orderPropertyValue = this.OrderPropertyValue.concat(this.create.models.orderPropertyValue)\r\n        order.create(this.create.models).then(response => {\r\n          // 为方便连续添加，create对话框不关闭\r\n          // 需要清空的表单，手动清空，至少清空一个必填项，防止点击两遍\r\n          this.create.models.order.code = ''\r\n          this.create.models.order.customerId = ''\r\n          // 重新获取数据\r\n          this.getDatas()\r\n        }).catch(reject => {\r\n        })\r\n      })\r\n    },\r\n    handleUpdate(row) {\r\n      // 若列表数据展示了全部属性，则可直接拷贝列表数据用于编辑\r\n      // this.update.models = Object.assign({}, row)\r\n      this.update.dialog.visible = true\r\n      this.loading.update = true\r\n      order.get(row.id).then(response => {\r\n        this.update.models.order = response.data\r\n        this.loading.update = false\r\n      }).catch(reject => {\r\n        this.loading.update = false\r\n      })\r\n      this.$nextTick(() => {\r\n        this.$refs['formUpdate'].clearValidate()\r\n      })\r\n    },\r\n    updateData() {\r\n      this.$refs['formUpdate'].validate((valid) => {\r\n        if (!valid) return false\r\n        const tempData = Object.assign({}, this.update.models)\r\n        order.update(tempData).then(response => {\r\n          // 重新获取数据\r\n          this.getDatas()\r\n          this.update.dialog.visible = false\r\n        }).catch(reject => {\r\n        })\r\n      })\r\n    },\r\n    handleDelete(row) {\r\n      this.deleteData([row.id])\r\n    },\r\n    handleDeletes(row) {\r\n      if (this.$refs.listTable.selection.length === 0) {\r\n        this.$message({ type: 'warning', message: '请在下面表格的左侧勾选需要删除的数据', duration: 4000 })\r\n        return false\r\n      }\r\n      const ids = []\r\n      for (const item of this.$refs.listTable.selection) {\r\n        ids.push(item.id)\r\n      }\r\n      this.deleteData(ids)\r\n    },\r\n    deleteData(ids) {\r\n      this.$confirm('数据删除后，不可恢复, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        order.del(ids).then(response => {\r\n          // 重新获取数据\r\n          this.getDatas()\r\n        }).catch(reject => {\r\n        })\r\n      })\r\n    },\r\n    handleExport() {\r\n      this.loading.export = true\r\n      order.exportExcel(this.query, this.page, this.sort).then(response => {\r\n        const blob = new Blob([response])\r\n        const fileName = '订单.xls'\r\n        const elink = document.createElement('a')\r\n        elink.onload = () => { URL.revokeObjectURL(elink.href) }\r\n        elink.download = fileName\r\n        elink.style.display = 'none'\r\n        elink.href = URL.createObjectURL(blob)\r\n        document.body.appendChild(elink)\r\n        elink.click()\r\n        document.body.removeChild(elink)\r\n        this.loading.export = false\r\n      }).catch(reject => {\r\n        this.loading.export = false\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n</style>\r\n"]}]}