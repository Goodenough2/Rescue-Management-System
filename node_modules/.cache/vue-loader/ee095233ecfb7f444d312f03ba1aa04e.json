CA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"order-detail.vue","sourceRoot":"src/views/order-business","sourcesContent":["<template>\r\n  <div class=\"app-container list\">\r\n    <div class=\"toolbar\">\r\n      <el-input v-model.trim=\"query.orderCode\" class=\"query-item\" style=\"width: 200px\" placeholder=\"客户订单号\" clearable @clear=\"handleQuery\" />\r\n      <el-input v-model.trim=\"query.code\" class=\"query-item\" style=\"width: 200px\" placeholder=\"订单详单编号\" clearable @clear=\"handleQuery\" />\r\n      <el-input v-model.trim=\"query.name\" class=\"query-item\" style=\"width: 200px\" placeholder=\"订单详单名称\" clearable @clear=\"handleQuery\" />\r\n      <el-select v-model=\"query.status\" class=\"query-item\" style=\"width: 120px\" placeholder=\"订单状态\" clearable @clear=\"handleQuery\">\r\n        <el-option label=\"全未入库\" value=\"0\" />\r\n        <el-option label=\"部分出库\" value=\"1\" />\r\n        <el-option label=\"完全出库\" value=\"2\" />\r\n      </el-select>\r\n      <el-select v-model=\"query.goodsCategoryId\" class=\"query-item\" style=\"width: 200px\" placeholder=\"货物类别\" clearable @clear=\"handleQuery\">\r\n        <el-option v-for=\"item in goodsCategories\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\r\n      </el-select>\r\n      <el-select v-model=\"query.operatorId\" class=\"query-item\" style=\"width: 200px\" placeholder=\"状态更新人员\" clearable @clear=\"handleQuery\">\r\n        <el-option v-for=\"item in operators\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\r\n      </el-select>\r\n      <el-button class=\"tool tool-query\" type=\"primary\" icon=\"el-icon-search\" @click=\"handleQuery\">查询</el-button>\r\n      <el-button v-if=\"access.record.allow\" class=\"tool tool-create\" type=\"primary\" icon=\"vue-icon-create\" @click=\"handleCreate\">添加</el-button>\r\n      <el-button v-if=\"access.delete.allow\" class=\"tool tool-delete\" type=\"danger\" icon=\"vue-icon-delete\" @click=\"handleDeletes\">批量删除</el-button>\r\n      <el-button v-if=\"access.export.allow\" class=\"tool tool-export\" :loading=\"loading.export\" type=\"primary\" icon=\"vue-icon-excel\" @click=\"handleExport\">导出</el-button>\r\n    </div>\r\n\r\n    <el-table ref=\"listTable\" v-loading=\"loading.list\" v-adaptive=\"{ bottomOffset: 55 }\" height=\"200px\" :data=\"datas\" :default-sort=\"sort\" border fit highlight-current-row @sort-change=\"handleSort\">\r\n      <el-table-column type=\"selection\" align=\"center\" width=\"35\" />\r\n      <el-table-column label=\"序号\" type=\"index\" align=\"center\" width=\"65\" fixed>\r\n        <template slot-scope=\"scope\">\r\n          <span>{{ (page.current - 1) * page.size + scope.$index + 1 }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"客户订单号\" prop=\"order.code\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"订单详单编号\" prop=\"code\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"订单详单名称\" prop=\"name\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"货物类别\" prop=\"goodsCategory.name\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"订购数量\" prop=\"quality\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"实际出库数量\" prop=\"qualityReal\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"订购金额\" prop=\"amount\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"交货时间\" prop=\"deliveryTime\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"订单状态\" prop=\"status\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip>\r\n        <template slot-scope=\"{row}\">\r\n          <span v-if=\"row.status==0\">全未出库</span>\r\n          <span v-if=\"row.status==1\">部分出库</span>\r\n          <span v-if=\"row.status==2\">完全出库</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"状态更新人员\" prop=\"operator.userName\" :sort-orders=\"sortOrders\" align=\"center\" show-overflow-tooltip />\r\n      <el-table-column fixed=\"right\" label=\"操作\" align=\"center\" width=\"140\">\r\n        <template slot-scope=\"{row}\">\r\n          <!-- todo=====页面跳转 -->\r\n          <!--<el-tooltip class=\"item\" effect=\"dark\" content=\"物料管理\" placement=\"top-end\">&ndash;&gt;-->\r\n          <!--<el-button type=\"primary\" plain class=\"button-operate button-detail\" size=\"mini\" @click=\"handleCreateMaterial(row)\"><i class=\"el-icon-circle-plus\" /></el-button>-->\r\n          <!--</el-tooltip>-->\r\n          <el-tooltip v-if=\"access.detail.allow\" class=\"item\" effect=\"dark\" content=\"详情\" placement=\"top-end\">\r\n            <el-button type=\"primary\" plain class=\"button-operate button-detail\" size=\"mini\" @click=\"handleDetail(row)\"><i class=\"vue-icon-detail\" /></el-button>\r\n          </el-tooltip>\r\n          <el-tooltip v-if=\"access.update.allow\" class=\"item\" effect=\"dark\" content=\"编辑\" placement=\"top-end\">\r\n            <el-button type=\"primary\" plain class=\"button-operate button-update\" size=\"mini\" @click=\"handleUpdate(row)\"><i class=\"vue-icon-update\" /></el-button>\r\n          </el-tooltip>\r\n          <el-tooltip v-if=\"access.delete.allow\" class=\"item\" effect=\"dark\" content=\"删除\" placement=\"top-end\">\r\n            <el-button type=\"danger\" plain class=\"button-operate button-delete\" size=\"mini\" @click=\"handleDelete(row)\"><i class=\"vue-icon-delete\" /></el-button>\r\n          </el-tooltip>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <pagination :hidden=\"page.total==0\" :total=\"page.total\" :page.sync=\"page.current\" :limit.sync=\"page.size\" @pagination=\"getDatas\" />\r\n\r\n    <el-dialog v-loading=\"loading.detail\" custom-class=\"dialog-detail\" :title=\"detail.dialog.title\" :visible.sync=\"detail.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\">\r\n      <el-form ref=\"formDetail\" label-position=\"right\" :label-width=\"detail.dialog.labelWidth\">\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"客户订单号\">\r\n              {{ detail.models.order.code }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单详单编号\">\r\n              {{ detail.models.code }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单详单名称\">\r\n              {{ detail.models.name }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"客户类别名称\">\r\n              {{ detail.models.goodsCategory.name }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订购数量\">\r\n              {{ detail.models.quality }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"实际出库数量\">\r\n              {{ detail.models.qualityReal }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订购金额\">\r\n              {{ detail.models.amount }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"交货时间\">\r\n              {{ detail.models.deliveryTime }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"实际交货时间\">\r\n              {{ detail.models.deliveryTimeReal }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单状态\">\r\n              {{ detail.models.status }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单状态改变时间\">\r\n              {{ detail.models.statusTime }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"状态更新人员\">\r\n              {{ detail.models.operator.userName }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n    </el-dialog>\r\n\r\n    <el-dialog custom-class=\"dialog-create\" :title=\"create.dialog.title\" :visible.sync=\"create.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\" :close-on-click-modal=\"false\">\r\n      <el-form ref=\"formCreate\" :rules=\"create.rules\" :model=\"create.models\" label-position=\"right\" :label-width=\"create.dialog.labelWidth\">\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"选择客户订单\" prop=\"orderDetail.orderId\">\r\n              <el-select v-model=\"create.models.orderDetail.orderId\" clearable>\r\n                <el-option v-for=\"item in orders\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单详单编号\" prop=\"orderDetail.code\">\r\n              <el-input v-model=\"create.models.orderDetail.code\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单详单名称\" prop=\"orderDetail.name\">\r\n              <el-input v-model=\"create.models.orderDetail.name\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"货物种类\" prop=\"orderDetail.goodsCategoryId\">\r\n              <el-select v-model=\"create.models.orderDetail.goodsCategoryId\" clearable>\r\n                <el-option v-for=\"item in goodsCategories\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\r\n              </el-select>\r\n              <el-tooltip class=\"item\" effect=\"dark\" content=\"添加货物种类\" placement=\"top-end\">\r\n                <el-button type=\"primary\" plain class=\"button-operate button-detail\" circle icon=\"el-icon-edit\" @click=\"handleCreateCategories()\" />\r\n              </el-tooltip>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订购数量\" prop=\"orderDetail.quality\">\r\n              <el-input v-model=\"create.models.orderDetail.quality\" type=\"number\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单交货时间\" prop=\"orderDetail.deliveryTime\">\r\n              <el-date-picker\r\n                v-model=\"create.models.orderDetail.deliveryTime \"\r\n                type=\"datetime\"\r\n                placeholder=\"选择交货时间\"\r\n                default-time=\"12:00:00\"\r\n              />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <!-- 书写扩展属性值 -->\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item v-for=\"(item,index) in orderProperties\" :key=\"item.id\" :label=\"item.name\">\r\n              <el-input v-model=\"OrderPropertyValue[index].value\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <div v-for=\"(orderPropertyValue, index) in create.models.orderPropertyValue\" :key=\"index\">\r\n          <el-row>\r\n            <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n              <el-form-item\r\n                v-if=\"orderPropertyValue.orderProperty\"\r\n                :key=\"orderPropertyValue.key\"\r\n                :label=\"'新增属性' + (index + 1)\"\r\n                :prop=\"'orderPropertyValue.' + index + '.orderProperty.name'\"\r\n                :rules=\"{\r\n                  required: true, message: '请输入扩展属性名称', trigger: 'blur',length: [0, 20]\r\n                }\"\r\n              >\r\n                <span>扩展属性名称</span><el-input v-model=\"orderPropertyValue.orderProperty.name\" />\r\n                <span>扩展属性描述(可空)</span><el-input v-model=\"orderPropertyValue.orderProperty.description\" />\r\n                <span>扩展属性值(可空)</span><el-input v-model=\"orderPropertyValue.value\" />\r\n                <el-button @click.prevent=\"removeDomain(orderPropertyValue)\">删除</el-button>\r\n              </el-form-item>\r\n            </el-col>\r\n          </el-row>\r\n        </div>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"create.dialog.visible = false\">取消</el-button>\r\n        <el-button @click=\"addDomain\">新增扩展属性</el-button>\r\n        <el-button type=\"primary\" @click=\"createData()\">提交</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n    <el-dialog v-loading=\"loading.update\" custom-class=\"dialog-update\" :title=\"update.dialog.title\" :visible.sync=\"update.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\" :close-on-click-modal=\"false\">\r\n      <el-form ref=\"formUpdate\" :rules=\"update.rules\" :model=\"update.models\" label-position=\"right\" :label-width=\"update.dialog.labelWidth\">\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"货物种类\" prop=\"goodsCategoryId\">\r\n              <el-select v-model=\"update.models.goodsCategoryId\" clearable>\r\n                <el-option v-for=\"item in goodsCategories\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\r\n              </el-select>\r\n              <el-tooltip class=\"item\" effect=\"dark\" content=\"添加货物种类\" placement=\"top-end\">\r\n                <el-button type=\"primary\" plain class=\"button-operate button-detail\" circle icon=\"el-icon-edit\" @click=\"handleCreateCategories()\" />\r\n              </el-tooltip>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订购数量\" prop=\"quality\">\r\n              <el-input v-model=\"update.models.quality\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"订单交货时间\" prop=\"deliveryTime\">\r\n              <el-date-picker\r\n                v-model=\"update.models.deliveryTime \"\r\n                type=\"datetime\"\r\n                placeholder=\"选择交货时间\"\r\n                default-time=\"12:00:00\"\r\n              />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"已有扩展属性\">\r\n              <el-tag v-for=\"item in orderProperties\" :key=\"item.id\">{{ item.name }}</el-tag>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <!--<el-row>-->\r\n        <!--<el-col :xl=\"6\" :md=\"12\" :sm=\"24\">-->\r\n        <!--<el-form-item-->\r\n        <!--v-for=\"(orderPropertyValue, index) in update.models.orderPropertyValue\"-->\r\n        <!--:key=\"orderPropertyValue.key\"-->\r\n        <!--:label=\"'新增属性' + (index + 1)\"-->\r\n        <!--:prop=\"'orderPropertyValue.' + index + '.orderProperty.name'\"-->\r\n        <!--:rules=\"{-->\r\n        <!--required: true, message: '请输入扩展属性名称', trigger: 'blur',length: [0, 20]-->\r\n        <!--}\"-->\r\n        <!--&gt;-->\r\n        <!--<span>扩展属性名称</span><el-input v-model=\"orderPropertyValue.orderProperty.name\" />-->\r\n        <!--<span>扩展属性描述(可空)</span><el-input v-model=\"orderPropertyValue.orderProperty.description\" />-->\r\n        <!--<span>扩展属性值(可空)</span><el-input v-model=\"orderPropertyValue.value\" />-->\r\n        <!--<el-button @click.prevent=\"removedomain(orderPropertyValue)\">删除</el-button>-->\r\n        <!--</el-form-item>-->\r\n        <!--</el-col>-->\r\n        <!--</el-row>-->\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"update.dialog.visible = false\">取消</el-button>\r\n        <!--<el-button @click=\"adddomain\">新增扩展属性</el-button>-->\r\n        <el-button type=\"primary\" @click=\"updateData()\">提交</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport adaptive from '@/directive/el-table'\r\nimport setRule from '@/utils/form-validate'\r\nimport Pagination from '@/components/Pagination'\r\nimport * as orderDetail from '@/api/order-manage/order-detail'\r\nimport * as orderProperty from '@/api/order-manage/order-property'\r\nimport * as goodsCategory from '@/api/goods-manage/goods-category'\r\nimport * as operator from '@/api/system-manage/user'\r\nimport * as order from '@/api/order-manage/order'\r\n\r\nexport default {\r\n  name: 'OrderDetail',\r\n  components: { Pagination },\r\n  directives: { adaptive },\r\n  data() {\r\n    return {\r\n      access: this.$store.getters.access['OrderBusiness']['OrderDetail'],\r\n      datas: null,\r\n      orderCode: null,\r\n      // orderId: null,\r\n      orders: null,\r\n      operators: null,\r\n      goodsCategories: null,\r\n      orderProperties: null,\r\n      OrderPropertyValue: [],\r\n      query: { code: null, name: null, orderCode: null, goodsCategoryId: null, status: null, operatorId: null },\r\n      page: { total: 0, current: 1, size: 20 },\r\n      sort: { prop: 'sort', order: 'ascending' },\r\n      detail: {\r\n        dialog: { title: '查看订单详单信息', visible: false, labelWidth: '150px' },\r\n        models: { code: null, name: null, order: { code: null }, goodsCategory: { name: null }, quality: null, qualityReal: null, deliveryTime: null, deliveryTimeReal: null, status: null, statusTime: null, operator: { userName: null }}\r\n      },\r\n      create: {\r\n        dialog: { title: '添加订单详单', visible: false, labelWidth: '150px' },\r\n        models: {\r\n          orderDetail: {\r\n            code: null,\r\n            name: null,\r\n            orderId: null,\r\n            goodsCategoryId: null,\r\n            quality: null,\r\n            deliveryTime: null\r\n          },\r\n          orderPropertyValue: []\r\n        },\r\n        rules: {\r\n          orderDetail: {\r\n            orderId: setRule('客户订单编号', [{ selected: true }]),\r\n            code: setRule('订单详单编号', [{ required: true }, { length: [0, 30] }]),\r\n            name: setRule('订单详单名称', [{ required: true }, { length: [0, 255] }]),\r\n            goodsCategoryId: setRule('货物种类', [{ selected: true }]),\r\n            quality: setRule('订购数量', [{ required: true }]),\r\n            deliveryTime: setRule('交货时间', [{ required: true }])\r\n          }\r\n        }\r\n      },\r\n      update: {\r\n        dialog: { title: '编辑订单详单信息', visible: false, labelWidth: '150px' },\r\n        models: {\r\n          order: { code: null },\r\n          goodsCategoryId: null,\r\n          quality: null,\r\n          qualityReal: null,\r\n          deliveryTime: null,\r\n          deliveryTimeReal: null,\r\n          status: null,\r\n          statusTime: null,\r\n          operatorId: null\r\n          // orderPropertyValue: [],\r\n        },\r\n        rules: {\r\n          orderId: setRule('客户订单编号', [{ selected: true }]),\r\n          goodsCategoryId: setRule('货物种类', [{ selected: true }]),\r\n          quality: setRule('订购数量', [{ required: true }]),\r\n          qualityReal: setRule('实际数量', [{ required: true }]),\r\n          amount: setRule('订购金额', [{ required: true }]),\r\n          deliveryTime: setRule('交货时间', [{ required: true }]),\r\n          status: setRule('订单状态', [{ required: true }]),\r\n          operatorId: setRule('状态更新人员', [{ selected: true }])\r\n        }\r\n      },\r\n      loading: { list: true, export: false, detail: false, update: false },\r\n      sortOrders: ['ascending', 'descending']\r\n    }\r\n  },\r\n  created() {\r\n    this.getDatas()\r\n    this.getOperators()\r\n    this.getGoodsCategories()\r\n    this.getOrderProperties()\r\n    this.getOrders()\r\n  },\r\n  methods: {\r\n    getDatas() {\r\n      this.loading.list = true\r\n      orderDetail.getList(this.query, this.page, this.sort).then(response => {\r\n        this.datas = response.data.items\r\n        this.page.total = response.data.total\r\n        this.loading.list = false\r\n      }).catch(reject => {\r\n        this.loading.list = false\r\n      })\r\n    },\r\n    addDomain() {\r\n      this.create.models.orderPropertyValue.push({\r\n        orderProperty: {\r\n          name: null,\r\n          description: null\r\n        },\r\n        value: null,\r\n        propertyId: null\r\n      })\r\n    },\r\n    removeDomain(item) {\r\n      var index = this.create.models.orderPropertyValue.indexOf(item)\r\n      if (index !== -1) {\r\n        this.create.models.orderPropertyValue.splice(index, 1)\r\n      }\r\n    },\r\n    adddomain() {\r\n      this.update.models.orderPropertyValue.push({\r\n        orderProperty: {\r\n          name: null,\r\n          description: null\r\n        },\r\n        value: null,\r\n        propertyId: null\r\n      })\r\n    },\r\n    removedomain(item) {\r\n      var index = this.update.models.orderPropertyValue.indexOf(item)\r\n      if (index !== -1) {\r\n        this.update.models.orderPropertyValue.splice(index, 1)\r\n      }\r\n    },\r\n    getOperators() {\r\n      operator.getlist().then(response => {\r\n        this.operators = response.data\r\n      }).catch(reject => {\r\n      })\r\n    },\r\n    getGoodsCategories() {\r\n      goodsCategory.getlist().then(response => {\r\n        this.goodsCategories = response.data\r\n      }).catch(reject => {\r\n      })\r\n    },\r\n    getOrderProperties() {\r\n      const orderType = 1\r\n      orderProperty.getlist(orderType).then(response => {\r\n        this.orderProperties = response.data\r\n        // 初始化OrderPropertyValue 数组，每条数据依次赋上propertyId的值\r\n        for (let i = 0; i < response.data.length; i++) {\r\n          this.OrderPropertyValue.push({\r\n            value: null,\r\n            propertyId: null\r\n          })\r\n          this.OrderPropertyValue[i].propertyId = response.data[i].id\r\n        }\r\n        this.loading.detail = false\r\n      }).catch(reject => {\r\n        this.loading.detail = false\r\n      })\r\n    },\r\n    getOrders() {\r\n      order.getlist().then(response => {\r\n        this.orders = response.data\r\n      }).catch(reject => {\r\n      })\r\n    },\r\n    getTime(datetimerange) {\r\n      this.query.loginTime = this.value1[0]\r\n      this.query.loginOutTime = this.value1[1]\r\n    },\r\n    // getPath() {\r\n    //   const orderId = this.$route.query.orderId\r\n    //   if (orderId !== undefined) {\r\n    //     this.query.orderId = orderId\r\n    //     this.orderId = orderId\r\n    //     this.getDatas()\r\n    //   }\r\n    // },\r\n    // handleCreateMaterial(row) {\r\n    //   const orderDetailId = row.id\r\n    //   this.$router.push({\r\n    //     path: '/order-manage/order-material',\r\n    //     query: { orderDetailId: orderDetailId }\r\n    //   })\r\n    // },\r\n    handleCreateCategories() {\r\n      this.$router.push({\r\n        path: '/goods-manage/goods-category',\r\n        query: { flag: true }\r\n      })\r\n    },\r\n    handleQuery() {\r\n      this.page.current = 1\r\n      this.getDatas()\r\n    },\r\n    handleSort(data) {\r\n      this.sort.prop = data.prop\r\n      this.sort.order = data.order\r\n      this.getDatas()\r\n    },\r\n    handleDetail(row) {\r\n      // 若列表数据展示了全部属性，则可直接拷贝列表数据\r\n      // this.detail.models = Object.assign({}, row)\r\n      this.detail.dialog.visible = true\r\n      this.loading.detail = true\r\n      orderDetail.get(row.id).then(response => {\r\n        this.detail.models = response.data\r\n        this.loading.detail = false\r\n      }).catch(reject => {\r\n        this.loading.detail = false\r\n      })\r\n      this.loading.detail = false\r\n    },\r\n    handleCreate() {\r\n      this.create.dialog.visible = true\r\n    },\r\n    createData() {\r\n      this.$refs['formCreate'].validate((valid) => {\r\n        if (!valid) return false\r\n        // this.create.models.orderDetail.orderId = this.orderId\r\n        this.create.models.orderPropertyValue = this.OrderPropertyValue.concat(this.create.models.orderPropertyValue)\r\n        orderDetail.create(this.create.models).then(response => {\r\n          // 为方便连续添加，create对话框不关闭\r\n          // 需要清空的表单，手动清空，至少清空一个必填项，防止点击两\r\n          this.create.models.orderDetail.goodsCategoryId = ''\r\n          // 重新获取数据\r\n          this.getDatas()\r\n        }).catch(reject => {\r\n        })\r\n      })\r\n    },\r\n    handleRankup(row) {\r\n      this.loading.list = true\r\n      orderDetail.rankup(row.id).then(response => {\r\n        this.getDatas()\r\n        this.loading.list = false\r\n      }).catch(reject => {\r\n        this.loading.list = false\r\n      })\r\n    },\r\n    handleUpdate(row) {\r\n      // 若列表数据展示了全部属性，则可直接拷贝列表数据用于编辑\r\n      // this.update.models = Object.assign({}, row)\r\n      this.update.dialog.visible = true\r\n      this.loading.update = true\r\n      orderDetail.get(row.id).then(response => {\r\n        this.update.models = response.data\r\n        this.loading.update = false\r\n      }).catch(reject => {\r\n        this.loading.update = false\r\n      })\r\n      this.$nextTick(() => {\r\n        this.$refs['formUpdate'].clearValidate()\r\n      })\r\n      this.loading.update = false\r\n    },\r\n    updateData() {\r\n      this.$refs['formUpdate'].validate((valid) => {\r\n        if (!valid) return false\r\n        const tempData = Object.assign({}, this.update.models)\r\n        orderDetail.update(tempData).then(response => {\r\n          // 重新获取数据\r\n          this.getDatas()\r\n          this.update.dialog.visible = false\r\n        }).catch(reject => {\r\n        })\r\n      })\r\n    },\r\n    handleDelete(row) {\r\n      this.deleteData([row.id])\r\n    },\r\n    handleDeletes(row) {\r\n      if (this.$refs.listTable.selection.length === 0) {\r\n        this.$message({ type: 'warning', message: '请在下面表格的左侧勾选需要删除的数据', duration: 4000 })\r\n        return false\r\n      }\r\n      const ids = []\r\n      for (const item of this.$refs.listTable.selection) {\r\n        ids.push(item.id)\r\n      }\r\n      this.deleteData(ids)\r\n    },\r\n    deleteData(ids) {\r\n      this.$confirm('数据删除后，不可恢复, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        orderDetail.del(ids).then(response => {\r\n          // 重新获取数据\r\n          this.getDatas()\r\n        }).catch(reject => {\r\n        })\r\n      })\r\n    },\r\n    handleExport() {\r\n      this.loading.export = true\r\n      orderDetail.exportExcel(this.query, this.page, this.sort).then(response => {\r\n        const blob = new Blob([response])\r\n        const fileName = '订单详单.xls'\r\n        const elink = document.createElement('a')\r\n        elink.onload = () => { URL.revokeObjectURL(elink.href) }\r\n        elink.download = fileName\r\n        elink.style.display = 'none'\r\n        elink.href = URL.createObjectURL(blob)\r\n        document.body.appendChild(elink)\r\n        elink.click()\r\n        document.body.removeChild(elink)\r\n        this.loading.export = false\r\n      }).catch(reject => {\r\n        this.loading.export = false\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n</style>\r\n"]}]}