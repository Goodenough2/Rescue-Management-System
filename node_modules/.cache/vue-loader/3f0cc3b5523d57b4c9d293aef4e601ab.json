el-tooltip v-if=\"access.delete.allow\" class=\"item\" effect=\"dark\" content=\"删除\" placement=\"top-end\">\r\n            <el-button type=\"danger\" plain class=\"button-operate button-delete\" size=\"mini\" @click=\"handleDelete(row)\"><i class=\"vue-icon-delete\" /></el-button>\r\n          </el-tooltip>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <pagination :hidden=\"page.total==0\" :total=\"page.total\" :page.sync=\"page.current\" :limit.sync=\"page.size\" @pagination=\"getDatas\" />\r\n\r\n    <el-dialog v-loading=\"loading.upload\" custom-class=\"dialog-fullscreen\" :title=\"upload.dialog.title\" :visible.sync=\"upload.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\">\r\n      <el-row>\r\n        <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n          <el-button class=\"tool tool-create\" type=\"primary\" icon=\"vue-icon-excel\" @click=\"ExportTemplate\">点击导出数据模板</el-button>\r\n        </el-col>\r\n      </el-row>\r\n      <br>\r\n      <el-row>\r\n        <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n          <el-upload\r\n            ref=\"upload\"\r\n            class=\"upload-demo\"\r\n            action=\"\"\r\n            :http-request=\"uploadUrl\"\r\n            :on-remove=\"handleRemove\"\r\n            :file-list=\"fileList\"\r\n            accept=\".xlsx, .xls\"\r\n            :auto-upload=\"false\"\r\n          >\r\n            <el-button slot=\"trigger\" size=\"small\" type=\"primary\">选取文件</el-button>\r\n            <el-button style=\"margin-left: 10px;\" size=\"small\" type=\"success\" @click=\"submitUpload\">上传到服务器</el-button>\r\n            <div slot=\"tip\" class=\"el-upload__tip\">仅支持上传Excel文件</div>\r\n          </el-upload>\r\n        </el-col>\r\n      </el-row>\r\n    </el-dialog>\r\n\r\n    <el-dialog v-loading=\"loading.detail\" custom-class=\"dialog-fullscreen\" :title=\"detail.dialog.title\" :visible.sync=\"detail.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\">\r\n      <el-form ref=\"formDetail\" label-position=\"right\" :label-width=\"detail.dialog.labelWidth\">\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"单元格编号\">\r\n              {{ detail.models.code }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"单元格名称\">\r\n              {{ detail.models.name }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"单元格类型\">\r\n              {{ detail.models.cellType.name }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"所属货柜名称\">\r\n              {{ detail.models.container.name }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"单元格RFID\">\r\n              {{ detail.models.rfidCode }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"单元格条形码\">\r\n              {{ detail.models.barCode }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"货物类别\">\r\n              {{ detail.models.goodsCategory.name }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"是否允许混合使用\">\r\n              {{ detail.models.canMix === 1 ? '允许' : '不允许' }}\r\n   