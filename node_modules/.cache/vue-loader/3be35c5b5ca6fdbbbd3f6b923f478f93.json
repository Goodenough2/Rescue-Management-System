em\" style=\"width: 120px\" placeholder=\"单元格状态\" clearable @clear=\"handleQuery\">\n        <el-option label=\"未启用\" value=\"0\" />\n        <el-option label=\"启用\" value=\"1\" />\n      </el-select>\n      <el-button class=\"tool tool-query\" type=\"primary\" icon=\"el-icon-search\" @click=\"handleQuery\">查询</el-button>\n      <el-button v-if=\"access.create.allow\" class=\"tool tool-create\" type=\"primary\" icon=\"vue-icon-create\" @click=\"handleCreate\">添加</el-button>\n      <el-button v-if=\"access.delete.allow\" class=\"tool tool-delete\" type=\"danger\" icon=\"vue-icon-delete\" @click=\"handleDeletes\">批量删除</el-button>\n      <el-button v-if=\"access.import.allow\" class=\"tool tool-import\" :loading=\"loading.upload\" type=\"primary\" icon=\"vue-icon-import\" @click=\"handleUpload\">导入</el-button>\n      <el-button v-if=\"access.export.allow\" class=\"tool tool-export\" :loading=\"loading.export\" type=\"primary\" icon=\"vue-icon-excel\" @click=\"handleExport\">导出</el-button>\n    </div>\n\n    <el-table ref=\"listTable\" v-loading=\"loading.list\" v-adaptive=\"{ bottomOffset: 55 }\" height=\"200px\" :data=\"datas\" :default-sort=\"sort\" border fit highlight-current-row @sort-change=\"handleSort\">\n      <el-table-column type=\"selection\" align=\"center\" width=\"35\" />\n      <el-table-column label=\"序号\" type=\"index\" align=\"center\" width=\"65\" fixed>\n        <template slot-scope=\"scope\">\n          <span>{{ (page.current - 1) * page.size + scope.$index + 1 }}</span>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"单元格编号\" prop=\"code\" align=\"center\" width=\"150\" show-overflow-tooltip />\n      <el-table-column label=\"单元格名称\" prop=\"name\" align=\"center\" width=\"150\" show-overflow-tooltip />\n      <el-table-column label=\"单元格类型\" prop=\"cellType.name\" align=\"center\" width=\"150\" show-overflow-tooltip />\n      <el-table-column label=\"所属货柜\" prop=\"container.name\" align=\"center\" show-overflow-tooltip />\n      <el-table-column label=\"单元格RFID\" prop=\"rfidCode\" align=\"center\" show-overflow-tooltip />\n      <el-table-column label=\"单元格条形码\" prop=\"barCode\" align=\"center\" show-overflow-tooltip />\n      <el-table-column label=\"是否允许混合使用\" prop=\"canMix\" align=\"center\" show-overflow-tooltip>\n        <template slot-scope=\"{row}\">\n          <span>{{ row.canMix===1 ? '允许' : '不允许' }}</span>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"启用\" align=\"center\" width=\"80\">\n        <template slot-scope=\"{row}\">\n          <span>{{ row.enabled===1 ? '启用' : '未启用' }}</span>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"货物类别\" prop=\"goodsCategory.name\" align=\"center\" show-overflow-tooltip />\n      <el-table-column fixed=\"right\" label=\"操作\" align=\"center\" width=\"180\">\n        <template slot-scope=\"{row}\">\n          <el-tooltip v-if=\"access.update.allow\" class=\"item\" effect=\"dark\" content=\"排序上移\" placement=\"top-end\">\n            <el-button type=\"primary\" plain class=\"button-operate button-rankup\" size=\"mini\" @click=\"handleRankup(row)\"><i class=\"vue-icon-rankup\" /></el-button>\n          </el-tooltip>\n          <el-tooltip v-if=\"access.detail.allow\" class=\"item\" effect=\"dark\" content=\"详情\" placement=\"top-end\">\n            <el-button type=\"primary\" plain class=\"button-operate button-detail\" size=\"mini\" @click=\"handleDetail(row)\"><i class=\"vue-icon-detail\" /></el-button>\n          </el-tooltip>\n          <el-tooltip v-if=\"access.update.allow\" class=\"item\" effect=\"dark\" content=\"编辑\" placement=\"top-end\">\n            <el-button type=\"primary\" plain class=\"button-operate button-update\" size=\"mini\" @click=\"handleUpdate(row)\"><i class=\"vue-icon-update\" /></el-button>\n          </el-tooltip>\n          <el-tooltip v-if=\"access.delete.allow\" class=\"item\" effect=\"dark\" content=\"删除\" placement=\"top-end\">\n            <el-button type=\"danger\" plain class=\"button-operate button-delete\" size=\"mini\" @click=\"handleDelete(row)\"><i class=\"vue-icon-delete\" /></el-button>\n          </el-tooltip>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <pagination :hidden=\"page.total==0\" :total=\"page.total\" :page.sync=\"page.current\" :limit.sync=\"page.size\" @pagination=\"getDatas\" />\n\n    <el-dialog v-loading=\"loading.upload\" custom-class=\"dialog-upload\" :title=\"upload.dialog.title\" :visible.sync=\"upload.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\">\n      <el-row>\n        <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n          <el-button class=\"tool tool-create\" type=\"primary\" icon=\"vue-icon-excel\" @click=\"ExportTemplate\">点击导出数据模板</el-button>\n        </el-col>\n      </el-row>\n      <br>\n      <el-row>\n        <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n          <el-upload\n            ref=\"upload\"\n            class=\"upload-demo\"\n            action=\"\"\n            :http-request=\"uploadUrl\"\n            :on-remove=\"handleRemove\"\n            :file-list=\"fileList\"\n            accept=\".xlsx, .xls\"\n            :auto-upload=\"false\"\n          >\n            <el-button slot=\"trigger\" size=\"small\" type=\"primary\">选取文件</el-button>\n            <el-button style=\"margin-left: 10px;\" size=\"small\" type=\"success\" @click=\"submitUpload\">上传到服务器</el-button>\n            <div slot=\"tip\" class=\"el-upload__tip\">仅支持上传Excel文件</div>\n          </el-upload>\n        </el-col>\n      </el-row>\n    </el-dialog>\n\n    <el-dialog v-loading=\"loading.detail\" custom-class=\"dialog-detail\" :title=\"detail.dialog.title\" :visible.sync=\"detail.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\">\n      <el-form ref=\"formDetail\" label-position=\"right\" :label-width=\"detail.dialog.labelWidth\">\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"单元格编号\">\n              {{ detail.models.code }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"单元格名称\">\n              {{ detail.models.name }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"单元格类型\">\n              {{ detail.models.cellType.name }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"所属货柜名称\">\n              {{ detail.models.container.name }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"单元格RFID\">\n              {{ detail.models.rfidCode }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"单元格条形码\">\n              {{ detail.models.barCode }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物类别\">\n              {{ detail.models.goodsCategory.name }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"是否允许混合使用\">\n              {{ detail.models.canMix === 1 ? '允许' : '不允许' }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"是否启用\">\n              {{ detail.models.enabled === 1 ? '启用' : '未启用' }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n      </el-form>\n    </el-dialog>\n\n    <el-dialog custom-class=\"dialog-create\" :title=\"create.dialog.title\" :visible.sync=\"create.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\" :close-on-click-modal=\"false\">\n      <el-form ref=\"formCreate\" :rules=\"create.rules\" :model=\"create.models\" label-position=\"right\" :label-width=\"create.dialog.labelWidth\">\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"单元格编号\" prop=\"code\">\n              <el-input v-model=\"create.models.code\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"单元格名称\" prop=\"name\">\n              <el-input v-model=\"create.models.name\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"单元格类型\" prop=\"typeId\">\n              <el-select v-model=\"create.models.typeId\" clearable>\n                <el-option v-for=\"item in cellTypes\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n              </el-select>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"所属货柜名称\" prop=\"containerId\">\n              <el-select v-model=\"create.models.containerId\" clearable>\n                <el-option v-for=\"item in containers\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n              </el-select>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"单元格RFID\" prop=\"rfidCode\">\n              <el-input v-model=\"create.models.rfidCode\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"单元格条形码\" prop=\"barCode\">\n              <el-input v-model=\"create.models.barCode\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物类别\" prop=\"goodsCategoryId\">\n              <el-select v-model=\"create.models.goodsCategoryId\" filterable placeholder=\"请选择\">\n                <el-option v-for=\"item in goodsCategories\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n              </el-select>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"是否允许混合使用\" prop=\"canMix\">\n              <el-switch v-model=\"create.models.canMix\" :active-value=\"1\" :inactive-value=\"0\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"是否启用\" prop=\"enabled\">\n              <el-switch v-model=\"create.models.enabled\" :active-value=\"1\" :inactive-value=\"0\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"create.dialog.visible = false\">取消</el-button>\n        <el-button type=\"primary\" @click=\"createData()\">提交</el-button>\n      </div>\n    </el-dialog>\n\n    <el-dialog v-loading=\"loading.update\" custom-class=\"dialog-update\" :title=\"update.dialog.title\" :visible.sync=\"update.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\" :close-on-click-modal=\"false\">\n      <el-form ref=\"formUpdate\" :rules=\"update.rules\" :model=\"update.models\" label-position=\"right\" :label-width=\"update.dialog.labelWidth\">\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"单元格编号\" prop=\"code\">\n              <el-input v-model=\"update.models.code\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"单元格名称\" prop=\"name\">\n              <el-input v-model=\"update.models.name\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"单元格类型\" prop=\"typeId\">\n              <el-select v-model=\"update.models.typeId\" clearable>\n                <el-option v-for=\"item in cellTypes\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n              </el-select>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"所属货柜名称\" prop=\"containerId\">\n              <el-select v-model=\"update.models.containerId\" clearable>\n                <el-option v-for=\"item in containers\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n              </el-select>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"单元格RFID\" prop=\"rfidCode\">\n              <el-input v-model=\"update.models.rfidCode\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"单元格条形码\" prop=\"barCode\">\n              <el-input v-model=\"update.models.barCode\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物类别\" prop=\"goodsCategoryId\">\n              <el-select v-model=\"update.models.goodsCategoryId\" filterable placeholder=\"请选择\">\n                <el-option v-for=\"item in goodsCategories\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n              </el-select>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"是否允许混合使用\" prop=\"canMix\">\n              <el-switch v-model=\"update.models.canMix\" :active-value=\"1\" :inactive-value=\"0\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"是否启用\" prop=\"enabled\">\n              <el-switch v-model=\"update.models.enabled\" :active-value=\"1\" :inactive-value=\"0\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"update.dialog.visible = false\">取消</el-button>\n        <el-button type=\"primary\" @click=\"updateData()\">提交</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport adaptive from '@/directive/el-table'\nimport setRule from '@/utils/form-validate'\nimport Pagination from '@/components/Pagination'\nimport * as container from '@/api/store-manage/container'\nimport * as cell from '@/api/store-manage/cell'\nimport * as cellType from '@/api/store-manage/cell-type'\nimport * as goodsCategory from '@/api/goods-manage/goods-category'\n\nexport default {\n  name: 'Cell',\n  components: { Pagination },\n  directives: { adaptive },\n  data() {\n    return {\n      access: this.$store.getters.access['StoreManage']['Cell'],\n      datas: null,\n      containers: null,\n      cellTypes: null,\n      goodsCategories: null,\n      fileList: [],\n      query: { code: null, name: null, typeId: null, containerId: null, canMix: null, enabled: null },\n      page: { total: 0, current: 1, size: 20 },\n      sort: { prop: 'sort', order: 'ascending' },\n      detail: {\n        dialog: { title: '货柜单元格信息', visible: false, labelWidth: '150px' },\n        models: { code: null, name: null, cellType: { name: null }, container: { name: null }, rfidCode: null, barCode: null, enabled: null, canMix: null, goodsCategory: { name: null }}\n      },\n      create: {\n        dialog: { title: '添加货柜单元格基础信息', visible: false, labelWidth: '150px' },\n        models: { name: null, typeId: null, containerId: null, rfidCode: null, barCode: null, goodsCategoryId: null, enabled: 1, canMix: null },\n        rules: {\n          code: setRule('单元格编号', [{ required: true }, { length: [0, 80] }]),\n          name: setRule('单元格名称', [{ required: true }, { length: [0, 50] }]),\n          typeId: setRule('单元格类型', [{ selected: true }]),\n          containerId: setRule('所属货柜名称', [{ selected: true }]),\n          rfidCode: setRule('单元格RFID', [{ length: [0, 255] }]),\n          barCode: setRule('单元格条形码', [{ length: [0, 255] }]),\n          goodsCategoryId: setRule('货物类别', [{ selected: true }])\n        }\n      },\n      update: {\n        dialog: { title: '编辑货柜单元格基础信息', visible: false, labelWidth: '150px' },\n        models: { name: null, typeId: null, containerId: null, rfidCode: null, barCode: null, goodsCategoryId: null, enabled: null, canMix: null },\n        rules: {\n          code: setRule('单元格编号', [{ required: true }, { length: [0, 80] }]),\n          name: setRule('单元格名称', [{ required: true }, { length: [0, 50] }]),\n          typeId: setRule('单元格类型', [{ selected: true }]),\n          containerId: setRule('所属货柜名称', [{ selected: true }]),\n          rfidCode: setRule('单元格RFID', [{ length: [0, 255] }]),\n          barCode: setRule('单元格条形码', [{ length: [0, 255] }]),\n          goodsCategoryId: setRule('货物类别', [{ selected: true }])\n        }\n      },\n      upload: {\n        dialog: { title: '导入文件', visible: false, labelWidth: '150px' }\n      },\n      loading: { list: true, export: false, detail: false, update: false, upload: false },\n      sortOrders: ['ascending', 'descending']\n    }\n  },\n  created() {\n    this.getDatas()\n    this.getContainers()\n    this.getCellTypes()\n    this.getGoodsCategories()\n  },\n  methods: {\n    getDatas() {\n      this.loading.list = true\n      cell.getList(this.query, this.page, this.sort).then(response => {\n        this.datas = response.data.items\n        this.page.total = response.data.total\n        this.loading.list = false\n      }).catch(reject => {\n        this.loading.list = false\n      })\n    },\n    getContainers() {\n      container.getlist().then(response => {\n        this.containers = response.data\n      }).catch(reject => {\n      })\n    },\n    getCellTypes() {\n      cellType.getlist().then(response => {\n        this.cellTypes = response.data\n      }).catch(reject => {\n      })\n    },\n    getGoodsCategories() {\n      goodsCategory.getlist().then(response => {\n        this.goodsCategories = response.data\n      }).catch(reject => {\n      })\n    },\n    handleQuery() {\n      this.page.current = 1\n      this.getDatas()\n    },\n    handleSort(data) {\n      this.sort.prop = data.prop\n      this.sort.order = data.order\n      this.getDatas()\n    },\n    handleDetail(row) {\n      // 若列表数据展示了全部属性，则可直接拷贝列表数据\n      // this.detail.models = Object.assign({}, row)\n      this.detail.dialog.visible = true\n      this.loading.detail = true\n      cell.get(row.id).then(response => {\n        this.detail.models = response.data\n        this.loading.detail = false\n      }).catch(reject => {\n        this.loading.detail = false\n      })\n    },\n    handleCreate() {\n      this.create.dialog.visible = true\n    },\n    createData() {\n      this.$refs['formCreate'].validate((valid) => {\n        if (!valid) return false\n        cell.create(this.create.models).then(response => {\n          // 为方便连续添加，create对话框不关闭\n          // 需要清空的表单，手动清空，至少清空一个必填项，防止点击两遍\n          this.create.models.name = ''\n          this.create.models.code = ''\n          this.create.models.rfidCode = ''\n          this.create.models.barCode = ''\n          // 重新获取数据\n          this.getDatas()\n        }).catch(reject => {\n        })\n      })\n    },\n    handleRankup(row) {\n      this.loading.list = true\n      cell.rankup(row.id).then(response => {\n        this.getDatas()\n        this.loading.list = false\n      }).catch(reject => {\n        this.loading.list = false\n      })\n    },\n    handleUpdate(row) {\n      // 若列表数据展示了全部属性，则可直接拷贝列表数据用于编辑\n      // this.update.models = Object.assign({}, row)\n      this.update.dialog.visible = true\n      this.loading.update = true\n      cell.get(row.id).then(response => {\n        console.log(response.data)\n        this.update.models = response.data\n        this.loading.update = false\n      }).catch(reject => {\n        this.loading.update = false\n      })\n      this.$nextTick(() => {\n        this.$refs['formUpdate'].clearValidate()\n      })\n    },\n    updateData() {\n      this.$refs['formUpdate'].validate((valid) => {\n        if (!valid) return false\n        const tempData = Object.assign({}, this.update.models)\n        cell.update(tempData).then(response => {\n          // 重新获取数据\n          this.getDatas()\n          this.update.dialog.visible = false\n        }).catch(reject => {\n        })\n      })\n    },\n    handleDelete(row) {\n      this.deleteData([row.id])\n    },\n    handleDeletes(row) {\n      if (this.$refs.listTable.selection.length === 0) {\n        this.$message({ type: 'warning', message: '请在下面表格的左侧勾选需要删除的数据', duration: 4000 })\n        return false\n      }\n      const ids = []\n      for (const item of this.$refs.listTable.selection) {\n        ids.push(item.id)\n      }\n      this.deleteData(ids)\n    },\n    deleteData(ids) {\n      this.$confirm('数据删除后，不可恢复, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        cell.del(ids).then(response => {\n          // 重新获取数据\n          this.getDatas()\n        }).catch(reject => {\n        })\n      })\n    },\n    handleUpload() {\n      this.upload.dialog.visible = true\n    },\n    submitUpload() {\n      this.$refs.upload.submit()\n    },\n    handleRemove() {\n      this.$refs.upload.clearFiles()\n    },\n    uploadUrl(param) {\n      // 先判断图片大小\n      const _file = param.file\n      // const _fileSize = _file.size < 1024 * 500\n      // if (!_fileSize) {\n      //   that.$message.warning('请上传500KB以下的文件')\n      //   return false\n      // }\n      // 参数\n      const params = new FormData()\n      params.append('excel', _file)\n      cell.uploadExcel(params).then(response => {\n        const failureDetails = response.data.failureDetails\n        let message = ''\n        failureDetails.forEach((item) => {\n          message = message + '/' + item\n        })\n        this.$alert('成功：' + response.data.success + ' ' +\n          '失败：' + response.data.failure + ' ' +\n          '读取总数：' + response.data.total + ' ' +\n          '/' + message,\n        '消息提示',\n        {\n          confirmButtonText: '确定',\n          callback: action => {\n            this.upload.dialog.visible = false\n            this.handleRemove()\n            this.getDatas()\n          }\n        })\n      }).catch(reject => {\n      })\n    },\n    ExportTemplate() {\n      this.loading.export = true\n      import('@/vendor/Export2Excel').then(excel => {\n        const tHeader = ['编号', '名称', '类型名称', '货柜编号', '条码', 'RFID', '是否启用', '允许混合存放']\n        const data = ''\n        excel.export_json_to_excel({\n          header: tHeader,\n          data,\n          filename: '单元格'\n        })\n        this.loading.export = false\n      })\n    },\n    handleExport() {\n      this.loading.export = true\n      cell.exportExcel(this.query, this.page, this.sort).then(response => {\n        const blob = new Blob([response])\n        const fileName = '单元格.xls'\n        const elink = document.createElement('a')\n        elink.onload = () => { URL.revokeObjectURL(elink.href) }\n        elink.download = fileName\n        elink.style.display = 'none'\n        elink.href = URL.createObjectURL(blob)\n        document.body.appendChild(elink)\n        elink.click()\n        document.body.removeChild(elink)\n        this.loading.export = false\n      }).catch(reject => {\n        this.loading.export = false\n      })\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n</style>\n"]}]}