-button v-if=\"access.delete.allow\" class=\"tool tool-delete\" type=\"danger\" icon=\"vue-icon-delete\" @click=\"handleDeletes\">批量删除</el-button>\n      <el-button v-if=\"access.export.allow\" class=\"tool tool-export\" :loading=\"loading.export\" type=\"primary\" icon=\"vue-icon-excel\" @click=\"handleExport\">导出</el-button>\n    </div>\n\n    <el-table ref=\"listTable\" v-loading=\"loading.list\" v-adaptive=\"{ bottomOffset: 55 }\" height=\"200px\" :data=\"datas\" :default-sort=\"sort\" border fit highlight-current-row @sort-change=\"handleSort\">\n      <el-table-column type=\"selection\" align=\"center\" width=\"35\" />\n      <el-table-column label=\"序号\" type=\"index\" align=\"center\" width=\"65\" fixed>\n        <template slot-scope=\"scope\">\n          <span>{{ (page.current - 1) * page.size + scope.$index + 1 }}</span>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"所属仓库名称\" prop=\"warehouse.name\" :sort-orders=\"sortOrders\" align=\"center\" show-overflow-tooltip />\n      <el-table-column label=\"编号\" prop=\"code\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\n      <el-table-column label=\"区域名称\" prop=\"name\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\n      <el-table-column label=\"区域类型\" prop=\"areaType.name\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\n      <el-table-column label=\"最大承重值\" prop=\"supportingVal\" :sort-orders=\"sortOrders\" align=\"center\" width=\"120\" show-overflow-tooltip />\n      <el-table-column label=\"区域容积\" prop=\"volume\" :sort-orders=\"sortOrders\" align=\"center\" width=\"120\" show-overflow-tooltip />\n      <el-table-column label=\"区域地址\" prop=\"address\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\n      <el-table-column label=\"承重系数\" prop=\"safetyFact\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\n      <el-table-column label=\"启用\" :sort-orders=\"sortOrders\" align=\"center\" width=\"80\">\n        <template slot-scope=\"{row}\">\n          <span>{{ row.enabled===1 ? '启用' : '未启用' }}</span>\n        </template>\n      </el-table-column>\n      <el-table-column fixed=\"right\" label=\"操作\" align=\"center\" width=\"180\">\n        <template slot-scope=\"{row}\">\n          <el-tooltip v-if=\"access.update.allow\" class=\"item\" effect=\"dark\" content=\"排序上移\" placement=\"top-end\">\n            <el-button type=\"primary\" plain class=\"button-operate button-rankup\" size=\"mini\" @click=\"handleRankup(row)\"><i class=\"vue-icon-rankup\" /></el-button>\n          </el-tooltip>\n          <el-tooltip v-if=\"access.detail.allow\" class=\"item\" effect=\"dark\" content=\"详情\" placement=\"top-end\">\n            <el-button type=\"primary\" plain class=\"button-operate button-detail\" size=\"mini\" @click=\"handleDetail(row)\"><i class=\"vue-icon-detail\" /></el-button>\n          </el-tooltip>\n          <el-tooltip v-if=\"access.update.allow\" class=\"item\" effect=\"dark\" content=\"编辑\" placement=\"top-end\">\n            <el-button type=\"primary\" plain class=\"button-operate button-update\" size=\"mini\" @click=\"handleUpdate(row)\"><i class=\"vue-icon-update\" /></el-button>\n          </el-tooltip>\n          <el-tooltip v-if=\"access.delete.allow\" class=\"item\" effect=\"dark\" content=\"删除\" placement=\"top-end\">\n            <el-button type=\"danger\" plain class=\"button-operate button-delete\" size=\"mini\" @click=\"handleDelete(row)\"><i class=\"vue-icon-delete\" /></el-button>\n          </el-tooltip>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <pagination :hidden=\"page.total==0\" :total=\"page.total\" :page.sync=\"page.current\" :limit.sync=\"page.size\" @pagination=\"getDatas\" />\n\n    <el-dialog v-loading=\"loading.upload\" custom-class=\"dialog-fullscreen dialog-update\" :title=\"update.dialog.title\" :visible.sync=\"upload.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\">\n      <el-row>\n        <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n          <el-button class=\"tool tool-create\" type=\"primary\" icon=\"vue-icon-excel\" @click=\"ExportTemplate\">点击导出数据模板</el-button>\n        </el-col>\n      </el-row>\n      <br>\n      <el-row>\n        <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n          <el-upload\n            ref=\"upload\"\n            class=\"upload-demo\"\n            action=\"\"\n            :http-request=\"uploadUrl\"\n            :on-remove=\"handleRemove\"\n            :file-list=\"fileList\"\n            accept=\".xlsx, .xls\"\n            :auto-upload=\"false\"\n          >\n            <el-button slot=\"trigger\" size=\"small\" type=\"primary\">选取文件</el-button>\n            <el-button style=\"margin-left: 10px;\" size=\"small\" type=\"success\" @click=\"submitUpload\">上传到服务器</el-button>\n            <div slot=\"tip\" class=\"el-upload__tip\">仅支持上传Excel文件</div>\n          </el-upload>\n        </el-col>\n      </el-row>\n    </el-dialog>\n\n    <el-dialog v-loading=\"loading.detail\" custom-class=\"dialog-fullscreen dialog-detail\" :title=\"detail.dialog.title\" :visible.sync=\"detail.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\">\n      <el-form ref=\"formDetail\" label-position=\"right\" :label-width=\"detail.dialog.labelWidth\">\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"区域编号\">\n              {{ detail.models.code }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"区域名称\">\n              {{ detail.models.name }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"区域类型\">\n              {{ detail.models.areaType.name }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"最大承重值\">\n              {{ detail.models.supportingVal }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"有效内长\">\n              {{ detail.models.length }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"有效内宽\">\n              {{ detail.models.width }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"有效内高\">\n              {{ detail.models.height }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"容积\">\n              {{ detail.models.volume }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"地址\">\n              {{ detail.models.address }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"承重系数\">\n              {{ detail.models.safetyFact }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"所属仓库名称\">\n              {{ detail.models.warehouse.name }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"是否启用\">\n              {{ detail.models.enabled === 1 ? '启用' : '未启用' }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n      </el-form>\n    </el-dialog>\n\n    <el-dialog custom-class=\"dialog-fullscreen dialog-create\" :title=\"create.dialog.title\" :visible.sync=\"create.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\" :close-on-click-modal=\"false\">\n      <el-form ref=\"formCreate\" :rules=\"create.rules\" :model=\"create.models\" label-position=\"right\" :label-width=\"create.dialog.labelWidth\">\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"所属仓库\" prop=\"houseId\">\n              <el-select v-model=\"create.models.houseId\" clearable>\n                <el-option v-for=\"item in warehouses\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n              </el-select>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"区域编号\" prop=\"code\">\n              <el-input v-model=\"create.models.code\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"区域名称\" prop=\"name\">\n              <el-input v-model=\"create.models.name\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"区域类型\" prop=\"typeId\">\n              <el-select v-model=\"create.models.typeId\" clearable>\n                <el-option v-for=\"item in areaTypes\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n              </el-select>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"最大承重值\" prop=\"supportingVal\">\n              <el-input v-model=\"create.models.supportingVal\" type=\"number\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"有效内长\" prop=\"length\">\n              <el-input v-model=\"create.models.length\" type=\"number\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"有效内宽\" prop=\"width\">\n              <el-input v-model=\"create.models.width\" type=\"number\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"有效内高\" prop=\"height\">\n              <el-input v-model=\"create.models.height\" type=\"number\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"区域位置\" prop=\"address\">\n              <el-input v-model=\"create.models.address\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"承重系数\" prop=\"safetyFact\">\n              <el-input v-model=\"create.models.safetyFact\" type=\"number\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"是否启用\" prop=\"enabled\">\n              <el-switch v-model=\"create.models.enabled\" :active-value=\"1\" :inactive-value=\"0\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"create.dialog.visible = false\">取消</el-button>\n        <el-button type=\"primary\" @click=\"createData()\">提交</el-button>\n      </div>\n    </el-dialog>\n\n    <el-dialog v-loading=\"loading.update\" custom-class=\"dialog-fullscreen dialog-update\" :title=\"update.dialog.title\" :visible.sync=\"update.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\" :close-on-click-modal=\"false\">\n      <el-form ref=\"formUpdate\" :rules=\"update.rules\" :model=\"update.models\" label-position=\"right\" :label-width=\"update.dialog.labelWidth\">\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"所属仓库\" prop=\"houseId\">\n              <el-select v-model=\"update.models.houseId\" clearable>\n                <el-option v-for=\"item in warehouses\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n              </el-select>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"区域编号\" prop=\"code\">\n              <el-input v-model=\"update.models.code\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"区域名称\" prop=\"name\">\n              <el-input v-model=\"update.models.name\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"区域类型\" prop=\"typeId\">\n              <el-select v-model=\"update.models.typeId\" clearable>\n                <el-option v-for=\"item in areaTypes\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n              </el-select>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"最大承重值\" prop=\"supportingVal\">\n              <el-input v-model=\"update.models.supportingVal\" type=\"number\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"有效内长\" prop=\"length\">\n              <el-input v-model=\"update.models.length\" type=\"number\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"有效内宽\" prop=\"width\">\n              <el-input v-model=\"update.models.width\" type=\"number\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"有效内高\" prop=\"height\">\n              <el-input v-model=\"update.models.height\" type=\"number\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"区域位置\" prop=\"address\">\n              <el-input v-model=\"update.models.address\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"承重系数\" prop=\"safetyFact\">\n              <el-input v-model=\"update.models.safetyFact\" type=\"number\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"是否启用\" prop=\"enabled\">\n              <el-switch v-model=\"update.models.enabled\" :active-value=\"1\" :inactive-value=\"0\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"update.dialog.visible = false\">取消</el-button>\n        <el-button type=\"primary\" @click=\"updateData()\">提交</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport adaptive from '@/directive/el-table'\nimport setRule from '@/utils/form-validate'\nimport Pagination from '@/components/Pagination'\nimport * as warehouse from '@/api/store-manage/store'\nimport * as area from '@/api/store-manage/area'\nimport * as areaType from '@/api/store-manage/area-type'\n\nexport default {\n  name: 'Area',\n  components: { Pagination },\n  directives: { adaptive },\n  data() {\n    return {\n      access: this.$store.getters.access['StoreManage']['Area'],\n      datas: null,\n      warehouses: null,\n      areaTypes: null,\n      fileList: null,\n      count: null,\n      query: { code: null, name: null, typeId: null, houseId: null, enable: null },\n      page: { total: 0, current: 1, size: 20 },\n      sort: { prop: 'sort', order: 'ascending' },\n      detail: {\n        dialog: { title: '区域信息', visible: false, labelWidth: '120px' },\n        models: { code: null, name: null, areaType: { name: null }, supportingVal: null, length: null, width: null, height: null, volume: null, address: null, safetyFact: null, warehouse: { name: null }, enabled: null }\n      },\n      create: {\n        dialog: { title: '添加区域', visible: false, labelWidth: '120px' },\n        models: { code: null, name: null, typeId: null, supportingVal: null, length: null, width: null, height: null, address: null, safetyFact: 0.9, houseId: null, enabled: 1 },\n        rules: {\n          code: setRule('区域编号', [{ required: true }, { length: [0, 40] }]),\n          name: setRule('区域名称', [{ required: true }, { length: [0, 50] }]),\n          typeId: setRule('区域类型', [{ selected: true }]),\n          houseId: setRule('所属仓库', [{ selected: true }]),\n          supportingVal: setRule('最大承重值', [{ required: true }]),\n          length: setRule('有效内长', [{ required: true }]),\n          width: setRule('有效内宽', [{ required: true }]),\n          height: setRule('有效内高', [{ required: true }]),\n          address: setRule('地址', [{ length: [0, 255] }]),\n          safetyFact: setRule('承重系数', [{ required: true }])\n        }\n      },\n      update: {\n        dialog: { title: '编辑区域信息', visible: false, labelWidth: '120px' },\n        models: { code: null, name: null, typeId: null, supportingVal: null, length: null, width: null, height: null, address: null, safetyFact: null, houseId: null, enabled: null },\n        rules: {\n          code: setRule('区域编号', [{ required: true }, { length: [0, 40] }]),\n          name: setRule('区域名称', [{ required: true }, { length: [0, 50] }]),\n          typeId: setRule('区域类型', [{ selected: true }]),\n          houseId: setRule('所属仓库', [{ selected: true }]),\n          supportingVal: setRule('最大承重值', [{ required: true }]),\n          length: setRule('有效内长', [{ required: true }]),\n          width: setRule('有效内宽', [{ required: true }]),\n          height: setRule('有效内高', [{ required: true }]),\n          address: setRule('地址', [{ length: [0, 255] }]),\n          safetyFact: setRule('承重系数', [{ required: true }])\n        }\n      },\n      upload: {\n        dialog: { title: '导入文件', visible: false, labelWidth: '150px' }\n      },\n      loading: { list: true, export: false, detail: false, update: false, upload: false },\n      sortOrders: ['ascending', 'descending']\n    }\n  },\n  created() {\n    this.getDatas()\n    this.getWarehouses()\n    this.getAreaTypes()\n  },\n  methods: {\n    getDatas() {\n      if (this.count === null) {\n        if (this.$route.query.storeId != null) {\n          this.query.houseId = this.$route.query.storeId\n          this.count = 1\n        }\n      }\n      this.loading.list = true\n      area.getList(this.query, this.page, this.sort).then(response => {\n        this.datas = response.data.items\n        this.page.total = response.data.total\n        this.loading.list = false\n      }).catch(reject => {\n        this.loading.list = false\n      })\n    },\n    getWarehouses() {\n      warehouse.getlist().then(response => {\n        this.warehouses = response.data\n      }).catch(reject => {\n      })\n    },\n    getAreaTypes() {\n      areaType.getlist().then(response => {\n        this.areaTypes = response.data\n      }).catch(reject => {\n      })\n    },\n    handleQuery() {\n      this.page.current = 1\n      this.getDatas()\n    },\n    handleSort(data) {\n      this.sort.prop = data.prop\n      this.sort.order = data.order\n      this.getDatas()\n    },\n    handleDetail(row) {\n      // 若列表数据展示了全部属性，则可直接拷贝列表数据\n      this.detail.models = Object.assign({}, row)\n      this.detail.dialog.visible = true\n      // this.loading.detail = true\n      // area.get(row.id).then(response => {\n      //   this.detail.models = response.data\n      //   this.loading.detail = false\n      // }).catch(reject => {\n      //   this.loading.detail = false\n      // })\n    },\n    handleCreate() {\n      this.create.dialog.visible = true\n    },\n    createData() {\n      this.$refs['formCreate'].validate((valid) => {\n        if (!valid) return false\n        area.create(this.create.models).then(response => {\n          // 为方便连续添加，create对话框不关闭\n          // 需要清空的表单，手动清空，至少清空一个必填项，防止点击两遍\n          this.create.models.name = ''\n          // 重新获取数据\n          this.getDatas()\n        }).catch(reject => {\n        })\n      })\n    },\n    handleRankup(row) {\n      this.loading.list = true\n      area.rankup(row.id).then(response => {\n        this.getDatas()\n        this.loading.list = false\n      }).catch(reject => {\n        this.loading.list = false\n      })\n    },\n    handleUpdate(row) {\n      // 若列表数据展示了全部属性，则可直接拷贝列表数据用于编辑\n      this.update.models = Object.assign({}, row)\n      this.update.dialog.visible = true\n      this.loading.update = true\n      // area.get(row.id).then(response => {\n      //   this.update.models = response.data\n      //   this.loading.update = false\n      // }).catch(reject => {\n      //   this.loading.update = false\n      // })\n      this.$nextTick(() => {\n        this.$refs['formUpdate'].clearValidate()\n      })\n      this.loading.update = false\n    },\n    updateData() {\n      this.$refs['formUpdate'].validate((valid) => {\n        if (!valid) return false\n        const tempData = Object.assign({}, this.update.models)\n        console.log(tempData)\n        area.update(tempData).then(response => {\n          // 重新获取数据\n          this.getDatas()\n          this.update.dialog.visible = false\n        }).catch(reject => {\n        })\n      })\n    },\n    handleDelete(row) {\n      this.deleteData([row.id])\n    },\n    handleDeletes(row) {\n      if (this.$refs.listTable.selection.length === 0) {\n        this.$message({ type: 'warning', message: '请在下面表格的左侧勾选需要删除的数据', duration: 4000 })\n        return false\n      }\n      const ids = []\n      for (const item of this.$refs.listTable.selection) {\n        ids.push(item.id)\n      }\n      this.deleteData(ids)\n    },\n    deleteData(ids) {\n      this.$confirm('数据删除后，不可恢复, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        area.del(ids).then(response => {\n          // 重新获取数据\n          this.getDatas()\n        }).catch(reject => {\n        })\n      })\n    },\n    handleUpload() {\n      this.upload.dialog.visible = true\n    },\n    submitUpload() {\n      this.$refs.upload.submit()\n    },\n    handleRemove() {\n      this.$refs.upload.clearFiles()\n    },\n    uploadUrl(param) {\n      // 先判断图片大小\n      const _file = param.file\n      // const _fileSize = _file.size < 1024 * 500\n      // if (!_fileSize) {\n      //   that.$message.warning('请上传500KB以下的文件')\n      //   return false\n      // }\n      // 参数\n      const params = new FormData()\n      params.append('excel', _file)\n      area.uploadExcel(params).then(response => {\n        const failureDetails = response.data.failureDetails\n        let message = ''\n        failureDetails.forEach((item) => {\n          message = message + '/' + item\n        })\n        this.$alert('成功：' + response.data.success + ' ' +\n          '失败：' + response.data.failure + ' ' +\n          '读取总数：' + response.data.total + ' ' +\n          '/' + message,\n        '消息提示',\n        {\n          confirmButtonText: '确定',\n          callback: action => {\n            this.upload.dialog.visible = false\n            this.handleRemove()\n            this.getDatas()\n          }\n        })\n      }).catch(reject => {\n      })\n    },\n    ExportTemplate() {\n      this.loading.export = true\n      import('@/vendor/Export2Excel').then(excel => {\n        const tHeader = ['编号', '名称', '类型', '所属仓库编号', '位置', '承重', '承重系数', '长', '宽', '高', '是否启用']\n        const data = ''\n        excel.export_json_to_excel({\n          header: tHeader,\n          data,\n          filename: '区域'\n        })\n        this.loading.export = false\n      })\n    },\n    handleExport() {\n      this.loading.export = true\n      area.exportExcel(this.query, this.page, this.sort).then(response => {\n        const blob = new Blob([response])\n        const fileName = '区域.xls'\n        const elink = document.createElement('a')\n        elink.onload = () => { URL.revokeObjectURL(elink.href) }\n        elink.download = fileName\n        elink.style.display = 'none'\n        elink.href = URL.createObjectURL(blob)\n        document.body.appendChild(elink)\n        elink.click()\n        document.body.removeChild(elink)\n        this.loading.export = false\n      }).catch(reject => {\n        this.loading.export = false\n      })\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n</style>\n"]}]}