{"remainingRequest":"C:\\Users\\Neal Wang\\IdeaProjects\\rescue_web\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\Users\\Neal Wang\\IdeaProjects\\rescue_web\\src\\views\\sample-manage\\sample.vue?vue&type=script&lang=js&","dependencies":[{"path":"C:\\Users\\Neal Wang\\IdeaProjects\\rescue_web\\src\\views\\sample-manage\\sample.vue","mtime":1606282276274},{"path":"C:\\Users\\Neal Wang\\IdeaProjects\\rescue_web\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Neal Wang\\IdeaProjects\\rescue_web\\node_modules\\babel-loader\\lib\\index.js","mtime":499162500000},{"path":"C:\\Users\\Neal Wang\\IdeaProjects\\rescue_web\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Neal Wang\\IdeaProjects\\rescue_web\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["sample.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA2KA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"sample.vue","sourceRoot":"src/views/sample-manage","sourcesContent":["<template>\n  <div class=\"app-container list\">\n    <div class=\"toolbar\">\n      <el-input v-model.trim=\"query.code\" class=\"query-item\" style=\"width: 120px\" placeholder=\"款式编号\" clearable @clear=\"handleQuery\" />\n      <el-input v-model.trim=\"query.name\" class=\"query-item\" style=\"width: 120px\" placeholder=\"款式名称\" clearable @clear=\"handleQuery\" />\n      <el-select v-model=\"query.operatorId\" class=\"query-item\" style=\"width: 150px\" placeholder=\"录入人员\" clearable @clear=\"handleQuery\">\n        <el-option v-for=\"item in creaters\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n      </el-select>\n\n      <el-date-picker\n        v-model=\"value1\"\n        align=\"right\"\n        end-placeholder=\"结束日期\"\n        range-separator=\"至\"\n        start-placeholder=\"开始日期\"\n        type=\"datetimerange\"\n        value-format=\"yyyy-MM-dd HH:mm:ss\"\n        @change=\"getTime\"\n      />\n      <el-button class=\"tool tool-query\" type=\"primary\" icon=\"el-icon-search\" @click=\"handleQuery\">查询</el-button>\n      <el-button v-if=\"access.create.allow\" class=\"tool tool-create\" type=\"primary\" icon=\"vue-icon-create\" @click=\"handleCreate\">添加</el-button>\n      <el-button v-if=\"access.delete.allow\" class=\"tool tool-delete\" type=\"danger\" icon=\"vue-icon-delete\" @click=\"handleDeletes\">批量删除</el-button>\n<!--      <el-button v-if=\"access.export.allow\" class=\"tool tool-export\" :loading=\"loading.export\" type=\"primary\" icon=\"vue-icon-excel\" @click=\"handleExport\">导出</el-button>-->\n    </div>\n\n    <el-table ref=\"listTable\" v-loading=\"loading.list\" v-adaptive=\"{ bottomOffset: 55 }\" height=\"200px\" :data=\"datas\" :default-sort=\"sort\" border fit highlight-current-row @sort-change=\"handleSort\">\n      <el-table-column type=\"selection\" align=\"center\" width=\"35\" />\n      <el-table-column label=\"序号\" type=\"index\" align=\"center\" width=\"65\" fixed>\n        <template slot-scope=\"scope\">\n          <span>{{ (page.current - 1) * page.size + scope.$index + 1 }}</span>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"款式编号\" prop=\"code\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\n      <el-table-column label=\"款式名称\" prop=\"name\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\n      <el-table-column label=\"打样数目\" prop=\"quality\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\n      <el-table-column label=\"打样时间\" prop=\"operateTime\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\n      <!--      <el-table-column label=\"打样人\" prop=\"creater.userName\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />-->\n      <el-table-column label=\"备注\" prop=\"amount\" :sort-orders=\"sortOrders\" align=\"center\" show-overflow-tooltip />\n      <el-table-column fixed=\"right\" label=\"操作\" align=\"center\" width=\"140\">\n        <template slot-scope=\"{row}\">\n          <el-tooltip v-if=\"access.detail.allow\" class=\"item\" effect=\"dark\" content=\"详情\" placement=\"top-end\">\n            <el-button type=\"primary\" plain class=\"button-operate button-detail\" size=\"mini\" @click=\"handleDetail(row)\"><i class=\"vue-icon-detail\" /></el-button>\n          </el-tooltip>\n          <el-tooltip v-if=\"access.update.allow\" class=\"item\" effect=\"dark\" content=\"编辑\" placement=\"top-end\">\n            <el-button type=\"primary\" plain class=\"button-operate button-update\" size=\"mini\" @click=\"handleUpdate(row)\"><i class=\"vue-icon-update\" /></el-button>\n          </el-tooltip>\n          <el-tooltip v-if=\"access.delete.allow\" class=\"item\" effect=\"dark\" content=\"删除\" placement=\"top-end\">\n            <el-button type=\"danger\" plain class=\"button-operate button-delete\" size=\"mini\" @click=\"handleDelete(row)\"><i class=\"vue-icon-delete\" /></el-button>\n          </el-tooltip>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <pagination :hidden=\"page.total==0\" :total=\"page.total\" :page.sync=\"page.current\" :limit.sync=\"page.size\" @pagination=\"getDatas\" />\n\n    <el-dialog v-loading=\"loading.detail\" custom-class=\"dialog-fullscreen dialog-detail\" :title=\"detail.dialog.title\" :visible.sync=\"detail.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\">\n      <el-form ref=\"formDetail\" label-position=\"right\" :label-width=\"detail.dialog.labelWidth\">\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"款式编号\">\n              {{ detail.models.code }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"款式名称\">\n              {{ detail.models.name }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"打样数目\">\n              {{ detail.models.quality }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"打样时间\">\n              {{ detail.models.operateTime }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"备注\">\n              {{ detail.models.remark }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n      </el-form>\n    </el-dialog>\n\n    <el-dialog custom-class=\"dialog-fullscreen dialog-create\" :title=\"create.dialog.title\" :visible.sync=\"create.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\" :close-on-click-modal=\"false\">\n      <el-form ref=\"formCreate\" :rules=\"create.rules\" :model=\"create.models\" label-position=\"right\" :label-width=\"create.dialog.labelWidth\">\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"打样编号\" prop=\"code\">\n              <el-input v-model=\"create.models.code\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"打样名称\" prop=\"name\">\n              <el-input v-model=\"create.models.name\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"数量\" prop=\"quality\">\n              <el-input v-model=\"create.models.quality\" type=\"number\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :sl=\"24\">\n            <el-form-item label=\"备注\" prop=\"remark\">\n              <el-input v-model=\"create.models.remark\" type=\"textarea\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"create.dialog.visible = false\">取消</el-button>\n        <el-button type=\"primary\" @click=\"createData()\">提交</el-button>\n      </div>\n    </el-dialog>\n    <el-dialog v-loading=\"loading.update\" custom-class=\"dialog-fullscreen dialog-update\" :title=\"update.dialog.title\" :visible.sync=\"update.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\" :close-on-click-modal=\"false\">\n      <el-form ref=\"formUpdate\" :rules=\"update.rules\" :model=\"update.models\" label-position=\"right\" :label-width=\"update.dialog.labelWidth\">\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"打样编号\" prop=\"code\">\n              <el-input v-model=\"update.models.code\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"打样名称\" prop=\"name\">\n              <el-input v-model=\"update.models.name\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"数量\" prop=\"quality\">\n              <el-input v-model=\"update.models.quality\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :sl=\"24\">\n            <el-form-item label=\"备注\" prop=\"description\">\n              <el-input v-model=\"update.models.remark\" type=\"textarea\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"update.dialog.visible = false\">取消</el-button>\n        <el-button type=\"primary\" @click=\"updateData()\">提交</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport adaptive from '@/directive/el-table'\nimport setRule from '@/utils/form-validate'\nimport Pagination from '@/components/Pagination'\n// import * as order from '@/api/order-manage/order'\nimport * as customer from '@/api/order-manage/customer'\nimport * as user from '@/api/system-manage/user'\nimport * as orderProperty from '@/api/order-manage/order-property'\nimport * as sample from '@/api/sample-manage/sample'\n\nexport default {\n  name: 'Sample',\n  components: { Pagination },\n  directives: { adaptive },\n  data() {\n    return {\n      access: this.$store.getters.access['SampleManage']['Sample'],\n      datas: null,\n      customers: null,\n      creaters: null,\n      orderProperties: null,\n      OrderPropertyValue: [],\n      value1: [],\n      query: { code: null, name: null, customerId: null, createrId: null, status: null },\n      page: { total: 0, current: 1, size: 20 },\n      sort: { prop: 'sort', order: 'ascending' },\n      detail: {\n        dialog: { title: '打样信息', visible: false, labelWidth: '150px' },\n        models: {\n          code: null,\n          name: null,\n          quality: null,\n          operateTime: null,\n          remark: null,\n          customer: { name: null },\n          creater: { userName: null },\n          amount: null,\n          amountDone: null,\n          amountPre: null,\n          amountReal: null,\n          deliveryTime: null,\n          deliveryTImeReal: null,\n          description: null,\n          status: null,\n          statusTime: null,\n          nonexistentProperties: [],\n          existentProperties: []\n        }\n      },\n      create: {\n        dialog: { title: '添加打样', visible: false, labelWidth: '150px' },\n        models: {\n          code: null, name: null, quality: null, operateTime: null, phoOrVideo: 1, operatorId: null, remark: null\n        },\n        rules: {\n          code: setRule('打样编号', [{ required: true }, { length: [0, 32] }]),\n          name: setRule('打样名称', [{ required: true }, { length: [0, 255] }]),\n          quality: setRule('数量', [{ selected: true }]),\n          operateTime: setRule('打样时间', [{ required: true }]),\n          operatorId: setRule('操作人员', [{ required: true }])\n        }\n      },\n      update: {\n        dialog: { title: '编辑打样信息', visible: false, labelWidth: '120px' },\n        models: {\n          code: null, name: null, quality: null, operateTime: null, phoOrVideo: 1, operatorId: null, remark: null\n        },\n        rules: {\n          code: setRule('打样编号', [{ required: true }, { length: [0, 32] }]),\n          name: setRule('打样名称', [{ required: true }, { length: [0, 255] }]),\n          quality: setRule('数量', [{ selected: true }]),\n          operateTime: setRule('打样时间', [{ required: true }]),\n          operatorId: setRule('操作人员', [{ required: true }])\n        }\n      },\n      loading: { list: true, export: false, detail: false, update: false },\n      sortOrders: ['ascending', 'descending']\n    }\n  },\n  created() {\n    this.getDatas()\n    this.getCustomers()\n    this.getUsers()\n    this.getOrderProperties()\n  },\n  methods: {\n    getCustomers() {\n      customer.getlist().then(response => {\n        this.customers = response.data\n        this.loading.detail = false\n      }).catch(reject => {\n        this.loading.detail = false\n      })\n    },\n    getUsers() {\n      user.getlist().then(response => {\n        this.creaters = response.data\n      }).catch(reject => {\n      })\n    },\n    getOrderProperties() {\n      const orderType = 0\n      orderProperty.getlist(orderType).then(response => {\n        this.orderProperties = response.data\n        // 初始化OrderPropertyValue 数组，每条数据依次赋上propertyId的值\n        for (let i = 0; i < response.data.length; i++) {\n          this.OrderPropertyValue.push({\n            value: null,\n            propertyId: null\n          })\n          this.OrderPropertyValue[i].propertyId = response.data[i].id\n        }\n        this.loading.detail = false\n      }).catch(reject => {\n        this.loading.detail = false\n      })\n    },\n    getDatas() {\n      this.loading.list = true\n      sample.getList(this.query, this.page, this.sort).then(response => {\n        this.datas = response.data.items\n        this.page.total = response.data.total\n        this.loading.list = false\n      }).catch(reject => {\n        this.loading.list = false\n      })\n    },\n    getTime(datetimerange) {\n      this.query.loginTime = this.value1[0]\n      this.query.loginOutTime = this.value1[1]\n    },\n    handleQuery() {\n      this.page.current = 1\n      this.getDatas()\n    },\n    handleSort(data) {\n      this.sort.prop = data.prop\n      this.sort.order = data.order\n      this.getDatas()\n    },\n    handleClean() {\n      this.create.models.order.propertyId = null\n    },\n    handleDetail(row) {\n      // 若列表数据展示了全部属性，则可直接拷贝列表数据\n      this.detail.models = Object.assign({}, row)\n      this.detail.dialog.visible = true\n      this.loading.detail = true\n      this.loading.detail = false\n      // order.get(row.id).then(response => {\n      //   this.detail.models = response.data\n      //   console.log(this.detail.models)\n      //   this.loading.detail = false\n      // }).catch(reject => {\n      //   this.loading.detail = false\n      // })\n    },\n    handleCreateCustomer() {\n      this.$router.push({\n        path: '/order-manage/customer',\n        query: { flag: true }\n      })\n    },\n    handleCreateDetail(row) {\n      const orderId = row.id\n      const orderCode = row.code\n      this.$router.push({\n        path: '/order-manage/order-detail',\n        query: {\n          orderCode: orderCode,\n          orderId: orderId\n        }\n      })\n    },\n    handleCreateRequest(row) {\n      const orderId = row.id\n      const orderCode = row.code\n      this.$router.push({\n        path: '/goods-manage/request',\n        query: {\n          orderId: orderId,\n          orderCode: orderCode\n        }\n      })\n    },\n    handleCreate() {\n      this.create.dialog.visible = true\n    },\n    createData() {\n      this.$refs['formCreate'].validate((valid) => {\n        if (!valid) return false\n        sample.create(this.create.models).then(response => {\n          // 为方便连续添加，create对话框不关闭\n          // 需要清空的表单，手动清空，至少清空一个必填项，防止点击两遍\n          this.create.models.order.code = ''\n          // 重新获取数据\n          this.getDatas()\n        }).catch(reject => {\n        })\n      })\n    },\n    handleUpdate(row) {\n      // 若列表数据展示了全部属性，则可直接拷贝列表数据用于编辑\n      this.update.models = Object.assign({}, row)\n      this.update.dialog.visible = true\n      this.loading.update = true\n      this.loading.update = false\n      // order.get(row.id).then(response => {\n      //   this.update.models = response.data\n      //   this.loading.update = false\n      // }).catch(reject => {\n      //   this.loading.update = false\n      // })\n      this.$nextTick(() => {\n        this.$refs['formUpdate'].clearValidate()\n      })\n    },\n    updateData() {\n      this.$refs['formUpdate'].validate((valid) => {\n        if (!valid) return false\n        const tempData = Object.assign({}, this.update.models)\n        sample.update(tempData).then(response => {\n          // 重新获取数据\n          this.getDatas()\n          this.update.dialog.visible = false\n        }).catch(reject => {\n        })\n      })\n    },\n    handleDelete(row) {\n      this.deleteData([row.id])\n    },\n    handleDeletes(row) {\n      if (this.$refs.listTable.selection.length === 0) {\n        this.$message({ type: 'warning', message: '请在下面表格的左侧勾选需要删除的数据', duration: 4000 })\n        return false\n      }\n      const ids = []\n      for (const item of this.$refs.listTable.selection) {\n        ids.push(item.id)\n      }\n      this.deleteData(ids)\n    },\n    deleteData(ids) {\n      this.$confirm('数据删除后，不可恢复, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        sample.del(ids).then(response => {\n          // 重新获取数据\n          this.getDatas()\n        }).catch(reject => {\n        })\n      })\n    },\n    deleteOrderProperties(id) {\n      this.$confirm('扩展属性删除后，不可恢复, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        orderProperty.del(id).then(response => {\n          // 重新获取数据\n          this.getDatas()\n          this.getOrderProperties()\n        }).catch(reject => {\n        })\n      })\n    },\n    handleExport() {\n      this.loading.export = true\n      sample.exportExcel(this.query, this.page, this.sort).then(response => {\n        const blob = new Blob([response])\n        const fileName = '打样.xls'\n        const elink = document.createElement('a')\n        elink.onload = () => { URL.revokeObjectURL(elink.href) }\n        elink.download = fileName\n        elink.style.display = 'none'\n        elink.href = URL.createObjectURL(blob)\n        document.body.appendChild(elink)\n        elink.click()\n        document.body.removeChild(elink)\n        this.loading.export = false\n      }).catch(reject => {\n        this.loading.export = false\n      })\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n</style>\n"]}]}