=\"width: 120px\" placeholder=\"货物种类\" clearable @clear=\"handleQuery\">-->\r\n      <!--<el-option v-for=\"item in goodsCategories\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />-->\r\n      <!--</el-select>-->\r\n      <el-cascader\r\n        v-model=\"query.goodsCategoryId\"\r\n        placeholder=\"请选择货物类别\"\r\n        :options=\"goodsCategories\"\r\n        :props=\"{ expandTrigger: 'hover', checkStrictly: true }\"\r\n        @change=\"handleChange\"\r\n      />\r\n      <el-select v-model=\"query.unitId\" class=\"query-item\" style=\"width: 120px\" placeholder=\"货物单位\" clearable @clear=\"handleQuery\">\r\n        <el-option v-for=\"item in goodsUnits\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\r\n      </el-select>\r\n      <el-select v-model=\"query.operatorId\" class=\"query-item\" style=\"width: 120px\" placeholder=\"操作人\" clearable @clear=\"handleQuery\">\r\n        <el-option v-for=\"item in operators\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\r\n      </el-select>\r\n      <el-button class=\"tool tool-query\" type=\"primary\" icon=\"el-icon-search\" @click=\"handleQuery\">查询</el-button>\r\n      <el-button v-if=\"access.export.allow\" class=\"tool tool-export\" :loading=\"loading.export\" type=\"primary\" icon=\"vue-icon-excel\" @click=\"handleExport\">导出</el-button>\r\n    </div>\r\n\r\n    <el-table ref=\"listTable\" v-loading=\"loading.list\" v-adaptive=\"{ bottomOffset: 55 }\" height=\"200px\" :data=\"datas\" :default-sort=\"sort\" border fit highlight-current-row @sort-change=\"handleSort\">\r\n      <el-table-column label=\"序号\" type=\"index\" align=\"center\" width=\"65\" fixed>\r\n        <template slot-scope=\"scope\">\r\n          <span>{{ (page.current - 1) * page.size + scope.$index + 1 }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"货物种类\" prop=\"goodsCategory.name\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />\r\n      <el-table-column label=\"货物单位\" prop=\"goodsUnit.name\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />\r\n      <el-table-column label=\"数量\" prop=\"quality\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />\r\n      <el-table-column label=\"运输设备上数量\" prop=\"qualityOnTemDev\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"单元格中数量\" prop=\"qualityOnTemCell\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"容器中数量\" prop=\"qualityOnTemVes\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"同一种类锁定总数量\" prop=\"qualityLocked\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"未验货数量\" prop=\"inspectionresultNull\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"验货未通过数量\" prop=\"inspectionresultNo\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"验货通过数量\" prop=\"inspectionresultYes\" :sort-orders=\"sortOrders\" align=\"center\" show-overflow-tooltip />\r\n      <!--<el-table-column label=\"更新时间\" prop=\"updateTime\" sortable=\"custom\" :sort-orders=\"sortOrders\" align=\"center\" show-overflow-tooltip />-->\r\n      <!--<el-table-column label=\"操作人\" prop=\"operator.name\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />-->\r\n      <el-table-column fixed=\"right\" label=\"操作\" align=\"center\" width=\"60\">\r\n        <template slot-scope=\"{row}\">\r\n          <el-tooltip v-if=\"access.detail.allow\" class=\"item\" effect=\"dark\" content=\"详情\" placement=\"top-end\">\r\n            <el-button type=\"primary\" plain class=\"button-operate button-detail\" size=\"mini\" @click=\"handleDetail(row)\"><i class=\"vue-icon-detail\" /></el-button>\r\n          </el-tooltip>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <pagination :hidden=\"page.total==0\" :total=\"page.total\" :page.sync=\"page.current\" :limit.sync=\"page.size\" @pagination=\"getDatas\" />\r\n\r\n    <el-dialog v-loading=\"loading.detail\" custom-class=\"dialog-detail\" :title=\"detail.dialog.title\" :visible.sync=\"detail.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\">\r\n      <el-form ref=\"formDetail\" label-position=\"right\" :label-width=\"detail.dialog.labelWidth\">\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"货物类别\">\r\n              {{ detail.models.goodsCategory.name }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"货物单位\">\r\n              {{ detail.models.goodsUnit.name }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"数量\">\r\n              {{ detail.models.quality }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"运输设备上总数量\">\r\n              {{ detail.models.qualityOnTemDev }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"单元格上总数量\">\r\n              {{ detail.models.qualityOnTemCell }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"容器中总数量\">\r\n              {{ detail.models.qualityOnTemVes }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"同一货物锁定总数量\">\r\n              {{ detail.models.qualityLocked }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"未验货数量\">\r\n              {{ detail.models.inspectionresultNull }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"验货不通过数量\">\r\n              {{ detail.models.inspectionresultNo }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"验货通过数量\">\r\n              {{ detail.models.inspectionresultYes }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"更新时间\">\r\n              {{ detail.models.updateTime }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"操作人员\">\r\n              {{ detail.models.operator.userName }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport adaptive from '@/directive/el-table'\r\nimport Pagination from '@/components/Pagination'\r\nimport * as goodsCategoryClassification from '@/api/goods-manage/goods-category-classification'\r\nimport * as viewStackBaseongoods from '@/api/data-statistics/view-stack-baseongoods'\r\nimport * as goodsUnit from '@/api/goods-manage/goods-unit'\r\nimport * as operator from '@/api/system-manage/user'\r\n\r\nexport default {\r\n  name: 'ViewStockBaseongoods',\r\n  components: { Pagination },\r\n  directives: { adaptive },\r\n  data() {\r\n    return {\r\n      access: this.$store.getters.access['DataStatistics']['ViewStockBaseongoods'],\r\n      datas: null,\r\n      goodsCategories: null,\r\n      goodsUnits: null,\r\n      operators: null,\r\n      query: { goodsCategoryId: null, unitId: null, operatorId: null },\r\n      page: { total: 0, current: 1, size: 20 },\r\n      sort: { prop: 'sort', order: 'ascending' },\r\n      detail: {\r\n        dialog: { title: '各类容器现状统计', visible: false, labelWidth: '150px' },\r\n        models: { goodsCategory: { name: null }, goodsUnit: { name: null }, quality: null, qualityOnTemDev: null, qualityOnTemCel: null, qualityOnTemVes: null, qualityLocked: null, inspectionresultNull: null, inspectionresultNo: null, inspectionresultYes: null, updateTime: null, operator: { userName: null }}\r\n      },\r\n      loading: { list: true, export: false, detail: false, update: false },\r\n      sortOrders: ['ascending', 'descending']\r\n    }\r\n  },\r\n  created() {\r\n    this.getDatas()\r\n    this.getGoodsCategoryClassification()\r\n    this.getGoodsUnits()\r\n    this.getOperators()\r\n  },\r\n  methods: {\r\n    getDatas() {\r\n      this.loading.list = true\r\n      viewStackBaseongoods.getList(this.query, this.page, this.sort).then(response => {\r\n        this.datas = response.data.items\r\n        this.page.total = response.data.total\r\n        this.loading.list = false\r\n      }).catch(reject => {\r\n        this.loading.list = false\r\n      })\r\n    },\r\n    getGoodsCategoryClassification() {\r\n      goodsCategoryClassification.getlist(2).then(response => {\r\n        this.goodsCategories = response.data\r\n      }).catch(reject => {\r\n      })\r\n    },\r\n    getGoodsUnits() {\r\n      goodsUnit.getlist().then(response => {\r\n        this.goodsUnits = response.data\r\n      }).catch(reject => {\r\n      })\r\n    },\r\n    getOperators() {\r\n      operator.getlist().then(response => {\r\n        this.operators = response.data\r\n      }).catch(reject => {\r\n      })\r\n    },\r\n    handleChange(value) {\r\n      const id = value[value.length - 1]\r\n      this.query.goodsCategoryId = id\r\n    },\r\n    handleQuery() {\r\n      this.page.current = 1\r\n      this.getDatas()\r\n    },\r\n    handleSort(data) {\r\n      this.sort.prop = data.prop\r\n      this.sort.order = data.order\r\n      this.getDatas()\r\n    },\r\n    handleDetail(row) {\r\n      // 若列表数据展示了全部属性，则可直接拷贝列表数据\r\n      this.detail.models = Object.assign({}, row)\r\n      this.detail.dialog.visible = true\r\n      // this.loading.detail = true\r\n      // viewStackBaseongoods.get(row.id).then(response => {\r\n      //   this.detail.models = response.data\r\n      //   this.loading.detail = false\r\n      // }).catch(reject => {\r\n      //   this.loading.detail = false\r\n      // })\r\n    },\r\n    handleExport() {\r\n      this.loading.export = true\r\n      viewStackBaseongoods.exportExcel(this.query, this.page, this.sort).then(response => {\r\n        const blob = new Blob([response])\r\n        const fileName = '仓储现状.xls'\r\n        const elink = document.createElement('a')\r\n        elink.onload = () => { URL.revokeObjectURL(elink.href) }\r\n        elink.download = fileName\r\n        elink.style.display = 'none'\r\n        elink.href = URL.createObjectURL(blob)\r\n        document.body.appendChild(elink)\r\n        elink.click()\r\n        document.body.removeChild(elink)\r\n        this.loading.export = false\r\n      }).catch(reject => {\r\n        this.loading.export = false\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n</style>\r\n"]}]}