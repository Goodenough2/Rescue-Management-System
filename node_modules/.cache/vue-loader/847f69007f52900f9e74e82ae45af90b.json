{"remainingRequest":"C:\\Users\\Neal Wang\\IdeaProjects\\mingxin_web\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\Users\\Neal Wang\\IdeaProjects\\mingxin_web\\src\\views\\goods-manage\\goods-category-classification.vue?vue&type=style&index=1&lang=css&","dependencies":[{"path":"C:\\Users\\Neal Wang\\IdeaProjects\\mingxin_web\\src\\views\\goods-manage\\goods-category-classification.vue","mtime":1596434368944},{"path":"C:\\Users\\Neal Wang\\IdeaProjects\\mingxin_web\\node_modules\\css-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Neal Wang\\IdeaProjects\\mingxin_web\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":499162500000},{"path":"C:\\Users\\Neal Wang\\IdeaProjects\\mingxin_web\\node_modules\\postcss-loader\\src\\index.js","mtime":499162500000},{"path":"C:\\Users\\Neal Wang\\IdeaProjects\\mingxin_web\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Neal Wang\\IdeaProjects\\mingxin_web\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["goods-category-classification.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAgYA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"goods-category-classification.vue","sourceRoot":"src/views/goods-manage","sourcesContent":["<template>\r\n  <div class=\"app-container list\">\r\n    <div class=\"toolbar\">\r\n      <el-input v-model.trim=\"query.code\" class=\"query-item\" style=\"width: 160px\" placeholder=\"货物类别分类编号\" clearable @clear=\"handleQuery\" />\r\n      <el-input v-model.trim=\"query.name\" class=\"query-item\" style=\"width: 160px\" placeholder=\"货物类别分类名称\" clearable @clear=\"handleQuery\" />\r\n      <el-button class=\"tool tool-query\" type=\"primary\" icon=\"el-icon-search\" @click=\"handleQuery\">查询</el-button>\r\n      <el-button class=\"tool tool-create\" type=\"primary\" icon=\"vue-icon-create\" @click=\"handleCreate\">添加</el-button>\r\n      <el-button class=\"tool tool-delete\" type=\"danger\" icon=\"vue-icon-delete\" @click=\"handleDeletes\">批量删除</el-button>\r\n      <el-button class=\"tool tool-export\" :loading=\"loading.export\" type=\"primary\" icon=\"vue-icon-excel\" @click=\"handleExport\">导出</el-button>\r\n    </div>\r\n\r\n    <el-table\r\n      ref=\"listTable\"\r\n      v-loading=\"loading.list\"\r\n      v-adaptive=\"{ bottomOffset: 55 }\"\r\n      height=\"200px\"\r\n      :data=\"datas\"\r\n      :cell-class-name=\"tableRowClassName\"\r\n      :default-sort=\"sort\"\r\n      border\r\n      fit\r\n      highlight-current-row\r\n      @sort-change=\"handleSort\"\r\n    >\r\n      <el-table-column type=\"selection\" align=\"center\" width=\"35\" />\r\n      <el-table-column label=\"货物类别分类编号\" prop=\"code\" :sort-orders=\"sortOrders\" align=\"center\" width=\"200\" show-overflow-tooltip />\r\n      <el-table-column label=\"货物类别分类名称\" prop=\"name\" :sort-orders=\"sortOrders\" align=\"center\" width=\"200\" show-overflow-tooltip />\r\n      <el-table-column label=\"类型描述\" prop=\"description\" :sort-orders=\"sortOrders\" align=\"center\" show-overflow-tooltip />\r\n      <el-table-column fixed=\"right\" label=\"操作\" align=\"center\" width=\"180\">\r\n        <template slot-scope=\"{row}\">\r\n          <el-tooltip class=\"item\" effect=\"dark\" content=\"排序上移\" placement=\"top-end\">\r\n            <el-button type=\"primary\" plain class=\"button-operate button-rankup\" size=\"mini\" @click=\"handleRankup(row)\"><i class=\"vue-icon-rankup\" /></el-button>\r\n          </el-tooltip>\r\n          <el-tooltip class=\"item\" effect=\"dark\" content=\"详情\" placement=\"top-end\">\r\n            <el-button type=\"primary\" plain class=\"button-operate button-detail\" size=\"mini\" @click=\"handleDetail(row)\"><i class=\"vue-icon-detail\" /></el-button>\r\n          </el-tooltip>\r\n          <el-tooltip class=\"item\" effect=\"dark\" content=\"编辑\" placement=\"top-end\">\r\n            <el-button type=\"primary\" plain class=\"button-operate button-update\" size=\"mini\" @click=\"handleUpdate(row)\"><i class=\"vue-icon-update\" /></el-button>\r\n          </el-tooltip>\r\n          <el-tooltip class=\"item\" effect=\"dark\" content=\"删除\" placement=\"top-end\">\r\n            <el-button type=\"danger\" plain class=\"button-operate button-delete\" size=\"mini\" @click=\"handleDelete(row)\"><i class=\"vue-icon-delete\" /></el-button>\r\n          </el-tooltip>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <el-dialog v-loading=\"loading.detail\" custom-class=\"dialog-fullscreen\" :title=\"detail.dialog.title\" :visible.sync=\"detail.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\">\r\n      <el-form ref=\"formDetail\" label-position=\"right\" :label-width=\"detail.dialog.labelWidth\">\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"货物类别分类编号\">\r\n              {{ detail.models.code }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"货物类别分类名称\">\r\n              {{ detail.models.name }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item v-if=\"detail.models.parent != null\" label=\"父类别名称\">\r\n              {{ detail.models.parent.name }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"货物类别分类描述\">\r\n              {{ detail.models.description }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n    </el-dialog>\r\n\r\n    <el-dialog custom-class=\"dialog-fullscreen\" :title=\"create.dialog.title\" :visible.sync=\"create.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\" :close-on-click-modal=\"false\">\r\n      <el-form ref=\"formCreate\" :rules=\"create.rules\" :model=\"create.models\" label-position=\"right\" :label-width=\"create.dialog.labelWidth\">\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"编号\" prop=\"code\">\r\n              <el-input v-model=\"create.models.code\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"名称\" prop=\"name\">\r\n              <el-input v-model=\"create.models.name\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"分类\" prop=\"parentId\">\r\n              <el-cascader\r\n                v-model=\"create.models.parentId\"\r\n                :options=\"goodsCategoryClassifications\"\r\n                :props=\"{ expandTrigger: 'hover', checkStrictly: true }\"\r\n                @change=\"handleChange\"\r\n              />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"描述\" prop=\"description\">\r\n              <el-input v-model=\"create.models.description\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"create.dialog.visible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"createData()\">提交</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n    <el-dialog v-loading=\"loading.update\" custom-class=\"dialog-fullscreen\" :title=\"update.dialog.title\" :visible.sync=\"update.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\" :close-on-click-modal=\"false\">\r\n      <el-form ref=\"formUpdate\" :rules=\"update.rules\" :model=\"update.models\" label-position=\"right\" :label-width=\"update.dialog.labelWidth\">\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"货物类别分类编号\" prop=\"code\">\r\n              <el-input v-model=\"update.models.code\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"货物类别分类名称\" prop=\"name\">\r\n              <el-input v-model=\"update.models.name\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"父级别\" prop=\"parentId\">\r\n              <el-cascader\r\n                v-model=\"update.models.parentId\"\r\n                :options=\"goodsCategoryClassifications\"\r\n                :props=\"{ expandTrigger: 'hover', checkStrictly: true }\"\r\n                @change=\"handleChange\"\r\n              />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"货物类别分类描述\" prop=\"description\">\r\n              <el-input v-model=\"update.models.description\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"update.dialog.visible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"updateData()\">提交</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport adaptive from '@/directive/el-table'\r\nimport setRule from '@/utils/form-validate'\r\nimport * as goodsCategoryClassification from '@/api/goods-manage/goods-category-classification'\r\n\r\nexport default {\r\n  name: 'GoodsType',\r\n  directives: { adaptive },\r\n  data() {\r\n    return {\r\n      access: this.$store.getters.access['GoodsManage']['GoodsCategoryClassification'],\r\n      datas: null,\r\n      query: { code: null, name: null },\r\n      page: { total: 0, current: 1, size: 20 },\r\n      sort: { prop: 'sort', order: 'ascending' },\r\n      goodsCategoryClassifications: [],\r\n      detail: {\r\n        dialog: { title: '货物类别分类信息', visible: false, labelWidth: '150px' },\r\n        models: { code: null, name: null, parent: { name: null }, description: null }\r\n      },\r\n      create: {\r\n        dialog: { title: '添加货物类别分类', visible: false, labelWidth: '150px' },\r\n        models: { code: null, name: null, parentId: null, description: null, level: null },\r\n        rules: {\r\n          code: setRule('货物类别分类编号', [{ required: true }, { length: [0, 32] }]),\r\n          name: setRule('货物类别分类名称', [{ required: true }, { length: [0, 20] }])\r\n        }\r\n      },\r\n      update: {\r\n        dialog: { title: '编辑类别信息', visible: false, labelWidth: '130px' },\r\n        models: { code: null, name: null, parentId: null, description: null, level: null },\r\n        rules: {\r\n          code: setRule('货物类别分类编号', [{ required: true }, { length: [0, 32] }]),\r\n          name: setRule('货物类别分类名称', [{ required: true }, { length: [0, 20] }])\r\n        }\r\n      },\r\n      loading: { list: true, export: false, detail: false, update: false },\r\n      sortOrders: ['ascending', 'descending']\r\n    }\r\n  },\r\n  created() {\r\n    this.getDatas()\r\n  },\r\n  mounted() {\r\n    this.getClassifications()\r\n  },\r\n  methods: {\r\n    getDatas() {\r\n      this.loading.list = true\r\n      goodsCategoryClassification.getList(this.query, this.page, this.sort).then(response => {\r\n        this.datas = response.data.items\r\n        this.page.total = response.data.total\r\n        this.loading.list = false\r\n      }).catch(reject => {\r\n        this.loading.list = false\r\n      })\r\n    },\r\n    getClassifications() {\r\n      goodsCategoryClassification.getlist(1).then(response => {\r\n        this.goodsCategoryClassifications = response.data\r\n      }).catch(reject => {\r\n      })\r\n    },\r\n    handleQuery() {\r\n      this.page.current = 1\r\n      this.getDatas()\r\n    },\r\n    handleSort(data) {\r\n      this.sort.prop = data.prop\r\n      this.sort.order = data.order\r\n      this.getDatas()\r\n    },\r\n    handleChange(value) {\r\n      const id = value[value.length - 1]\r\n      goodsCategoryClassification.get(id).then(response => {\r\n        this.update.models.parentId = id\r\n        this.create.models.parentId = id\r\n        this.create.models.level = response.data.level + 1\r\n        this.update.models.level = response.data.level + 1\r\n      }).catch(reject => {\r\n      })\r\n    },\r\n    handleDetail(row) {\r\n      // 若列表数据展示了全部属性，则可直接拷贝列表数据\r\n      this.detail.models = Object.assign({}, row)\r\n      this.detail.dialog.visible = true\r\n      this.loading.detail = true\r\n      // goodsCategoryClassification.get(row.id).then(response => {\r\n      //   this.detail.models = response.data\r\n      //   this.loading.detail = false\r\n      // }).catch(reject => {\r\n      //   this.loading.detail = false\r\n      // })\r\n      this.loading.detail = false\r\n    },\r\n    handleCreate() {\r\n      this.create.dialog.visible = true\r\n      this.getClassifications()\r\n    },\r\n    createData() {\r\n      this.$refs['formCreate'].validate((valid) => {\r\n        if (!valid) return false\r\n        goodsCategoryClassification.create(this.create.models).then(response => {\r\n          // 为方便连续添加，create对话框不关闭\r\n          // 需要清空的表单，手动清空，至少清空一个必填项，防止点击两遍\r\n          this.create.models.name = ''\r\n          this.create.models.code = ''\r\n          // 重新获取数据\r\n          this.getDatas()\r\n        }).catch(reject => {\r\n        })\r\n      })\r\n    },\r\n    handleRankup(row) {\r\n      this.loading.list = true\r\n      goodsCategoryClassification.rankup(row.id).then(response => {\r\n        this.getDatas()\r\n        this.loading.list = false\r\n      }).catch(reject => {\r\n        this.loading.list = false\r\n      })\r\n    },\r\n    handleUpdate(row) {\r\n      //  若列表数据展示了全部属性，则可直接拷贝列表数据用于编辑\r\n      this.update.models = Object.assign({}, row)\r\n      this.getClassifications()\r\n      this.update.dialog.visible = true\r\n      this.loading.update = true\r\n      // goodsType.get(row.id).then(response => {\r\n      //   this.update.models = response.data\r\n      //   this.loading.update = false\r\n      // }).catch(reject => {\r\n      //   this.loading.update = false\r\n      // })\r\n      this.$nextTick(() => {\r\n        this.$refs['formUpdate'].clearValidate()\r\n      })\r\n      this.loading.update = false\r\n    },\r\n    updateData() {\r\n      this.$refs['formUpdate'].validate((valid) => {\r\n        if (!valid) return false\r\n        const tempData = Object.assign({}, this.update.models)\r\n        goodsCategoryClassification.update(tempData).then(response => {\r\n          // 重新获取数据\r\n          this.getDatas()\r\n          this.update.dialog.visible = false\r\n        }).catch(reject => {\r\n        })\r\n      })\r\n    },\r\n    handleDelete(row) {\r\n      this.deleteData([row.id])\r\n    },\r\n    handleDeletes(row) {\r\n      if (this.$refs.listTable.selection.length === 0) {\r\n        this.$message({ type: 'warning', message: '请在下面表格的左侧勾选需要删除的数据', duration: 4000 })\r\n        return false\r\n      }\r\n      const ids = []\r\n      for (const item of this.$refs.listTable.selection) {\r\n        ids.push(item.id)\r\n      }\r\n      this.deleteData(ids)\r\n    },\r\n    deleteData(ids) {\r\n      this.$confirm('数据删除后，不可恢复, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        goodsCategoryClassification.del(ids).then(response => {\r\n          // 重新获取数据\r\n          this.getDatas()\r\n        }).catch(reject => {\r\n          this.getDatas()\r\n        })\r\n      })\r\n    },\r\n    handleExport() {\r\n      this.loading.export = true\r\n      goodsCategoryClassification.exportExcel(this.query, this.page, this.sort).then(response => {\r\n        const blob = new Blob([response])\r\n        const fileName = '货物类别分类.xls'\r\n        const elink = document.createElement('a')\r\n        elink.onload = () => { URL.revokeObjectURL(elink.href) }\r\n        elink.download = fileName\r\n        elink.style.display = 'none'\r\n        elink.href = URL.createObjectURL(blob)\r\n        document.body.appendChild(elink)\r\n        elink.click()\r\n        document.body.removeChild(elink)\r\n        this.loading.export = false\r\n      }).catch(reject => {\r\n        this.loading.export = false\r\n      })\r\n    },\r\n    tableRowClassName({ row, column, rowIndex, columnIndex }) {\r\n      if (columnIndex === 2) {\r\n        if (row.level === 1) {\r\n          return 'level1'\r\n        } else if (row.level === 2) {\r\n          return 'level2'\r\n        } else if (row.level === 3) {\r\n          return 'level3'\r\n        } else if (row.level === 4) {\r\n          return 'level4'\r\n        }\r\n      }\r\n      return ''\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n</style>\r\n<style>\r\n   .level1 {\r\n    text-indent: 5em;\r\n  }\r\n\r\n   .level2 {\r\n        text-indent: 10em;\r\n    }\r\n   .level3 {\r\n     text-indent: 15em;\r\n   }\r\n   .level4 {\r\n     text-indent: 20em;\r\n   }\r\n</style>\r\n"]}]}