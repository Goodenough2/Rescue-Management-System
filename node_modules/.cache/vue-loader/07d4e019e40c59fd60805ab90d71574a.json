{"remainingRequest":"C:\\Users\\Neal Wang\\IdeaProjects\\rescue_web\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\Users\\Neal Wang\\IdeaProjects\\rescue_web\\src\\views\\business-operation\\goods-preoutput.vue?vue&type=style&index=0&id=8681025c&lang=scss&scoped=true&","dependencies":[{"path":"C:\\Users\\Neal Wang\\IdeaProjects\\rescue_web\\src\\views\\business-operation\\goods-preoutput.vue","mtime":1607915911854},{"path":"C:\\Users\\Neal Wang\\IdeaProjects\\rescue_web\\node_modules\\css-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Neal Wang\\IdeaProjects\\rescue_web\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":499162500000},{"path":"C:\\Users\\Neal Wang\\IdeaProjects\\rescue_web\\node_modules\\postcss-loader\\src\\index.js","mtime":499162500000},{"path":"C:\\Users\\Neal Wang\\IdeaProjects\\rescue_web\\node_modules\\sass-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Neal Wang\\IdeaProjects\\rescue_web\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Neal Wang\\IdeaProjects\\rescue_web\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["goods-preoutput.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAqXA;AACA;AACA;AACA;AACA","file":"goods-preoutput.vue","sourceRoot":"src/views/business-operation","sourcesContent":["<template>\n  <div class=\"app-container list\">\n    <el-form ref=\"formOut\" :rules=\"out.rules\" :model=\"out.models\" label-position=\"right\" :label-width=\"out.dialog.labelWidth\">\n<!--      <el-row>-->\n<!--        <el-col :xl=\"24\">-->\n<!--          <el-form-item label=\"操作意图\" prop=\"storeOperateTypeId\">-->\n<!--            <el-select v-model=\"out.models.storeOperateTypeId\" clearable filterable>-->\n<!--              <el-option v-for=\"item in storeOperateTypes\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />-->\n<!--            </el-select>-->\n<!--          </el-form-item>-->\n<!--        </el-col>-->\n<!--      </el-row>-->\n      <el-row>\n        <el-col :xl=\"4\" :lg=\"8\" :md=\"12\" :sm=\"18\" :xs=\"24\">\n          <el-form-item label=\"点击添加\">\n            <el-button @click=\"addDomain\">新增出库货物</el-button>\n          </el-form-item>\n        </el-col>\n      </el-row>\n      <div v-for=\"(outGood, index) in out.models.outGoods\" :key=\"index\" class=\"unit\">\n        <el-row>\n          <el-col :xl=\"15\" :lg=\"16\" :md=\"16\" :sm=\"18\" :xs=\"24\">\n            <el-form-item :key=\"outGood.key\" :label=\" '货物条形码/RFID码' \" :prop=\"'outGoods.' + index + '.goodsCode'\" :rules=\"{required: true, message: '请输入货物条形码/RFID码', trigger: 'blur'}\">\n              <el-input v-model=\"outGood.goodsCode\" :disabled=\"true\" />\n            </el-form-item>\n          </el-col>\n          <el-col :xl=\"1\" :lg=\"1\" :sm=\"1\" :xs=\"1\">\n            <el-tooltip class=\"search-unit\" effect=\"dark\" content=\"搜索货物\" placement=\"top-end\">\n              <el-button type=\"primary\" plain class=\"button-operate button-detail\" circle icon=\"el-icon-search\" @click=\"handleCreate(outGood)\" />\n            </el-tooltip>\n          </el-col>\n          <!--          <el-col :xl=\"4\" :lg=\"8\" :md=\"12\" :sm=\"18\" :xs=\"24\">-->\n          <!--            <el-form-item :key=\"outGood.key\" :label=\" '货物实际总数量' \" :prop=\"'outGoods.' + index + '.quality'\" :rules=\"{ required: true, message: '请输入货物实际总数量', trigger: 'blur' }\">-->\n          <!--              <el-input v-model=\"outGood.quality\" :disabled=\"true\" type=\"number\" />-->\n          <!--            </el-form-item>-->\n          <!--          </el-col>-->\n          <el-tooltip class=\"remove-unit\" effect=\"dark\" content=\"删除货物存储模式\" placement=\"top-end\">\n            <el-button type=\"danger\" plain icon=\"el-icon-delete\" circle @click.prevent=\"removeDomain(outGood)\" />\n          </el-tooltip>\n        </el-row>\n      </div>\n      <!--      <el-row>-->\n      <!--        <el-col :xl=\"24\">-->\n      <!--          <el-form-item label=\"领料详单编号\" prop=\"requestDetailCode\">-->\n      <!--            <el-input v-model=\"out.models.requestDetailCode\" />-->\n      <!--          </el-form-item>-->\n      <!--        </el-col>-->\n      <!--      </el-row>-->\n      <el-row>\n        <el-col :xl=\"24\">\n          <el-form-item label=\"提货人员信息\" prop=\"deliveryman\">\n            <el-input v-model=\"out.models.deliveryman\" />\n          </el-form-item>\n        </el-col>\n      </el-row>\n      <el-row>\n        <el-col :xl=\"24\">\n          <el-form-item label=\"出库备注描述\" prop=\"description\">\n            <el-input v-model=\"out.models.description\" type=\"textarea\" />\n          </el-form-item>\n        </el-col>\n      </el-row>\n      <el-row>\n        <el-col :xl=\"24\">\n          <el-form-item>\n            <el-button type=\"primary\" @click=\"outData()\">预出库</el-button>\n          </el-form-item>\n        </el-col>\n      </el-row>\n    </el-form>\n    <el-dialog custom-class=\"dialog-fullscreen dialog-create\" :title=\"create.dialog.title\" :visible.sync=\"create.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\" :close-on-click-modal=\"false\">\n      <div class=\"toolbar\">\n        <el-cascader\n          v-model=\"query.multiLevelStoreId\"\n          placeholder=\"请选择存储级别\"\n          :options=\"fiveLevelStores\"\n          :props=\"{ expandTrigger: 'hover', checkStrictly: true }\"\n          clearable\n          @change=\"handleStoreChange\"\n          @clear=\"handleQuery\"\n        />\n        <el-cascader\n          v-model=\"query.goodsCategoryId\"\n          placeholder=\"请选择货物类别\"\n          :options=\"goodsCategoryClassificationWithCategory\"\n          :props=\"{ expandTrigger: 'hover', checkStrictly: true }\"\n          clearable\n          @change=\"handleCategoryChange\"\n          @clear=\"handleQuery\"\n        />\n        <el-select v-model=\"query.storageModeId\" class=\"query-item\" style=\"width: 120px\" placeholder=\"存储模式\" clearable @clear=\"handleQuery\">\n          <el-option v-for=\"item in goodsStorageModes\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n        </el-select>\n        <el-select v-model=\"query.inspectorId\" class=\"query-item\" style=\"width: 120px\" placeholder=\"验货人员\" clearable @clear=\"handleQuery\">\n          <el-option v-for=\"item in operators\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n        </el-select>\n        <el-select v-model=\"query.locationType\" class=\"query-item\" style=\"width: 150px\" placeholder=\"正常存储位置\" clearable @clear=\"handleQuery\">\n          <el-option value=\"0\" label=\"运输设备\" />\n          <el-option value=\"1\" label=\"单元格\" />\n          <el-option value=\"2\" label=\"容器\" />\n        </el-select>\n        <el-select v-model=\"query.locked\" class=\"query-item\" style=\"width: 120px\" placeholder=\"锁定状态\" clearable @clear=\"handleQuery\">\n          <el-option value=\"0\" label=\"未锁定\" />\n          <el-option value=\"1\" label=\"部分锁定\" />\n          <el-option value=\"2\" label=\"锁定\" />\n        </el-select>\n        <el-select v-model=\"query.inspectionResult\" class=\"query-item\" style=\"width: 120px\" placeholder=\"验货结果\" clearable @clear=\"handleQuery\">\n          <el-option value=\"0\" label=\"未通过\" />\n          <el-option value=\"1\" label=\"通过\" />\n        </el-select>\n        <el-button class=\"tool tool-query\" type=\"primary\" icon=\"el-icon-search\" @click=\"handleQuery\">查询</el-button>\n      </div>\n      <el-table ref=\"listTable\" v-loading=\"loading.list\" v-adaptive=\"{ bottomOffset: 55 }\" height=\"200px\" :data=\"datas\" :default-sort=\"sort\" border fit highlight-current-row @sort-change=\"handleSort\">\n        <el-table-column type=\"selection\" align=\"center\" width=\"35\" />\n        <el-table-column label=\"序号\" type=\"index\" align=\"center\" width=\"65\" fixed>\n          <template slot-scope=\"scope\">\n            <span>{{ (page.current - 1) * page.size + scope.$index + 1 }}</span>\n          </template>\n        </el-table-column>\n        <el-table-column label=\"最近更新时间\" prop=\"updateTime\" sortable align=\"center\" width=\"160\" show-overflow-tooltip />\n        <el-table-column label=\"货物RFID\" prop=\"goodsRfidCode\" sortable align=\"center\" width=\"150\" show-overflow-tooltip />\n        <el-table-column label=\"货物条形码\" prop=\"goodsBarCode\" sortable align=\"center\" width=\"150\" show-overflow-tooltip />\n        <el-table-column label=\"货物类别\" prop=\"goodsCategory.name\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />\n        <el-table-column label=\"货物实际总数量\" prop=\"quality\" sortable align=\"center\" width=\"100\" show-overflow-tooltip />\n        <el-table-column label=\"被锁定的数量\" prop=\"qualityLocked\" sortable align=\"center\" show-overflow-tooltip />\n        <el-table-column label=\"存储模式名称\" prop=\"goodsStoragemode.name\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />\n        <el-table-column label=\"货物存储位置类型\" prop=\"locationType\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip>\n          <template slot-scope=\"{row}\">\n            <span v-if=\"row.locationType == 0\">运输设备</span>\n            <span v-if=\"row.locationType == 1\">单元格</span>\n            <span v-if=\"row.locationType == 2\">容器</span>\n          </template>\n        </el-table-column>\n        <el-table-column label=\"锁定状态\" prop=\"locked\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip>\n          <template slot-scope=\"{row}\">\n            <span v-if=\"row.locked == 0\">未锁定</span>\n            <span v-if=\"row.locked == 1\">部分锁定</span>\n            <span v-if=\"row.locked == 2\">锁定</span>\n          </template>\n        </el-table-column>\n        <el-table-column label=\"验货结果\" prop=\"inspectionResult\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip>\n          <template slot-scope=\"{row}\">\n            <span v-if=\"row.inspectionResult == 0\">未通过</span>\n            <span v-if=\"row.inspectionResult == 1\">通过</span>\n          </template>\n        </el-table-column>\n        <!--        <el-table-column fixed=\"right\" label=\"出库操作\" align=\"center\" width=\"60\" style=\"font-size: 1.2em;\">-->\n        <!--          <template slot-scope=\"{row}\">-->\n        <!--            <el-tooltip class=\"item\" effect=\"dark\" content=\"出库\" placement=\"top-end\">-->\n        <!--              <el-button type=\"primary\" plain class=\"button-operate button-detail\" size=\"mini\" @click=\"handleOutput(row)\"><i class=\"el-icon-d-arrow-left\" /></el-button>-->\n        <!--            </el-tooltip>-->\n        <!--          </template>-->\n        <!--        </el-table-column>-->\n      </el-table>\n      <pagination :hidden=\"page.total==0\" :total=\"page.total\" :page.sync=\"page.current\" :limit.sync=\"page.size\" @pagination=\"getDatas\" />\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"create.dialog.visible = false\">取消</el-button>\n        <el-button type=\"primary\" @click=\"handleCreates()\">确认出库</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport adaptive from '@/directive/el-table'\nimport setRule from '@/utils/form-validate'\nimport Pagination from '@/components/Pagination'\nimport * as goods from '@/api/goods-manage/goods'\nimport * as storeOperateType from '@/api/log-manage/store-operate-type'\nimport * as operator from '@/api/system-manage/user'\nimport * as goodsCategoryClassification from '@/api/goods-manage/goods-category-classification'\nimport * as store from '@/api/store-manage/store'\nimport * as goodsStorageMode from '@/api/goods-manage/goods-storageMode'\n\nexport default {\n  name: 'GoodsPreOutput',\n  components: { Pagination },\n  directives: { adaptive },\n  data() {\n    return {\n      storeOperateTypes: null,\n      outGoodModel: null,\n      datas: null,\n      operators: null,\n      fiveLevelStores: null,\n      goodsStorageModes: null,\n      goodsCategoryClassificationWithCategory: null,\n      query: { multiLevelStoreId: null, goodsCategoryId: null, storageModeId: null, locationType: null, locked: null, inspectionResult: null, inspectorId: null },\n      page: { total: 0, current: 1, size: 20 },\n      sort: { prop: 'sort', order: 'ascending' },\n      sortOrders: ['ascending', 'descending'],\n      out: {\n        dialog: { title: '货物出库', visible: false, labelWidth: '150px' },\n        models: {\n          storeOperateTypeId: null,\n          requestDetailCode: null,\n          deliveryman: null,\n          description: null,\n          goodsCode: null,\n          outGoods: [\n          ] },\n        rules: {\n          storeOperateTypeId: setRule('操作类型', [{ selected: true }]),\n          goodsCode: setRule('货物RFID码或条形码', [{ required: true }, { length: [0, 255] }]),\n          requestDetailCode: setRule('领料详单编号', [{ required: true }, { length: [0, 32] }]),\n          deliveryman: setRule('提货人信息', [{ length: [0, 50] }]),\n          description: setRule('出库备注描述', [{ length: [0, 255] }])\n        }\n      },\n      loading: { list: true, export: false, detail: false, update: false },\n      create: {\n        dialog: { title: '搜索货物', visible: false, labelWidth: '120px' },\n        models: { name: null, description: null },\n        rules: {\n          name: setRule('名称', [{ required: true }, { length: [0, 50] }]),\n          // ioType: setRule('出入类型', [{ selected: true }]),\n          canBeModified: setRule('出入类型', [{ selected: true }]),\n          description: setRule('描述', [{ length: [0, 255] }])\n        }\n      }\n    }\n  },\n  created() {\n    this.getStoreOperateTypes()\n    this.getOperators()\n    this.getGoodsCategoryClassificationWithCategory()\n    this.getFiveLevelStores()\n    this.getGoodsStorageModes()\n    this.getPath()\n  },\n  methods: {\n    getPath() {\n      let goods = ''\n      if (this.$route.query.goods != null) {\n        goods = this.$route.query.goods\n        const temp = { goodsCode: '', quality: '' }\n        temp.quality = goods.quality\n        temp.goodsCode = goods.goodsBarCode !== null ? goods.goodsBarCode : goods.goodsRfidCode\n        this.out.models.outGoods.push(temp)\n      }\n    },\n    getStoreOperateTypes() {\n      // const ioType = 1\n      storeOperateType.list().then(response => {\n        this.storeOperateTypes = response.data\n      }).catch(reject => {\n      })\n    },\n    getDatas() {\n      this.loading.list = true\n      const status = 0\n      goods.getList(this.query, this.page, this.sort, status).then(response => {\n        this.datas = response.data.items\n        this.page.total = response.data.total\n        this.loading.list = false\n        for (let i = 0; i < this.page.total; i++) {\n          this.datas[i].quality = this.datas[i].quality.toString() + this.datas[i].goodsCategory.unitName\n          this.datas[i].qualityLocked = this.datas[i].qualityLocked.toString() + this.datas[i].goodsCategory.unitName\n        }\n        this.loading.list = false\n      }).catch(reject => {\n        this.loading.list = false\n      })\n    },\n    getOperators() {\n      operator.getlist().then(response => {\n        this.operators = response.data\n      }).catch(reject => {\n      })\n    },\n    getGoodsCategoryClassificationWithCategory() {\n      goodsCategoryClassification.getlist(2).then(response => {\n        this.goodsCategoryClassificationWithCategory = response.data\n      }).catch(reject => {\n      })\n    },\n    getFiveLevelStores() {\n      store.getFiveLevelStores().then(response => {\n        this.fiveLevelStores = response.data\n      }).catch(reject => {\n      })\n    },\n    getGoodsStorageModes() {\n      goodsStorageMode.getlist().then(response => {\n        this.goodsStorageModes = response.data\n      }).catch(reject => {\n      })\n    },\n    outData() {\n      this.$refs['formOut'].validate((valid) => {\n        if (!valid) return false\n        goods.preOutput(this.out.models).then(response => {\n          // 为方便连续添加，create对话框不关闭\n          // 需要清空的表单，手动清空，至少清空一个必填项，防止点击两遍\n          this.out.models.requestDetailCode = ''\n          this.out.models.outGoods = [{ goodsCode: null, quality: null }]\n          this.out.models.storeOperateTypeId = ''\n          // 重新获取数据\n          this.getDatas()\n        }).catch(reject => {\n        })\n      })\n    },\n    handleCreate(outGood) {\n      this.create.dialog.visible = true\n      this.outGoodModel = outGood\n      this.getDatas()\n    },\n    addDomain() {\n      this.out.models.outGoods.push({\n        goodsCode: '',\n        quality: ''\n      })\n    },\n    removeDomain(item) {\n      if (this.out.models.outGoods.length > -1) {\n        var index = this.out.models.outGoods.indexOf(item)\n        if (index !== -1) {\n          this.out.models.outGoods.splice(index, 1)\n        }\n      }\n    },\n    handleQuery() {\n      this.page.current = 1\n      this.getDatas()\n    },\n    handleSort(data) {\n      this.sort.prop = data.prop\n      this.sort.order = data.order\n      this.getDatas()\n    },\n    handleStoreChange(value) {\n      const id = value[value.length - 1]\n      this.query.multiLevelStoreId = id\n    },\n    handleCategoryChange(value) {\n      const id = value[value.length - 1]\n      this.query.goodsCategoryId = id\n    },\n    handleCreates() {\n      // 提示选择货物\n      if (this.$refs.listTable.selection.length === 0) {\n        this.$message({ type: 'warning', message: '请在下面表格的左侧勾选需要出库的货物', duration: 4000 })\n        return false\n      }\n      // if (this.out.models.outGoods.length !== 0) {\n      //   this.out.models.outGoods.pop()\n      // }\n      // 清除选中对应位置的货物\n      var index = this.out.models.outGoods.indexOf(this.outGoodModel)\n      if (index !== -1) {\n        this.out.models.outGoods.splice(index, 1)\n      }\n      // 添加货物并判重\n      for (const item of this.$refs.listTable.selection) {\n        var outGood = { goodsCode: null, quality: null }\n        outGood.goodsCode = item.goodsBarCode !== null ? item.goodsBarCode : item.goodsRfidCode\n        outGood.quality = parseInt(item.quality)\n        for (let i = 0; i < this.out.models.outGoods.length; i++) {\n          if (this.out.models.outGoods[i].goodsCode === outGood.goodsCode) {\n            this.$message({ type: 'warning', message: '选择货物存在重复，请重新选择', duration: 4000 })\n            return false\n          }\n        }\n        this.out.models.outGoods.push(outGood)\n      }\n      this.create.dialog.visible = false\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n  /*.app-container{*/\n  /*  width: 40%;*/\n  /*  margin: 150px auto;*/\n  /*  overflow: auto;*/\n  /*}*/\n</style>\n<style lang=\"scss\" scoped>\n  .app-container {\n    height: 100%;\n    overflow: auto;\n  }\n  .unit {\n    position: relative;\n    margin: -1px 750px 0 150px;\n    border: 1px dashed #ccc;\n    padding-bottom: 8px;\n    padding-top: 8px;\n\n    .remove-unit {\n      position: absolute;\n      top: 10px;\n      right: 5px;\n    }\n  }\n  .search-unit {\n    position: relative;\n    top: 10px;\n    left: 4px;\n    /*right: 1000px;*/\n  }\n</style>\n"]}]}