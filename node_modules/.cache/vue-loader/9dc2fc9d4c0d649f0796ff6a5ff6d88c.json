   <el-form-item label=\"验货结果\" prop=\"inspectionResult\">\n        <el-radio-group v-model=\"check.models.inspectionResult\">\n          <el-radio :label=\"0\">未通过</el-radio>\n          <el-radio :label=\"1\">通过</el-radio>\n        </el-radio-group>\n      </el-form-item>\n      <el-form-item>\n        <el-button type=\"primary\" @click=\"checkData()\">验货完成</el-button>\n      </el-form-item>\n    </el-form>\n  </div>\n</template>\n\n<script>\nimport adaptive from '@/directive/el-table'\nimport setRule from '@/utils/form-validate'\nimport * as goods from '@/api/goods-manage/goods'\n\nexport default {\n  name: 'GoodsCheck',\n  // components: { Pagination },\n  directives: { adaptive },\n  data() {\n    return {\n      check: {\n        dialog: { title: '货物验货', visible: false, labelWidth: '150px' },\n        models: { goodsCode: null, inspectionResult: null },\n        rules: {\n          goodsCode: setRule('货物的RFID码或条形码', [{ required: true }, { length: [0, 255] }]),\n          inspectionResult: setRule('验货结果', [{ selected: true }])\n        }\n      },\n      loading: { list: true, export: false, detail: false, update: false }\n    }\n  },\n  created() {\n\n  },\n  methods: {\n    checkData() {\n      this.$refs['formCheck'].validate((valid) => {\n        if (!valid) return false\n        goods.check(this.check.models).then(response => {\n          // 为方便连续添加，create对话框不关闭\n          // 需要清空的表单，手动清空，至少清空一个必填项，防止点击两遍\n          this.check.models.goodsCode = ''\n          // 重新获取数据\n          this.getDatas()\n        }).catch(reject => {\n        })\n      })\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n  .app-container{\n    width: 40%;\n    margin: 150px auto;\n  }\n</style>\n"]}]}