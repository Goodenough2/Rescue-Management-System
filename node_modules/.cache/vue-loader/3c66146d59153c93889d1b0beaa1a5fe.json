ACA;AACA;AACA;AACA;AACA;AACA","file":"goods-input.vue","sourceRoot":"src/views/business-operation","sourcesContent":["<template>\n  <div class=\"app-container list\">\n    <el-form ref=\"formCreate\" :rules=\"create.rules\" :model=\"create.models\" label-position=\"right\" :label-width=\"create.dialog.labelWidth\">\n      <el-form-item label=\"操作类型\" prop=\"storeOperateTypeId\">\n        <el-select v-model=\"create.models.storeOperateTypeId\" clearable>\n          <el-option v-for=\"item in storeOperateTypes\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n        </el-select>\n      </el-form-item>\n      <el-form-item label=\"货物类型\" prop=\"goods.goodsCategoryId\">\n        <el-select v-model=\"create.models.goods.goodsCategoryId\" filterable placeholder=\"请选择\">\n          <el-option v-for=\"item in goodsCategories\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n        </el-select>\n        <el-tooltip class=\"item\" effect=\"dark\" content=\"货物类别\" placement=\"top-end\">\n          <el-button type=\"primary\" plain class=\"button-operate button-detail\" circle icon=\"el-icon-edit\" @click=\"handleCreateCategories()\" />\n        </el-tooltip>\n      </el-form-item>\n      <el-form-item label=\"入库数量\" prop=\"goods.quality\">\n        <el-input v-model.number=\"create.models.goods.quality\" type=\"number\" />\n      </el-form-item>\n      <el-form-item label=\"点击添加\">\n        <el-button @click=\"addDomain\">新增存储模式</el-button>\n      </el-form-item>\n      <div v-for=\"(goodsStorageMode, index) in create.models.goodsStorageModes\" :key=\"index\">\n        <el-form-item :key=\"goodsStorageMode.key\" :label=\"(index + 1) + '级货物单位' \" :prop=\"'goodsStorageModes.' + index + '.unitId'\" :rules=\"{ required: true, message: '请选择货物单位', trigger: 'blur' }\">\n          <el-select v-model=\"goodsStorageMode.unitId\" clearable>\n            <el-option v-for=\"item in goodsUnits\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n          </el-select>\n          <el-tooltip class=\"item\" effect=\"dark\" content=\"删除货物存储模式\" placement=\"top-end\">\n            <el-button type=\"primary\" plain icon=\"el-icon-minus\" circle @click.prevent=\"removeDomain(goodsStorageMode)\" />\n          </el-tooltip>\n        </el-form-item>\n        <el-form-item :key=\"goodsStorageMode.key\" :label=\" '名称' \" :prop=\"'goodsStorageModes.' + index + '.name'\" :rules=\"[{ required: true, message: '请输入存储模式名称', trigger: 'blur' }]\">\n          <el-input v-model=\"goodsStorageMode.name\" />\n        </el-form-item>\n        <el-form-item :key=\"goodsStorageMode.key\" :label=\" '换算系数' \" :prop=\"'goodsStorageModes.' + index + '.ratio'\" :rules=\"{required: true, message: '请输入存储模式换算系数', trigger: 'blur'}\">\n          <el-input v-model=\"goodsStorageMode.ratio\" type=\"number\" />\n        </el-form-item>\n        <el-form-item :key=\"goodsStorageMode.key\" :label=\" '重量/kg:' \" :prop=\"'goodsStorageModes.' + index + '.weight'\" :rules=\"{ required: true, message: '请输入存储模式重量', trigger: 'blur' }\">\n          <el-input v-model=\"goodsStorageMode.weight\" type=\"number\" />\n        </el-form-item>\n        <el-form-item :key=\"goodsStorageMode.key\" :label=\" '长度/cm:' \" :prop=\"'goodsStorageModes.' + index + '.length'\" :rules=\"{ required: true, message: '请输入存储模式长度', trigger: 'blur' }\">\n          <el-input v-model=\"goodsStorageMode.length\" type=\"number\" />\n        </el-form-item>\n        <el-form-item :key=\"goodsStorageMode.key\" :label=\" '宽度/cm:' \" :prop=\"'goodsStorageModes.' + index + '.width'\" :rules=\"{ required: true, message: '请输入存储模式宽度', trigger: 'blur' }\">\n          <el-input v-model=\"goodsStorageMode.width\" type=\"number\" />\n        </el-form-item>\n        <el-form-item :key=\"goodsStorageMode.key\" :label=\" '高度/cm:' \" :prop=\"'goodsStorageModes.' + index + '.height'\" :rules=\"{ required: true, message: '请输入存储模式高度', trigger: 'blur' }\">\n          <el-input v-model=\"goodsStorageMode.height\" type=\"number\" />\n        </el-form-item>\n      </div>\n      <el-form-item label=\"目的位置条码\" prop=\"locationCode\">\n        <el-input v-model=\"create.models.locationCode\" />\n      </el-form-item>\n      <el-form-item label=\"入库备注说明\" prop=\"description\">\n        <el-input v-model=\"create.models.description\" />\n      </el-form-item>\n      <el-form-item label=\"货物条形码\" prop=\"goods.goodsBarCode\">\n        <el-input v-model=\"create.models.goods.goodsBarCode\" />\n      </el-form-item>\n      <el-form-item label=\"货物RFID\" prop=\"goods.goodsRfidCode\">\n        <el-input v-model=\"create.models.goods.goodsRfidCode\" />\n      </el-form-item>\n      <el-form-item label=\"送货人信息\" prop=\"deliveryman\">\n        <el-input v-model=\"create.models.deliveryman\" />\n      </el-form-item>\n      <el-form-item>\n        <el-button type=\"primary\" @click=\"createData()\">入库</el-button>\n      </el-form-item>\n    </el-form>\n  </div>\n</template>\n\n<script>\nimport adaptive from '@/directive/el-table'\nimport setRule from '@/utils/form-validate'\nimport * as goods from '@/api/goods-manage/goods'\nimport * as goodsCategory from '@/api/goods-manage/goods-category'\nimport * as goodsUnit from '@/api/goods-manage/goods-unit'\nimport * as operator from '@/api/system-manage/user'\nimport * as cell from '@/api/store-manage/cell'\nimport * as device from '@/api/store-manage/device'\nimport * as vessel from '@/api/store-manage/vessel'\nimport * as storeOperateType from '@/api/log-manage/store-operate-type'\n\nexport default {\n  name: 'GoodsInput',\n  // components: { Pagination },\n  directives: { adaptive },\n  data() {\n    return {\n      goodsCategories: null,\n      operators: null,\n      goodsUnits: null,\n      storeOperateTypes: null,\n      cells: null,\n      devices: null,\n      vessels: null,\n      create: {\n        dialog: { title: '货物入库', visible: false, labelWidth: '100px' },\n        models: {\n          goods: {\n            goodsCategoryId: null,\n            quality: null,\n            inspectorId: null,\n            goodsBarCode: null,\n            goodsRfidCode: null\n          },\n          storeOperateTypeId: null,\n          description: null,\n          deliveryman: null,\n          locationCode: null,\n          goodsStorageModes: [\n            { ratio: null, unitId: null, name: null, weight: null, length: null, width: null, height: null }\n          ]\n        },\n        rules: {\n          storeOperateTypeId: setRule('操作类型', [{ selected: true }]),\n          locationCode: setRule('单元格条码', [{ required: true }, { length: [0, 255] }]),\n          goods: {\n            goodsCategoryId: setRule('货物种类', [{ selected: true }]),\n            storageModeId: setRule('货物存储模式', [{ selected: true }]),\n            quality: setRule('货物数量', [{ required: true }]),\n            goodsBarCode: setRule('货物条形码', [{ length: [0, 255] }]),\n            goodsRfidCode: setRule('货物RFID', [{ length: [0, 255] }])\n          },\n          description: setRule('入库备注描述', [{ length: [0, 255] }]),\n          deliveryman: setRule('送货人信息', [{ required: true }, { length: [0, 50] }])\n        }\n      },\n      loading: { list: true, export: false, detail: false, update: false }\n    }\n  },\n  created() {\n    this.getGoodsCategories()\n    this.getOperators()\n    this.getGoodsUnits()\n    this.getCells()\n    this.getStoreOperateTypes()\n  },\n  methods: {\n    getGoodsCategories() {\n      goodsCategory.getlist().then(response => {\n        this.goodsCategories = response.data\n      }).catch(reject => {\n      })\n    },\n    getOperators() {\n      operator.getlist().then(response => {\n        this.operators = response.data\n      }).catch(reject => {\n      })\n    },\n    getCells() {\n      cell.getlist().then(response => {\n        this.cells = response.data\n      }).catch(reject => {\n      })\n    },\n    getVessels() {\n      vessel.getlist().then(response => {\n        this.vessels = response.data\n      }).catch(reject => {\n      })\n    },\n    getGoodsUnits() {\n      goodsUnit.getlist().then(response => {\n        this.goodsUnits = response.data\n      }).catch(reject => {\n      })\n    },\n    getStoreOperateTypes() {\n      const ioType = 0\n      storeOperateType.list(ioType).then(response => {\n        this.storeOperateTypes = response.data\n      }).catch(reject => {\n      })\n    },\n    getDevices() {\n      device.getlist().then(response => {\n        this.devices = response.data\n      }).catch(reject => {\n      })\n    },\n    handleOperate(row) {\n      const goodsCode = row.goodsRfidCode\n      this.$router.push({\n        path: '/business-operation/business-operation',\n        query: {\n          goodsCode: goodsCode\n        }\n      })\n    },\n    addDomain() {\n      this.create.models.goodsStorageModes.push({\n        ratio: '',\n        unitId: ''\n      })\n    },\n    removeDomain(item) {\n      if (this.create.models.goodsStorageModes.length > 1) {\n        var index = this.create.models.goodsStorageModes.indexOf(item)\n        if (index !== -1) {\n          this.create.models.goodsStorageModes.splice(index, 1)\n        }\n      }\n    },\n    handleCreateCategories() {\n      this.$router.push({\n        path: '/goods-manage/goods-category',\n        query: { flag: true }\n      })\n    },\n    handleCreate() {\n      this.create.dialog.visible = true\n    },\n    createData() {\n      this.$refs['formCreate'].validate((valid) => {\n        if (!valid) return false\n        goods.create(this.create.models).then(response => {\n          // 为方便连续添加，create对话框不关闭\n          // 需要清空的表单，手动清空，至少清空一个必填项，防止点击两遍\n          this.create.models.goods.goodsCategoryId = ''\n          this.create.models.goods.quality = ''\n          this.create.models.goods.goodsBarCode = ''\n          this.create.models.goods.goodsRfidCode = ''\n          // 重新获取数据\n          this.getDatas()\n        }).catch(reject => {\n        })\n      })\n    },\n    handleUpdate(row) {\n      // 若列表数据展示了全部属性，则可直接拷贝列表数据用于编辑\n      // this.update.models = Object.assign({}, row)\n      this.update.dialog.visible = true\n      this.loading.update = true\n      goods.get(row.id).then(response => {\n        this.update.models = response.data\n        this.loading.update = false\n      }).catch(reject => {\n        this.loading.update = false\n      })\n      this.$nextTick(() => {\n        this.$refs['formUpdate'].clearValidate()\n      })\n    },\n    updateData() {\n      this.$refs['formUpdate'].validate((valid) => {\n        if (!valid) return false\n        const tempData = Object.assign({}, this.update.models)\n        goods.update(tempData).then(response => {\n          // 重新获取数据\n          this.getDatas()\n          this.update.dialog.visible = false\n        }).catch(reject => {\n        })\n      })\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n  .app-container{\n    width: 60%;\n    height: 100%;\n    overflow: auto;\n    margin: 30px auto;\n  }\n</style>\n"]}]}