>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物规格\">\n              {{ detail.models.specification }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物计量单位\">\n              {{ detail.models.goodsUnit.name }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物颜色\">\n              {{ detail.models.goodsColour.name }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物单价\">\n              {{ detail.models.unitPrice }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物重量\">\n              {{ detail.models.unitWeight }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"供应商\">\n              {{ detail.models.supplier.name }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物类别描述\">\n              {{ detail.models.description }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n      </el-form>\n    </el-dialog>\n\n    <el-dialog custom-class=\"dialog-create\" :title=\"create.dialog.title\" :visible.sync=\"create.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\" :close-on-click-modal=\"false\">\n      <el-form ref=\"formCreate\" :rules=\"create.rules\" :model=\"create.models\" label-position=\"right\" :label-width=\"create.dialog.labelWidth\">\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物类别编号\" prop=\"code\">\n              <el-input v-model=\"create.models.code\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物类别名称\" prop=\"name\">\n              <el-input v-model=\"create.models.name\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物类别分类\" prop=\"classificationId\">\n              <el-cascader\n                v-model=\"create.models.classificationId\"\n                :options=\"goodsCategoryClassifications\"\n                :props=\"{ expandTrigger: 'hover', checkStrictly: true }\"\n                @change=\"handleChange\"\n              />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物批次\" prop=\"batch\">\n              <el-input v-model=\"create.models.batch\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物规格\" prop=\"specification\">\n              <el-input v-model=\"create.models.specification\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物计量单位\" prop=\"unitId\">\n              <el-select v-model=\"create.models.unitId\" filterable placeholder=\"请选择\">\n                <el-option v-for=\"item in goodsUnits\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n              </el-select>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物颜色\" prop=\"colourId\">\n              <el-select v-model=\"create.models.colourId\" filterable placeholder=\"请选择\">\n                <el-option v-for=\"item in goodsColours\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n              </el-select>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物单价\" prop=\"unitPrice\">\n              <el-input v-model=\"create.models.unitPrice\" type=\"number\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物重量\" prop=\"unitWeight\">\n              <el-input v-model=\"create.models.unitWeight\" type=\"number\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"供应商\" prop=\"supplierId\">\n              <el-select v-model=\"create.models.supplierId\" clearable>\n                <el-option v-for=\"item in suppliers\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n              </el-select>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物类别描述\" prop=\"description\">\n              <el-input v-model=\"create.models.description\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"create.dialog.visible = false\">取消</el-button>\n        <el-button type=\"primary\" @click=\"createData()\">提交</el-button>\n      </div>\n    </el-dialog>\n\n    <el-dialog v-loading=\"loading.update\" custom-class=\"dialog-update\" :title=\"update.dialog.title\" :visible.sync=\"update.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\" :close-on-click-modal=\"false\">\n      <el-form ref=\"formUpdate\" :rules=\"update.rules\" :model=\"update.models\" label-position=\"right\" :label-width=\"update.dialog.labelWidth\">\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物类别编号\" prop=\"code\">\n              <el-input v-model=\"update.models.code\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物类别名称\" prop=\"name\">\n              <el-input v-model=\"update.models.name\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物类别分类\" prop=\"classificationId\">\n              <el-cascader\n                v-model=\"update.models.classificationId\"\n                :options=\"goodsCategoryClassifications\"\n                :props=\"{ expandTrigger: 'hover', checkStrictly: true }\"\n                @change=\"handleChange\"\n              />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物批次\" prop=\"batch\">\n              <el-input v-model=\"update.models.batch\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物规格\" prop=\"specification\">\n              <el-input v-model=\"update.models.specification\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物计量单位\" prop=\"unitId\">\n              <el-select v-model=\"update.models.unitId\" clearable>\n                <el-option v-for=\"item in goodsUnits\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n              </el-select>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物颜色\" prop=\"colourId\">\n              <el-select v-model=\"update.models.colourId\" clearable>\n                <el-option v-for=\"item in goodsColours\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n              </el-select>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物单价\" prop=\"unitPrice\">\n              <el-input v-model=\"update.models.unitPrice\" type=\"number\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物重量\" prop=\"unitWeight\">\n              <el-input v-model=\"update.models.unitWeight\" type=\"number\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"供应商\" prop=\"supplierId\">\n              <el-select v-model=\"update.models.supplierId\" clearable>\n                <el-option v-for=\"item in suppliers\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n              </el-select>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"货物类别描述\" prop=\"description\">\n              <el-input v-model=\"update.models.description\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"update.dialog.visible = false\">取消</el-button>\n        <el-button type=\"primary\" @click=\"updateData()\">提交</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport adaptive from '@/directive/el-table'\nimport setRule from '@/utils/form-validate'\nimport Pagination from '@/components/Pagination'\nimport * as goodsColour from '@/api/goods-manage/goods-color'\nimport * as supplier from '@/api/order-manage/supplier'\nimport * as goodsCategory from '@/api/goods-manage/goods-category'\nimport * as goodsCategoryClassification from '@/api/goods-manage/goods-category-classification'\nimport * as goodsUnit from '@/api/goods-manage/goods-unit'\n\nexport default {\n  name: 'GoodsType',\n  components: { Pagination },\n  directives: { adaptive },\n  data() {\n    return {\n      access: this.$store.getters.access['GoodsManage']['GoodsCategory'],\n      datas: null,\n      goodsCategoryClassifications: null,\n      goodsColours: null,\n      goodcolor: null,\n      suppliers: null,\n      goodsUnits: null,\n      query: { code: null, name: null, classificationId: null, batch: null, colourId: null, supplierId: null, unitId: null },\n      page: { total: 0, current: 1, size: 20 },\n      sort: { prop: 'sort', order: 'ascending' },\n      detail: {\n        dialog: { title: '货物类别分类信息', visible: false, labelWidth: '150px' },\n        models: { code: null, name: null, goodsCategoryClassification: { name: null }, batch: null, specification: null, goodsUnit: { name: null }, goodsColour: { name: null }, unitPrice: null, unitWeight: null, supplier: { name: null }, description: null }\n      },\n      create: {\n        dialog: { title: '添加货物类别分类', visible: false, labelWidth: '150px' },\n        models: { code: null, name: null, level: null, parentId: null, description: null, classificationId: null },\n        rules: {\n          code: setRule('货物类别编号', [{ required: true }, { length: [0, 30] }]),\n          name: setRule('货物类别名称', [{ required: true }, { length: [0, 50] }]),\n          classificationId: setRule('货物类别分类', [{ selected: true }]),\n          batch: setRule('货物批次', [{ required: true }, { length: [0, 30] }]),\n          specification: setRule('货物规格', [{ length: [0, 30] }]),\n          unitId: setRule('货物基础单位', [{ selected: true }]),\n          // colourId: setRule('货物颜色', [{ selected: true }]),\n          // unitPrice: setRule('货物单价', [{ required: true }, { length: [0, 50] }]),\n          // unitWeight: setRule('货物单价', [{ required: true }, { length: [0, 50] }]),\n          // supplierId: setRule('供应商', [{ selected: true }]),\n          description: setRule('货物类别描述', [{ length: [0, 100] }])\n        }\n      },\n      update: {\n        dialog: { title: '编辑类别信息', visible: false, labelWidth: '130px' },\n        models: { code: null, name: null, level: null, parentId: null, description: null, classificationId: null },\n        rules: {\n          code: setRule('货物类别编号', [{ required: true }, { length: [0, 30] }]),\n          name: setRule('货物类别名称', [{ required: true }, { length: [0, 50] }]),\n          classificationId: setRule('货物类别分类', [{ selected: true }]),\n          batch: setRule('货物批次', [{ required: true }, { length: [0, 30] }]),\n          specification: setRule('货物规格', [{ length: [0, 30] }]),\n          unitId: setRule('货物基础单位', [{ selected: true }]),\n          // colourId: setRule('货物颜色', [{ selected: true }]),\n          // unitPrice: setRule('货物单价', [{ required: true }, { length: [0, 50] }]),\n          // unitWeight: setRule('货物单价', [{ required: true }, { length: [0, 50] }]),\n          // supplierId: setRule('供应商', [{ selected: true }]),\n          description: setRule('货物类别描述', [{ length: [0, 100] }])\n        }\n      },\n      loading: { list: true, export: false, detail: false, update: false },\n      sortOrders: ['ascending', 'descending']\n    }\n  },\n  created() {\n    this.getDatas()\n    this.getClassifications()\n    this.getColours()\n    this.getSuppliers()\n    this.getGoodsUnits()\n  },\n  mounted() {\n    const flag = this.$route.query.flag\n    this.handleCreateCategories(flag)\n  },\n  methods: {\n    getDatas() {\n      this.loading.list = true\n      goodsCategory.getList(this.query, this.page, this.sort).then(response => {\n        this.datas = response.data.items\n        this.page.total = response.data.total\n        this.loading.list = false\n      }).catch(reject => {\n        this.loading.list = false\n      })\n    },\n    getClassifications() {\n      goodsCategoryClassification.getlist(3).then(response => {\n        this.goodsCategoryClassifications = response.data\n      }).catch(reject => {\n      })\n    },\n    getColours() {\n      goodsColour.getlist().then(response => {\n        this.goodsColours = response.data\n      }).catch(reject => {\n      })\n    },\n    getSuppliers() {\n      supplier.getlist().then(response => {\n        this.suppliers = response.data\n      }).catch(reject => {\n      })\n    },\n    getGoodsUnits() {\n      goodsUnit.getlist().then(response => {\n        this.goodsUnits = response.data\n      }).catch(reject => {\n      })\n    },\n    handleCreateCategories(flag) {\n      if (flag === true) {\n        this.handleCreate()\n      }\n    },\n    handleQuery() {\n      this.page.current = 1\n      this.getDatas()\n    },\n    handleSort(data) {\n      this.sort.prop = data.prop\n      this.sort.order = data.order\n      this.getDatas()\n    },\n    handleChange(value) {\n      const id = value[value.length - 1]\n      this.create.models.classificationId = id\n    },\n    handleDetail(row) {\n      // 若列表数据展示了全部属性，则可直接拷贝列表数据\n      this.detail.models = Object.assign({}, row)\n      this.detail.dialog.visible = true\n      this.loading.detail = true\n      // goodsCategoryClassification.get(row.id).then(response => {\n      //   this.detail.models = response.data\n      //   this.loading.detail = false\n      // }).catch(reject => {\n      //   this.loading.detail = false\n      // })\n      this.loading.detail = false\n    },\n    handleCreate() {\n      this.create.dialog.visible = true\n    },\n    createData() {\n      this.$refs['formCreate'].validate((valid) => {\n        if (!valid) return false\n        goodsCategory.create(this.create.models).then(response => {\n          // 为方便连续添加，create对话框不关闭\n          // 需要清空的表单，手动清空，至少清空一个必填项，防止点击两遍\n          this.create.models.name = ''\n          this.create.models.code = ''\n          this.create.models.unitPrice = ''\n          this.create.models.unitWeight = ''\n          // 重新获取数据\n          this.getDatas()\n        }).catch(reject => {\n        })\n      })\n    },\n    handleRankup(row) {\n      this.loading.list = true\n      goodsCategory.rankup(row.id).then(response => {\n        this.getDatas()\n        this.loading.list = false\n      }).catch(reject => {\n        this.loading.list = false\n      })\n    },\n    handleUpdate(row) {\n      //  若列表数据展示了全部属性，则可直接拷贝列表数据用于编辑\n      this.update.models = Object.assign({}, row)\n      this.update.dialog.visible = true\n      this.loading.update = true\n      // goodsType.get(row.id).then(response => {\n      //   this.update.models = response.data\n      //   this.loading.update = false\n      // }).catch(reject => {\n      //   this.loading.update = false\n      // })\n      this.$nextTick(() => {\n        this.$refs['formUpdate'].clearValidate()\n      })\n      this.loading.update = false\n    },\n    updateData() {\n      this.$refs['formUpdate'].validate((valid) => {\n        if (!valid) return false\n        const tempData = Object.assign({}, this.update.models)\n        goodsCategory.update(tempData).then(response => {\n          // 重新获取数据\n          this.getDatas()\n          this.update.dialog.visible = false\n        }).catch(reject => {\n        })\n      })\n    },\n    handleDelete(row) {\n      this.deleteData([row.id])\n    },\n    handleDeletes(row) {\n      if (this.$refs.listTable.selection.length === 0) {\n        this.$message({ type: 'warning', message: '请在下面表格的左侧勾选需要删除的数据', duration: 4000 })\n        return false\n      }\n      const ids = []\n      for (const item of this.$refs.listTable.selection) {\n        ids.push(item.id)\n      }\n      this.deleteData(ids)\n    },\n    deleteData(ids) {\n      this.$confirm('数据删除后，不可恢复, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        goodsCategory.del(ids).then(response => {\n          // 重新获取数据\n          this.getDatas()\n        }).catch(reject => {\n        })\n      })\n    },\n    createFilter(queryString) {\n      return (color) => {\n        return (color.value.toLowerCase().indexOf(queryString.toLowerCase()) === 0)\n      }\n    },\n    querySearch(queryString, cb) {\n      var color = this.goodsColours\n      var results = queryString ? color.filter(this.createFilter(queryString)) : color\n      // 调用 callback 返回建议列表的数据\n      cb(results)\n    },\n    handleExport() {\n      this.loading.export = true\n      goodsCategory.exportExcel(this.query, this.page, this.sort).then(response => {\n        const blob = new Blob([response])\n        const fileName = '货物类别.xls'\n        const elink = document.createElement('a')\n        elink.onload = () => { URL.revokeObjectURL(elink.href) }\n        elink.download = fileName\n        elink.style.display = 'none'\n        elink.href = URL.createObjectURL(blob)\n        document.body.appendChild(elink)\n        elink.click()\n        document.body.removeChild(elink)\n        this.loading.export = false\n      }).catch(reject => {\n        this.loading.export = false\n      })\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n</style>\n"]}]}