el-input v-model.trim=\"query.goodsBarCode\" class=\"query-item\" style=\"width: 150px\" placeholder=\"货物条形码\" clearable @clear=\"handleQuery\" />\r\n      <el-input v-model.trim=\"query.quality\" class=\"query-item\" style=\"width: 120px\" placeholder=\"锁定数量\" clearable @clear=\"handleQuery\" />\r\n      <el-select v-model=\"query.operatorId\" class=\"query-item\" style=\"width: 200px\" placeholder=\"操作人员\" clearable @clear=\"handleQuery\">\r\n        <el-option v-for=\"item in operators\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\r\n      </el-select>\r\n      <el-select v-model=\"query.lockType\" class=\"query-item\" style=\"width: 120px\" placeholder=\"锁定类型\" clearable @clear=\"handleQuery\">\r\n        <el-option value=\"0\" label=\"正常锁定\" />\r\n        <el-option value=\"1\" label=\"临时锁定\" />\r\n      </el-select>\r\n      <el-button class=\"tool tool-query\" type=\"primary\" icon=\"el-icon-search\" @click=\"handleQuery\">查询</el-button>\r\n      <el-button v-if=\"access.export.allow\" class=\"tool tool-export\" :loading=\"loading.export\" type=\"primary\" icon=\"vue-icon-excel\" @click=\"handleExport\">导出</el-button>\r\n    </div>\r\n\r\n    <el-table ref=\"listTable\" v-loading=\"loading.list\" v-adaptive=\"{ bottomOffset: 55 }\" height=\"200px\" :data=\"datas\" :default-sort=\"sort\" border fit highlight-current-row @sort-change=\"handleSort\">\r\n      <el-table-column type=\"selection\" align=\"center\" width=\"35\" />\r\n      <el-table-column label=\"序号\" type=\"index\" align=\"center\" width=\"65\" fixed>\r\n        <template slot-scope=\"scope\">\r\n          <span>{{ (page.current - 1) * page.size + scope.$index + 1 }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"货物RFID码\" prop=\"goods.goodsRfidCode\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"货物条形码\" prop=\"goods.goodsBarCode\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <!--<el-table-column label=\"货物名称\" prop=\"goods.goodsCategory.name\" :sort-orders=\"sortOrders\" align=\"center\" width=\"120\" show-overflow-tooltip />-->\r\n      <!--<el-table-column label=\"货物规格\" prop=\"goods.goodsCategory.specification\" :sort-orders=\"sortOrders\" width=\"120\" align=\"center\" show-overflow-tooltip />-->\r\n      <!--<el-table-column label=\"商品批次\" prop=\"goods.goodsCategory.batch\" :sort-orders=\"sortOrders\" width=\"120\" align=\"center\" show-overflow-tooltip />-->\r\n      <!--<el-table-column label=\"存储方式名称\" prop=\"goods.goodsStorageMode.name\" :sort-orders=\"sortOrders\" width=\"120\" align=\"center\" show-overflow-tooltip />-->\r\n      <el-table-column label=\"货物锁定数量\" prop=\"quality\" :sort-orders=\"sortOrders\" width=\"120\" align=\"center\" show-overflow-tooltip />\r\n      <el-table-column label=\"锁定订单类型\" prop=\"lockType\" :sort-orders=\"sortOrders\" width=\"120\" align=\"center\" show-overflow-tooltip>\r\n        <template slot-scope=\"{row}\">\r\n          <span v-if=\"row.lockType==0\">正常锁定</span>\r\n          <span v-if=\"row.lockType==1\">临时锁定</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"锁定原因\" prop=\"reason\" :sort-orders=\"sortOrders\" width=\"120\" align=\"center\" show-overflow-tooltip />\r\n      <el-table-column label=\"锁定时间\" prop=\"lockTime\" :sort-orders=\"sortOrders\" width=\"220\" align=\"center\" show-overflow-tooltip />\r\n      <el-table-column label=\"操作人员名称\" prop=\"operator.userName\" :sort-orders=\"sortOrders\" align=\"center\" show-overflow-tooltip />\r\n      <el-table-column fixed=\"right\" label=\"操作\" width=\"60px\">\r\n        <template slot-scope=\"{row}\">\r\n          <el-tooltip v-if=\"access.detail.allow\" class=\"item\" effect=\"dark\" content=\"详情\" placement=\"top-end\">\r\n            <el-button type=\"primary\" plain class=\"button-operate button-detail\" size=\"mini\" @click=\"handleDetail(row)\"><i class=\"vue-icon-detail\" /></el-button>\r\n          </el-tooltip>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <pagination :hidden=\"page.total==0\" :total=\"page.total\" :page.sync=\"page.current\" :limit.sync=\"page.size\" @pagination=\"getDatas\" />\r\n\r\n    <el-dialog v-loading=\"loading.detail\" custom-class=\"dialog-fullscreen\" :title=\"detail.dialog.title\" :visible.sync=\"detail.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\">\r\n      <el-form ref=\"formDetail\" label-position=\"right\" :label-width=\"detail.dialog.labelWidth\">\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"货物的RFID码\">\r\n              {{ detail.models.goods.goodsRfidCode }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"货物的条形码\">\r\n              {{ detail.models.goods.goodsBarCode }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"锁定数量\">\r\n              {{ detail.models.quality }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"锁定类型\">\r\n              {{ detail.models.lockType===0?'正常锁定':'临时锁定' }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"客户订单\">\r\n              {{ detail.models.order.code }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"锁定原因\">\r\n              {{ detail.models.reason }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"锁定时间\">\r\n              {{ detail.models.lockTime }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"操作人员名称\">\r\n              {{ detail.models.operator.userName }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport adaptive from '@/directive/el-table'\r\nimport Pagination from '@/components/Pagination'\r\nimport * as goodsLock from '@/api/goods-manage/goods-lock'\r\nimport * as operator from '@/api/system-manage/user'\r\n\r\nexport default {\r\n  name: 'GoodsLock',\r\n  components: { Pagination },\r\n  directives: { adaptive },\r\n  data() {\r\n    return {\r\n      access: this.$store.getters.access['GoodsManage']['GoodsLock'],\r\n      datas: null,\r\n      operators: null,\r\n      goodsCategories: null,\r\n      lockType: [\r\n        { id: 0, name: '正常锁定' },\r\n        { id: 1, name: '临时锁定' }\r\n      ],\r\n      query: { goodsRfidCode: null, goodsBarCode: null, quality: null, lockType: null, operatorId: null },\r\n      page: { total: 0, current: 1, size: 20 },\r\n      sort: { prop: 'sort', order: 'ascending' },\r\n      detail: {\r\n        dialog: { title: '货物锁定信息', visible: false, labelWidth: '120px' },\r\n        models: {\r\n          goods: { goodsBarCode: null, goodsRfidCode: null },\r\n          quality: null,\r\n          lockType: null,\r\n          order: { code: null },\r\n          reason: null,\r\n          lockTime: null,\r\n          operator: { userName: null }\r\n        }\r\n      },\r\n      loading: { list: true, export: false, detail: false },\r\n      sortOrders: ['ascending', 'descending']\r\n    }\r\n  },\r\n  created() {\r\n    this.getDatas()\r\n    this.getOperators()\r\n  },\r\n  methods: {\r\n    handleSelect(item) {\r\n      goodsLock.getSelect(item.id).then(response => {\r\n        this.create.models.goods.batch = response.data.batch\r\n        this.create.models.goods.id = response.data.id\r\n        this.create.models.goods.specification = response.data.specification\r\n      })\r\n    },\r\n    getDatas() {\r\n      this.loading.list = true\r\n      goodsLock.getList(this.query, this.page, this.sort).then(response => {\r\n        console.log(response.data.items)\r\n        this.datas = response.data.items\r\n        this.page.total = response.data.total\r\n        this.loading.list = false\r\n      }).catch(reject => {\r\n        this.loading.list = false\r\n      })\r\n    },\r\n    getOperators() {\r\n      operator.getlist().then(response => {\r\n        this.operators = response.data\r\n      }).catch(reject => {\r\n      })\r\n    },\r\n    handleQuery() {\r\n      this.page.current = 1\r\n      this.getDatas()\r\n    },\r\n    handleSort(data) {\r\n      this.sort.prop = data.prop\r\n      this.sort.order = data.order\r\n      this.getDatas()\r\n    },\r\n    handleDetail(row) {\r\n      // 若列表数据展示了全部属性，则可直接拷贝列表数据\r\n      this.detail.models = Object.assign({}, row)\r\n      this.detail.dialog.visible = true\r\n      this.loading.detail = true\r\n      // goodsLocked.get(row.id).then(response => {\r\n      //   this.detail.models = response.data\r\n      //   this.loading.detail = false\r\n      // }).catch(reject => {\r\n      //   this.loading.detail = false\r\n      // })\r\n      this.loading.detail = false\r\n    },\r\n    handleExport() {\r\n      this.loading.export = true\r\n      goodsLock.exportExcel(this.query, this.page, this.sort).then(response => {\r\n        const blob = new Blob([response])\r\n        const fileName = '货物锁定.xls'\r\n        const elink = document.createElement('a')\r\n        elink.onload = () => { URL.revokeObjectURL(elink.href) }\r\n        elink.download = fileName\r\n        elink.style.display = 'none'\r\n        elink.href = URL.createObjectURL(blob)\r\n        document.body.appendChild(elink)\r\n        elink.click()\r\n        document.body.removeChild(elink)\r\n        this.loading.export = false\r\n      }).catch(reject => {\r\n        this.loading.export = false\r\n      })\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n</style>\r\n"]}]}