    <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"货柜名称\" prop=\"name\">\r\n              <el-input v-model=\"create.models.name\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"货柜类型\" prop=\"typeId\">\r\n              <el-select v-model=\"create.models.typeId\" clearable>\r\n                <el-option v-for=\"item in containerTypes\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"所属区域名称\" prop=\"areaId\">\r\n              <el-select v-model=\"create.models.areaId\" clearable>\r\n                <el-option v-for=\"item in areas\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"货柜RFID\" prop=\"rfidCode\">\r\n              <el-input v-model=\"create.models.rfidCode\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"货柜条形码\" prop=\"barCode\">\r\n              <el-input v-model=\"create.models.barCode\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"是否启用\" prop=\"enabled\">\r\n              <el-switch v-model=\"create.models.enabled\" :active-value=\"1\" :inactive-value=\"0\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n 