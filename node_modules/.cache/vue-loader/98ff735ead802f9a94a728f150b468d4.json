  </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"货柜RFID\" prop=\"rfidCode\">\r\n              <el-input v-model=\"update.models.rfidCode\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"货柜条形码\" prop=\"barCode\">\r\n              <el-input v-model=\"update.models.barCode\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"是否启用\" prop=\"enabled\">\r\n              <el-switch v-model=\"update.models.enabled\" :active-value=\"1\" :inactive-value=\"0\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"update.dialog.visible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"updateData()\">提交</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport adaptive from '@/directive/el-table'\r\nimport setRule from '@/utils/form-validate'\r\nimport Pagination from '@/components/Pagination'\r\nimport * as area from '@/api/store-manage/area'\r\nimport * as container from '@/api/store-manage/container'\r\nimport * as containerType from '@/api/store-manage/container-type'\r\n\r\nexport default {\r\n  name: 'Container',\r\n  components: { Pagination },\r\n  directives: { adaptive },\r\n  data() {\r\n    return {\r\n      access: this.$store.getters.access['StoreManage']['Container'],\r\n      datas: null,\r\n      areas: null,\r\n      containerTypes: null,\r\n      fileList: [],\r\n      query: { code: null, name: null, typeId: null, areaId: null, enabled: null },\r\n      page: { total: 0, current: 1, size: 20 },\r\n      sort: { prop: 'sort', order: 'ascending' },\r\n      detail: {\r\n        dialog: { title: '货柜信息', visible: false, labelWidth: '120px' },\r\n        models: { code: null, name: null, area: { name: null }, containerType: { name: null }, rfidCode: null, barCode: null, enabled: null }\r\n      },\r\n      create: {\r\n        dialog: { title: '添加货柜', visible: false, labelWidth: '120px' },\r\n        models: { code: null, name: null, typeId: null, areaId: null, rfidCode: null, barCode: null, enabled: 1 },\r\n        rules: {\r\n          code: setRule('货柜编号', [{ required: true }, { length: [0, 60] }]),\r\n          name: setRule('货柜名称', [{ required: true }, { length: [0, 50] }]),\r\n          typeId: setRule('货柜类型名称', [{ selected: true }]),\r\n          areaId: setRule('所属区域名称', [{ selected: true }]),\r\n          rfidCode: setRule('货柜RFID', [{ length: [0, 255] }]),\r\n          barCode: setRule('货柜条形码', [{ length: [0, 255] }])\r\n        }\r\n      },\r\n      update: {\r\n        dialog: { title: '编辑货柜信息', visible: false, labelWidth: '120px' },\r\n        models: { code: null, name: null, typeId: null, areaId: null, rfidCode: null, barCode: null, enabled: 1 },\r\n        rules: {\r\n          code: setRule('货柜编号', [{ required: true }, { length: [0, 60] }]),\r\n          name: setRule('货柜名称', [{ required: true }, { length: [0, 50] }]),\r\n          typeId: setRule('货柜类型名称', [{ selected: true }]),\r\n          areaId: setRule('所属区域名称', [{ selected: true }]),\r\n          rfidCode: setRule('货柜RFID', [{ length: [0, 255] }]),\r\n          barCode: setRule('货柜条形码', [{ length: [0, 255] }])\r\n        }\r\n      },\r\n      upload: {\r\n        dialog: { title: '导入文件', visible: false, labelWidth: '150px' }\r\n      },\r\n      loading: { list: true, export: false, detail: false, update: false, upload: false },\r\n      sortOrders: ['ascending', 'descending']\r\n    }\r\n  },\r\n  created() {\r\n    this.getDatas()\r\n    this.getContainerTypes()\r\n    this.getAreas()\r\n  },\r\n  methods: {\r\n    getDatas() {\r\n      this.loading.list = true\r\n      container.getList(this.query, this.page, this.sort).then(response => {\r\n        this.datas = response.data.items\r\n        this.page.total = response.data.total\r\n        this.loading.list = false\r\n      }).catch(reject => {\r\n        this.loading.list = false\r\n      })\r\n    },\r\n    getAreas() {\r\n      area.getlist().then(response => {\r\n        this.areas = response.data\r\n      }).catch(reject => {\r\n      })\r\n    },\r\n    getContainerTypes() {\r\n      containerType.getlist().then(response => {\r\n        this.containerTypes = response.data\r\n      }).catch(reject => {\r\n      })\r\n    },\r\n    handleQuery() {\r\n      this.page.current = 1\r\n      this.getDatas()\r\n    },\r\n    handleSort(data) {\r\n      this.sort.prop = data.prop\r\n      this.sort.order = data.order\r\n      this.getDatas()\r\n    },\r\n    handleDetail(row) {\r\n      // 若列表数据展示了全部属性，则可直接拷贝列表数据\r\n      this.detail.models = Object.assign({}, row)\r\n      this.detail.dialog.visible = true\r\n      this.loading.detail = true\r\n      // api.get(row.id).then(response => {\r\n      //   this.detail.models = response.data\r\n      //   this.loading.detail = false\r\n      // }).catch(reject => {\r\n      //   this.loading.detail = false\r\n      // })\r\n      this.loading.detail = false\r\n    },\r\n    handleCreate() {\r\n      this.create.dialog.visible = true\r\n    },\r\n    createData() {\r\n      this.$refs['formCreate'].validate((valid) => {\r\n        if (!valid) return false\r\n        container.create(this.create.models).then(response => {\r\n          // 为方便连续添加，create对话框不关闭\r\n          // 需要清空的表单，手动清空，至少清空一个必填项，防止点击两遍\r\n          this.create.models.code = ''\r\n          this.create.models.name = ''\r\n          // 重新获取数据\r\n          this.getDatas()\r\n        }).catch(reject => {\r\n        })\r\n      })\r\n    },\r\n    handleRankup(row) {\r\n      this.loading.list = true\r\n      container.rankup(row.id).then(response => {\r\n        this.getDatas()\r\n        this.loading.list = false\r\n      }).catch(reject => {\r\n        this.loading.list = false\r\n      })\r\n    },\r\n    handleUpdate(row) {\r\n      // 若列表数据展示了全部属性，则可直接拷贝列表数据用于编辑\r\n      this.update.models = Object.assign({}, row)\r\n      this.update.dialog.visible = true\r\n      this.loading.update = true\r\n      // api.get(row.id).then(response => {\r\n      //   this.update.models = response.data\r\n      //   this.loading.update = false\r\n      // }).catch(reject => {\r\n      //   this.loading.update = false\r\n      // })\r\n      // this.$nextTick(() => {\r\n      //   this.$refs['formUpdate'].clearValidate()\r\n      // })\r\n      this.loading.update = false\r\n    },\r\n    updateData() {\r\n      this.$refs['formUpdate'].validate((valid) => {\r\n        if (!valid) return false\r\n        const tempData = Object.assign({}, this.update.models)\r\n        container.update(tempData).then(response => {\r\n          // 重新获取数据\r\n          this.getDatas()\r\n          this.update.dialog.visible = false\r\n        }).catch(reject => {\r\n        })\r\n      })\r\n    },\r\n    handleDelete(row) {\r\n      this.deleteData([row.id])\r\n    },\r\n    handleDeletes(row) {\r\n      if (this.$refs.listTable.selection.length === 0) {\r\n        this.$message({ type: 'warning', message: '请在下面表格的左侧勾选需要删除的数据', duration: 4000 })\r\n        return false\r\n      }\r\n      const ids = []\r\n      for (const item of this.$refs.listTable.selection) {\r\n        ids.push(item.id)\r\n      }\r\n      this.deleteData(ids)\r\n    },\r\n    deleteData(ids) {\r\n      this.$confirm('数据删除后，不可恢复, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        container.del(ids).then(response => {\r\n          // 重新获取数据\r\n          this.getDatas()\r\n        }).catch(reject => {\r\n        })\r\n      })\r\n    },\r\n    handleUpload() {\r\n      this.upload.dialog.visible = true\r\n    },\r\n    submitUpload() {\r\n      this.$refs.upload.submit()\r\n    },\r\n    handleRemove() {\r\n      this.$refs.upload.clearFiles()\r\n    },\r\n    uploadUrl(param) {\r\n      // 先判断图片大小\r\n      const _file = param.file\r\n      // const _fileSize = _file.size < 1024 * 500\r\n      // if (!_fileSize) {\r\n      //   that.$message.warning('请上传500KB以下的文件')\r\n      //   return false\r\n      // }\r\n      console.log(_file)\r\n      // 参数\r\n      const params = new FormData()\r\n      params.append('excel', _file)\r\n      container.uploadExcel(params).then(response => {\r\n        const failureDetails = response.data.failureDetails\r\n        let message = ''\r\n        failureDetails.forEach((item) => {\r\n          message = message + '/' + item\r\n        })\r\n        this.$alert('成功：' + response.data.success + ' ' +\r\n          '失败：' + response.data.failure + ' ' +\r\n          '读取总数：' + response.data.total + ' ' +\r\n          '/' + message,\r\n        '消息提示',\r\n        {\r\n          confirmButtonText: '确定',\r\n          callback: action => {\r\n            this.upload.dialog.visible = false\r\n            this.handleRemove()\r\n            this.getDatas()\r\n          }\r\n        })\r\n      }).catch(reject => {\r\n      })\r\n    },\r\n    ExportTemplate() {\r\n      this.loading.export = true\r\n      import('@/vendor/Export2Excel').then(excel => {\r\n        const tHeader = ['编号', '名称', '类型', '所属区域', '条码', 'RFID', '是否禁用']\r\n        const data = ''\r\n        excel.export_json_to_excel({\r\n          header: tHeader,\r\n          data,\r\n          filename: '货柜'\r\n        })\r\n        this.loading.export = false\r\n      })\r\n    },\r\n    handleExport() {\r\n      this.loading.export = true\r\n      container.exportExcel(this.query, this.page, this.sort).then(response => {\r\n        const blob = new Blob([response])\r\n        const fileName = '货柜.xls'\r\n        const elink = document.createElement('a')\r\n        elink.onload = () => { URL.revokeObjectURL(elink.href) }\r\n        elink.download = fileName\r\n        elink.style.display = 'none'\r\n        elink.href = URL.createObjectURL(blob)\r\n        document.body.appendChild(elink)\r\n        elink.click()\r\n        document.body.removeChild(elink)\r\n        this.loading.export = false\r\n      }).catch(reject => {\r\n        this.loading.export = false\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n</style>\r\n"]}]}