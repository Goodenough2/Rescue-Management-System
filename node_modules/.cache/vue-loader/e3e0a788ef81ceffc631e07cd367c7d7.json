rimary\">选取文件</el-button>\r\n            <el-button style=\"margin-left: 10px;\" size=\"small\" type=\"success\" @click=\"submitUpload\">上传到服务器</el-button>\r\n            <div slot=\"tip\" class=\"el-upload__tip\">仅支持上传Excel文件</div>\r\n          </el-upload>\r\n        </el-col>\r\n      </el-row>\r\n    </el-dialog>\r\n\r\n    <el-dialog v-loading=\"loading.detail\" custom-class=\"dialog-fullscreen\" :title=\"detail.dialog.title\" :visible.sync=\"detail.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\">\r\n      <el-form ref=\"formDetail\" label-position=\"right\" :label-width=\"detail.dialog.labelWidth\">\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"容器编号\">\r\n              {{ detail.models.code }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"容器名称\">\r\n              {{ detail.models.name }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"容器类型\">\r\n              {{ detail.models.vesselType.name }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"存储级别\">\r\n              {{ detail.models.level }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"上级存储位置\">\r\n              <span v-if=\"detail.models.level == 0\">{{ '无' }}</span>\r\n              <span v-if=\"detail.models.level == 1\">{{ detail.models.parent.name }}</span>\r\n              <span v-if=\"detail.models.level == 2\">{{ detail.models.parent.name }}</span>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"容器RFID\">\r\n              {{ detail.models.rfidCode }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"容器条形码\">\r\n              {{ detail.models.barCode }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"是否允许混合使用\">\r\n              {{ detail.models.canMix === 1 ? '允许' : '不允许' }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"货物类别\">\r\n              {{ detail.models.canMix === 0 ? detail.models.goodsCategory === null?'无':detail.models.goodsCategory.name: '无' }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"是否启用\">\r\n              {{ detail.models.enabled === 1 ? '启用' : '未启用' }}\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n    </el-dialog>\r\n\r\n    <el-dialog custom-class=\"dialog-fullscreen\" :title=\"create.dialog.title\" :visible.sync=\"create.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\" :close-on-click-modal=\"false\">\r\n      <el-form ref=\"formCreate\" :rules=\"create.rules\" :model=\"create.models\" label-position=\"right\" :label-width=\"create.dialog.labelWidth\">\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"容器编号\" prop=\"code\">\r\n              <el-input v-model=\"create.models.code\" />\r\n            </el-for