th=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"容器类型\" prop=\"vesselType.name\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"存储级别\" prop=\"level\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"上级存储位置\" prop=\"parent.name\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"容器RFID\" prop=\"rfidCode\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"容器条形码\" prop=\"barCode\" align=\"center\" width=\"150\" show-overflow-tooltip />\r\n      <el-table-column label=\"是否允许混合使用\" prop=\"canMix\" align=\"center\" show-overflow-tooltip>\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.canMix===1 ? '允许' : '不允许' }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"货物类别\" prop=\"goodsCategory.name\" align=\"center\" show-overflow-tooltip />\r\n      <!--<template slot-scope=\"{row}\">-->\r\n      <!--<span>{{ row.canMix===0 ? row.goodsCategoryId : '无' }}</span>-->\r\n      <!--</template>-->\r\n      <!--</el-table-column>-->\r\n      <el-table-column label=\"启用\" align=\"center\" width=\"80\">\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.enabled===1 ? '启用' : '未启用' }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column fixed=\"right\" label=\"操作\" align=\"center\" width=\"180\">\r\n        <template slot-scope=\"{row}\">\r\n          <el-tooltip v-if=\"access.update.allow\" class=\"item\" effect=\"dark\" content=\"排序上移\" placement=\"top-end\">\r\n            <el-button type=\"primary\" plain class=\"button-operate button-rankup\" size=\"mini\" @click=\"handleRankup(row)\"><i class=\"vue-icon-rankup\" /></el-button>\r\n          </el-tooltip>\r\n          <el-tooltip v-if=\"access.detail.allow\" class=\"item\" effect=\"dark\" content=\"详情\" placement=\"top-end\">\r\n           