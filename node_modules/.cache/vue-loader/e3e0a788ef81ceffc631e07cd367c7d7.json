    </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item v-if=\"update.models.level == 1\" label=\"单元格\" prop=\"parentId\">\r\n              <el-select v-model=\"update.models.parentId\" clearable>\r\n                <el-option v-for=\"item in cells\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item v-if=\"update.models.level == 2\" label=\"容器\" prop=\"parentId\">\r\n              <el-cascader\r\n                v-model=\"update.models.parentId\"\r\n                :options=\"vessels\"\r\n                :props=\"{ expandTrigger: 'hover', checkStrictly: true }\"\r\n                @change=\"handleChange\"\r\n              />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"容器RFID\" prop=\"rfidCode\">\r\n              <el-input v-model=\"update.models.rfidCode\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"容器条形码\" prop=\"barCode\">\r\n              <el-input v-model=\"update.models.barCode\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-row>\r\n            <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n              <el-form-item label=\"是否允许混合存放\" prop=\"canMix\">\r\n                <el-switch v-model=\"update.models.canMix\" :active-value=\"1\" :inactive-value=\"0\" @change=\"handleClean\" />\r\n              </el-form-item>\r\n            </el-col>\r\n          </el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item v-if=\"update.models.canMix == 0\" label=\"货物类别\" prop=\"goodsCategoryId\">\r\n              <el-select v-model=\"update.models.goodsCategoryId\" clearable>\r\n                <el-option v-for=\"item in goodsCategories\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\r\n            <el-form-item label=\"是否启用\" prop=\"enabled\">\r\n              <el-switch v-model=\"update.models.enabled\" :active-value=\"1\" :inactive-value=\"0\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"update.dialog.visible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"updateData()\">提交</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport adaptive from '@/directive/el-table'\r\nimport setRule from '@/utils/form-validate'\r\nimport Pagination from '@/components/Pagination'\r\nimport * as vessel from '@/api/store-manage/vessel'\r\nimport * as cell from '@/api/store-manage/cell'\r\nimport * as vesselType from '@/api/store-manage/vessel-type'\r\nimport * as goodsCategory from '@/api/goods-manage/goods-category'\r\n\r\nexport default {\r\n  name: 'VesselType',\r\n  components: { Pagination },\r\n  directives: { adaptive },\r\n  data() {\r\n    return {\r\n      access: this.$store.getters.access['StoreManage']['Vessel'],\r\n      datas: null,\r\n      vesselTypes: null,\r\n      cells: null,\r\n      vessels: null,\r\n      goodsCategories: null,\r\n      fileList: null,\r\n      query: { code: null, name: null, typeId: null, level: null, canMix: null, enabled: null },\r\n      page: { total: 0, current: 1, size: 20 },\r\n      sort: { prop: 'sort', order: 'ascending' },\r\n      detail: {\r\n        dialog: { title: '容器信息', visible: false, labelWidth: '150px' },\r\n        models: { code: null, name: null, vesselType: { name: null }, level: null, parent: { name: null }, rfidCode: null, barCode: null, enabled: null, canMix: null, goodsCategory: { name: null }}\r\n      },\r\n      create: {\r\n        dialog: { title: '添加容器信息', visible: false, labelWidth: '150px' },\r\n        models: { code: null, name: null, typeId: null, level: null, parentId: null, rfidCode: null, barCode: null, enabled: 1, canMix: null, goodsCategoryId: null },\r\n        rules: {\r\n          code: setRule('容器编码', [{ selected: true }, { length: [0, 100] }]),\r\n          name: setRule('容器名称', [{ required: true }, { length: [0, 50] }]),\r\n          typeId: setRule('容器类型', [{ selected: true }]),\r\n          level: setRule('存储级别', [{ required: true }]),\r\n          parentId: setRule('上级存储位置', [{ selected: true }]),\r\n          rfidCode: setRule('容器RFID', [{ length: [0, 255] }]),\r\n          barCode: setRule('容器条形码', [{ length: [0, 255] }]),\r\n          goodsCategoryId: setRule('货物类别', [{ length: [0, 32] }])\r\n        }\r\n      },\r\n      update: {\r\n        dialog: { title: '编辑容器信息', visible: false, labelWidth: '150px' },\r\n        models: { code: null, name: null, typeId: null, level: null, parentId: null, rfidCode: null, barCode: null, enabled: null, canMix: null, goodsCategoryId: null },\r\n        rules: {\r\n          code: setRule('容器编码', [{ selected: true }, { length: [0, 100] }]),\r\n          name: setRule('容器名称', [{ required: true }, { length: [0, 50] }]),\r\n          typeId: setRule('容器类型', [{ selected: true }]),\r\n          level: setRule('存储级别', [{ required: true }]),\r\n          parentId: setRule('上级存储位置', [{ selected: true }]),\r\n          rfidCode: setRule('容器RFID', [{ length: [0, 255] }]),\r\n          barCode: setRule('容器条形码', [{ length: [0, 255] }]),\r\n          canMix: setRule('是否允许混合存放', [{ selected: true }]),\r\n          goodsCategoryId: setRule('货物类别