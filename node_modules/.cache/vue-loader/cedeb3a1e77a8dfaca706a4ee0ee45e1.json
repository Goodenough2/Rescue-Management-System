pplication-manage","sourcesContent":["<template>\n  <div class=\"app-container list\">\n    <div class=\"toolbar\">\n      <el-input v-model.trim=\"query.code\" class=\"query-item\" style=\"width: 120px\" placeholder=\"申请表编码\" clearable @clear=\"handleQuery\" />\n      <el-input v-model.trim=\"query.name\" class=\"query-item\" style=\"width: 120px\" placeholder=\"申请表编码\" clearable @clear=\"handleQuery\" />\n      <el-select v-model=\"query.status\" class=\"query-item\" style=\"width: 120px\" placeholder=\"当前状态\" clearable @clear=\"handleQuery\">\n        <el-option value=\"0\" label=\"申请中\"/>\n        <el-option value=\"1\" label=\"审批通过\" />\n        <el-option value=\"2\" label=\"审批退回\" />\n        <el-option value=\"3\" label=\"终审通过\" />\n        <el-option value=\"3\" label=\"终审退回\" />\n      </el-select>\n      <el-select v-model=\"query.applicationType\" class=\"query-item\" style=\"width: 120px\" placeholder=\"申请类型\" clearable @clear=\"handleQuery\">\n        <el-option value=\"0\" label=\"降残\"/>\n        <el-option value=\"1\" label=\"报废\" />\n      </el-select>\n      <el-button class=\"tool tool-query\" type=\"primary\" icon=\"el-icon-search\" @click=\"handleQuery\">查询</el-button>\n      <el-button class=\"tool tool-query\" type=\"primary\" icon=\"el-icon-data-line\" @click=\"handleResidual()\">申请降残</el-button>\n      <el-button class=\"tool tool-query\" type=\"primary\" icon=\"el-icon-warning-outline\" @click=\"handleScrap()\">申请报废</el-button>\n    </div>\n\n    <el-table ref=\"listTable\" v-loading=\"loading.list\" v-adaptive=\"{ bottomOffset: 55 }\" height=\"200px\" :data=\"datas\" :default-sort=\"sort\" border fit highlight-current-row @sort-change=\"handleSort\">\n      <el-table-column type=\"selection\" align=\"center\" width=\"35\" />\n      <el-table-column label=\"序号\" type=\"index\" align=\"center\" width=\"65\" fixed>\n        <template slot-scope=\"scope\">\n          <span>{{ (page.current - 1) * page.size + scope.$index + 1 }}</span>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"最近更新时间\" prop=\"applicationTime\" sortable align=\"center\" width=\"160\" show-overflow-tooltip />\n      <el-table-column label=\"申请表编码\" prop=\"code\"  align=\"center\" width=\"150\" show-overflow-tooltip />\n      <el-table-column label=\"申请表名称\" prop=\"name\"  align=\"center\" width=\"120\" show-overflow-tooltip />\n      <el-table-column label=\"申请货物条形码\" prop=\"goodsBarCode\" align=\"center\" width=\"120\" show-overflow-tooltip />\n      <el-table-column label=\"申请货物RFID码\" prop=\"goodsRfidCode\" align=\"center\" width=\"150\" show-overflow-tooltip />\n      <el-table-column label=\"申请总金额\" prop=\"amountNew\" sortable align=\"center\" width=\"120\" show-overflow-tooltip />\n      <el-table-column label=\"申请前总金额\" prop=\"amountOld\" sortable align=\"center\" show-overflow-tooltip />\n      <el-table-column label=\"当前状态\" prop=\"status\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip>\n        <template slot-scope=\"{row}\">\n          <span v-if=\"row.status == 0\">申请中</span>\n          <span v-if=\"row.status == 1\">审批通过</span>\n          <span v-if=\"row.status == 2\">审批退回</span>\n          <span v-if=\"row.status == 3\">终审通过</span>\n          <span v-if=\"row.status == 4\">终审退回</span>\n        </template>\n      </el-table-column>\n\n      <el-table-column label=\"申请类型\" prop=\"applicationType\" :sort-orders=\"sortOrders\" align=\"center\" width=\"150\" show-overflow-tooltip>\n        <template slot-scope=\"{row}\">\n          <span v-if=\"row.applicationType == 0\">降残</span>\n          <span v-if=\"row.applicationType == 1\">报废</span>\n        </template>\n      </el-table-column>\n      <el-table-column fixed=\"right\" label=\"操作\" align=\"center\" width=\"50\">\n        <template slot-scope=\"{row}\">\n          <el-tooltip class=\"item\" effect=\"dark\" content=\"详情\" placement=\"top-end\">\n            <el-button type=\"primary\" plain class=\"button-operate button-detail\" size=\"mini\" @click=\"handleDetail(row)\"><i class=\"vue-icon-detail\" /></el-button>\n          </el-tooltip>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <pagination :hidden=\"page.total==0\" :total=\"page.total\" :page.sync=\"page.current\" :limit.sync=\"page.size\" @pagination=\"getDatas\" />\n\n    <el-dialog custom-class=\"dialog-fullscreen dialog-create\" :title=\"select.dialog.title\" :visible.sync=\"select.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\" :close-on-click-modal=\"false\">\n      <div class=\"toolbar\">\n        <el-cascader\n          v-model=\"goodsQuery.multiLevelStoreId\"\n          placeholder=\"请选择存储级别\"\n          :options=\"fiveLevelStores\"\n          :props=\"{ expandTrigger: 'hover', checkStrictly: true }\"\n          clearable\n          @change=\"handleStoreChange\"\n          @clear=\"handleQuery\"\n        />\n        <el-cascader\n          v-model=\"goodsQuery.goodsCategoryId\"\n          placeholder=\"请选择货物类别\"\n          :options=\"goodsCategoryClassificationWithCategory\"\n          :props=\"{ expandTrigger: 'hover', checkStrictly: true }\"\n          clearable\n          @change=\"handleCategoryChange\"\n          @clear=\"handleQuery\"\n        />\n        <el-select v-model=\"goodsQuery.storageModeId\" class=\"query-item\" style=\"width: 120px\" placeholder=\"存储模式\" clearable @clear=\"handleQuery\">\n          <el-option v-for=\"item in goodsStorageModes\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n        </el-select>\n        <el-select v-model=\"goodsQuery.inspectorId\" class=\"query-item\" style=\"width: 120px\" placeholder=\"验货人员\" clearable @clear=\"handleQuery\">\n          <el-option v-for=\"item in operators\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\" />\n        </el-select>\n        <el-select v-model=\"goodsQuery.locationType\" class=\"query-item\" style=\"width: 150px\" placeholder=\"正常存储位置\" clearable @clear=\"handleQuery\">\n          <el-option value=\"0\" label=\"运输设备\" />\n          <el-option value=\"1\" label=\"单元格\" />\n          <el-option value=\"2\" label=\"容器\" />\n        </el-select>\n        <el-select v-model=\"goodsQuery.locked\" class=\"query-item\" style=\"width: 120px\" placeholder=\"锁定状态\" clearable @clear=\"handleQuery\">\n          <el-option value=\"0\" label=\"未锁定\" />\n          <el-option value=\"1\" label=\"部分锁定\" />\n          <el-option value=\"2\" label=\"锁定\" />\n        </el-select>\n        <el-select v-model=\"goodsQuery.inspectionResult\" class=\"query-item\" style=\"width: 120px\" placeholder=\"验货结果\" clearable @clear=\"handleQuery\">\n          <el-option value=\"0\" label=\"未通过\" />\n          <el-option value=\"1\" label=\"通过\" />\n        </el-select>\n        <el-button class=\"tool tool-query\" type=\"primary\" icon=\"el-icon-search\" @click=\"handleGoodsQuery\">查询</el-button>\n      </div>\n      <el-table ref=\"listTable\" v-loading=\"loading.list\" v-adaptive=\"{ bottomOffset: 40 }\" height=\"350px\" :data=\"goodsData\" :default-sort=\"goodsSort\" border fit highlight-current-row @sort-change=\"handleGoodsSort\">\n        <el-table-column label=\"序号\" type=\"index\" align=\"center\" width=\"65\" fixed>\n          <template slot-scope=\"scope\">\n            <span>{{ (page.current - 1) * page.size + scope.$index + 1 }}</span>\n          </template>\n        </el-table-column>\n        <el-table-column label=\"最近更新时间\" prop=\"updateTime\" sortable align=\"center\" width=\"160\" show-overflow-tooltip />\n        <el-table-column label=\"货物RFID\" prop=\"goodsRfidCode\" sortable align=\"center\" width=\"150\" show-overflow-tooltip />\n        <el-table-column label=\"货物条形码\" prop=\"goodsBarCode\" sortable align=\"center\" width=\"150\" show-overflow-tooltip />\n        <el-table-column label=\"货物类别\" prop=\"goodsCategory.name\" :sort-orders=\"goodsSortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />\n        <el-table-column label=\"货物实际总数量\" prop=\"quality\" sortable align=\"center\" width=\"100\" show-overflow-tooltip />\n        <el-table-column label=\"被锁定的数量\" prop=\"qualityLocked\" sortable align=\"center\" show-overflow-tooltip />\n        <el-table-column label=\"存储模式名称\" prop=\"goodsStoragemode.name\" :sort-orders=\"goodsSortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip />\n        <el-table-column label=\"货物存储位置类型\" prop=\"locationType\" :sort-orders=\"goodsSortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip>\n          <template slot-scope=\"{row}\">\n            <span v-if=\"row.locationType == 0\">运输设备</span>\n            <span v-if=\"row.locationType == 1\">单元格</span>\n            <span v-if=\"row.locationType == 2\">容器</span>\n          </template>\n        </el-table-column>\n        <el-table-column label=\"锁定状态\" prop=\"locked\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip>\n          <template slot-scope=\"{row}\">\n            <span v-if=\"row.locked == 0\">未锁定</span>\n            <span v-if=\"row.locked == 1\">部分锁定</span>\n            <span v-if=\"row.locked == 2\">锁定</span>\n          </template>\n        </el-table-column>\n        <el-table-column label=\"验货结果\" prop=\"inspectionResult\" :sort-orders=\"sortOrders\" align=\"center\" width=\"100\" show-overflow-tooltip>\n          <template slot-scope=\"{row}\">\n            <span v-if=\"row.inspectionResult == 0\">未通过</span>\n            <span v-if=\"row.inspectionResult == 1\">通过</span>\n          </template>\n        </el-table-column>\n        <el-table-column fixed=\"right\" label=\"操作\" align=\"center\" width=\"50\">\n          <template slot-scope=\"{row}\">\n            <el-tooltip v-if=\"judge == 0\" class=\"item\" effect=\"dark\" content=\"申请降低残值\" placement=\"top-end\">\n              <el-button type=\"primary\" plain class=\"button-operate button-update\" size=\"mini\" @click=\"selectResidualGood(row)\"><i class=\"el-icon-thumb\" /></el-button>\n            </el-tooltip>\n            <el-tooltip v-if=\"judge == 1\" class=\"item\" effect=\"dark\" content=\"申请报废\" placement=\"top-end\">\n              <el-button type=\"primary\" plain class=\"button-operate button-update\" size=\"mini\" @click=\"selectScrapGood(row)\"><i class=\"el-icon-thumb\" /></el-button>\n            </el-tooltip>\n          </template>\n        </el-table-column>\n      </el-table>\n      <pagination :hidden=\"goodsPage.total==0\" :total=\"goodsPage.total\" :page.sync=\"goodsPage.current\" :limit.sync=\"goodsPage.size\" @pagination=\"getGoods\" />\n    </el-dialog>\n    <el-dialog v-loading=\"loading.detail\" custom-class=\"dialog-fullscreen dialog-detail\" :title=\"detail.dialog.title\" :visible.sync=\"detail.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\">\n      <el-form ref=\"formDetail\" label-position=\"right\" :label-width=\"detail.dialog.labelWidth\">\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"申请时间\">\n              {{ detail.models.applicationTime }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"申请表名称\">\n              {{ detail.models.name }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"申请表编码\">\n              {{ detail.models.code }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"申请前总金额\">\n              {{ detail.models.amountOld }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"申请总金额\">\n              {{ detail.models.amountNew }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"申请类型\">\n              <span v-if=\"detail.models.applicationType==0\">降残</span>\n              <span v-if=\"detail.models.applicationType==1\">报废</span>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"当前状态\">\n              {{ detail.models.status }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"审批时间\">\n              {{ detail.models.approvalTime }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"审批结果\">\n              <span v-if=\"detail.models.approvalResult==0 && detail.models.approvalResult\">未通过</span>\n              <span v-if=\"detail.models.approvalResult==1 && detail.models.approvalResult\">通过</span>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"终级审批时间\">\n              {{ detail.models.finalApprovalTime }}\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"终级审批结果\">\n              <span v-if=\"detail.models.finalApprovalResult==0 && detail.models.finalApprovalResult\">未通过</span>\n              <span v-if=\"detail.models.finalApprovalResult==1 && detail.models.finalApprovalResult\">通过</span>\n            </el-form-item>\n          </el-col>\n        </el-row>\n      </el-form>\n    </el-dialog>\n    <el-dialog v-loading=\"loading.residual\" custom-class=\"dialog-fullscreen dialog-update\" :title=\"residual.dialog.title\" :visible.sync=\"residual.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\" :close-on-click-modal=\"false\">\n      <el-form ref=\"formUpdate\" :rules=\"residual.rules\" :model=\"residual.models\" label-position=\"right\" :label-width=\"residual.dialog.labelWidth\">\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"申请名称\" prop=\"code\">\n              <el-input v-model=\"residual.models.code\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"申请编码\" prop=\"name\">\n              <el-input v-model=\"residual.models.name\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"4\" :md=\"7\" :sm=\"15\">\n            <el-form-item label=\"货物RFID码/条形码\" prop=\"goodsCode\">\n              <el-input v-model=\"residual.models.goodsCode\" disabled=\"disabled\" />\n            </el-form-item>\n          </el-col>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-tooltip class=\"search-unit\" effect=\"dark\" content=\"搜索货物\" placement=\"top-end\">\n              <el-button type=\"primary\" plain class=\"button-operate button-detail\" circle icon=\"el-icon-search\" @click=\"handleResidualSelect()\" />\n            </el-tooltip>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"新的货物单价\" prop=\"unitPriceNew\">\n              <el-input v-model=\"residual.models.unitPriceNew\" type=\"number\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"数量\" prop=\"quality\">\n              <el-input v-model=\"residual.models.quality\" type=\"number\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"residual.dialog.visible = false\">取消</el-button>\n        <el-button type=\"primary\" @click=\"residualData()\">提交</el-button>\n      </div>\n    </el-dialog>\n    <el-dialog v-loading=\"loading.scrap\" custom-class=\"dialog-fullscreen dialog-create\" :title=\"scrap.dialog.title\" :visible.sync=\"scrap.dialog.visible\" :modal=\"false\" :modal-append-to-body=\"false\" :close-on-click-modal=\"false\">\n      <el-form ref=\"formUpdate\" :rules=\"scrap.rules\" :model=\"scrap.models\" label-position=\"right\" :label-width=\"scrap.dialog.labelWidth\">\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"申请表名称\" prop=\"code\">\n              <el-input v-model=\"scrap.models.code\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-form-item label=\"申请表编码\" prop=\"name\">\n              <el-input v-model=\"scrap.models.name\" />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :xl=\"4\" :md=\"7\" :sm=\"15\">\n            <el-form-item label=\"货物RFID码/条形码\" prop=\"goodsCode\">\n              <el-input v-model=\"scrap.models.goodsCode\" disabled=\"disabled\" />\n            </el-form-item>\n          </el-col>\n          <el-col :xl=\"6\" :md=\"12\" :sm=\"24\">\n            <el-tooltip class=\"search-unit\" effect=\"dark\" content=\"搜索货物\" placement=\"top-end\">\n              <el-button type=\"primary\" plain class=\"button-operate button-detail\" circle icon=\"el-icon-search\" @click=\"handleScrapSelect()\" />\n            </el-tooltip>\n          </el-col>\n        </el-row>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"scrap.dialog.visible = false\">取消</el-button>\n        <el-button type=\"primary\" @click=\"scrapData()\">提交</el-button>\n      </div>\n    </el-dialog>\n\n  </div>\n</template>\n\n<script>\nimport adaptive from '@/directive/el-table'\nimport setRule from '@/utils/form-validate'\nimport Pagination from '@/components/Pagination'\nimport * as application from '@/api/application-manage/residual-application'\nimport * as operator from '@/api/system-manage/user'\nimport * as goodsCategoryClassification from '@/api/goods-manage/goods-category-classification'\nimport * as store from '@/api/store-manage/store'\nimport * as goodsStorageMode from '@/api/goods-manage/goods-storageMode'\nimport * as goods from '@/api/goods-manage/goods'\n\nexport default {\n  name: 'ResidualApplication',\n  components: { Pagination },\n  directives: { adaptive },\n  data() {\n    return {\n      access: this.$store.getters.access['ApplicationManage']['ResidualApplication'],\n      datas: null,\n      goodsData: null,\n      fiveLevelStores: null,\n      operators: null,\n      goodsCategoryClassificationWithCategory: null,\n      storeOperateTypes: null,\n      goodsStorageModes: null,\n      goodTemp: null,\n      goodQuality: null,\n      goodUnitPriceNew: null,\n      judge: 1,\n      query: { applicationTime: null, amountOld: null, amountNew: null, applicationType: null, approvalResult: null, approvalTime: null, approverId: null, code: null, finalApprovalResult: null, finalApprovalTime: null, finalApproverId: null, name: null, status: null },\n      goodsQuery: { multiLevelStoreId: null, goodsCategoryId: null, storageModeId: null, locationType: null, locked: null, inspectionResult: null, inspectorId: null },\n      page: { total: 0, current: 1, size: 20 },\n      goodsPage: { total: 0, current: 1, size: 20 },\n      sort: { prop: 'sort', order: 'ascending' },\n      goodsSort: { prop: 'sort', order: 'ascending' },\n      detail: {\n        dialog: { title: '申请信息', visible: false, labelWidth: '150px' },\n        models: { applicationTime: null, amountOld: null, amountNew: null, applicationType: null, approvalResult: null, approvalTime: null, approverId: null, code: null, finalApprovalResult: null, finalApprovalTime: null, finalApproverId: null, name: null, status: null }\n      },\n      select: {\n        dialog: { title: '货物信息', visible: false, labelWidth: '120px' }\n      },\n      scrap: {\n        dialog: { title: '申请报废', visible: false, labelWidth: '150px' },\n        models: { code: null, name: null, quality: null, unitPriceNew: null, applicationType: null, goodsCode: null },\n        rules: {\n          code: setRule('申请表编码', [{ required: true }, { length: [0, 20] }]),\n          name: setRule('申请表名称', [{ required: true }, { length: [0, 20] }]),\n          goodsCode: setRule('货物RFID码/条形码', [{ required: true }, { length: [0, 20] }])\n        }\n      },\n      residual: {\n        dialog: { title: '申请降低残值', visible: false, labelWidth: '150px' },\n        models: { code: null, name: null, quality: null, unitPriceNew: null, applicationType: null, goodsCode: null },\n        rules: {\n          code: setRule('申请表编码', [{ required: true }, { length: [0, 20] }]),\n          name: setRule('申请表名称', [{ required: true }, { length: [0, 20] }]),\n          unitPriceNew: setRule('新的货物价格', [{ required: true }, { length: [0, 20] }]),\n          goodsCode: setRule('货物RFID码/条形码', [{ required: true }, { length: [0, 20] }]),\n          quality: setRule('数量', [{ required: true }])\n        }\n      },\n      loading: { list: true, export: false, detail: false, residual: false, select: false, scrap: false, goodsCode: null },\n      sortOrders: ['ascending', 'descending'],\n      goodsSortOrders: ['ascending', 'descending']\n    }\n  },\n  created() {\n    this.getDatas()\n    this.getOperators()\n    this.getGoodsCategoryClassificationWithCategory()\n    this.getFiveLevelStores()\n    this.getGoodsStorageModes()\n    this.getGoods()\n  },\n  methods: {\n    getDatas() {\n      this.loading.list = true\n      application.getList(this.query, this.page, this.sort).then(response => {\n        this.datas = response.data.items\n        this.page.total = response.data.total\n        this.loading.list = false\n      }).catch(reject => {\n        this.loading.list = false\n      })\n    },\n    getGoods() {\n      this.loading.list = true\n      goods.getList(this.goodsQuery, this.goodsPage, this.goodsSort).then(response => {\n        this.goodsData = response.data.items\n        this.goodsPage.total = response.data.total\n        this.loading.list = false\n      }).catch(reject => {\n        this.loading.list = false\n      })\n    },\n    getGoodsStorageModes() {\n      goodsStorageMode.getlist().then(response => {\n        this.goodsStorageModes = response.data\n      }).catch(reject => {\n      })\n    },\n    getFiveLevelStores() {\n      store.getFiveLevelStores().then(response => {\n        this.fiveLevelStores = response.data\n      }).catch(reject => {\n      })\n    },\n    getGoodsCategoryClassificationWithCategory() {\n      goodsCategoryClassification.getlist(2).then(response => {\n        this.goodsCategoryClassificationWithCategory = response.data\n      }).catch(reject => {\n      })\n    },\n    getOperators() {\n      operator.getlist().then(response => {\n        this.operators = response.data\n      }).catch(reject => {\n      })\n    },\n    handleStoreChange(value) {\n      const id = value[value.length - 1]\n      this.query.multiLevelStoreId = id\n    },\n    handleCategoryChange(value) {\n      const id = value[value.length - 1]\n      this.query.goodsCategoryId = id\n    },\n    handleQuery() {\n      this.page.current = 1\n      this.getDatas()\n    },\n    handleGoodsQuery() {\n      this.page.current = 1\n      this.getGoods()\n    },\n    handleSort(data) {\n      this.sort.prop = data.prop\n      this.sort.order = data.order\n      this.getDatas()\n    },\n    handleGoodsSort(data) {\n      this.goodsSort.prop = data.prop\n      this.goodsSort.order = data.order\n      this.getDatas()\n    },\n    handleDetail(row) {\n      // 若列表数据展示了全部属性，则可直接拷贝列表数据\n      this.detail.models = Object.assign({}, row)\n      this.detail.dialog.visible = true\n      this.loading.detail = true\n      // api.get(row.id).then(response => {\n      //   this.detail.models = response.data\n      //   this.loading.detail = false\n      // }).catch(reject => {\n      //   this.loading.detail = false\n      // })\n      this.loading.detail = false\n    },\n    handleResidualSelect() {\n      this.select.dialog.visible = true\n      this.loading.detail = true\n      this.judge = 0\n      // api.get(row.id).then(response => {\n      //   this.detail.models = response.data\n      //   this.loading.detail = false\n      // }).catch(reject => {\n      //   this.loading.detail = false\n      // })\n      this.loading.select = false\n    },\n    handleScrapSelect() {\n      this.select.dialog.visible = true\n      this.loading.detail = true\n      this.judge = 1\n      this.loading.select = false\n    },\n    handleResidual() {\n      //  若列表数据展示了全部属性，则可直接拷贝列表数据用于编辑\n      this.residual.dialog.visible = true\n      this.loading.residual = true\n      // goodsType.get(row.id).then(response => {\n      //   this.update.models = response.data\n      //   this.loading.update = false\n      // }).catch(reject => {\n      //   this.loading.update = false\n      // })\n      this.$nextTick(() => {\n        this.$refs['formUpdate'].clearValidate()\n      })\n      this.loading.residual = false\n    },\n    residualData() {\n      this.$refs['formUpdate'].validate((valid) => {\n        if (!valid) return false\n        this.residual.models.applicationType = 0\n        this.goodQuality = this.residual.models.quality\n        this.goodUnitPriceNew = this.residual.models.unitPriceNew\n        const tempData = Object.assign({}, this.residual.models)\n        application.create(tempData, this.goodTemp, this.goodQuality, this.goodUnitPriceNew).then(response => {\n          // 重新获取数据\n          this.getDatas()\n          this.residual.dialog.visible = false\n        }).catch(reject => {\n        })\n      })\n    },\n    handleScrap(row) {\n      //  若列表数据展示了全部属性，则可直接拷贝列表数据用于编辑\n      this.scrap.models = Object.assign({}, row)\n      this.scrap.dialog.visible = true\n      this.scrap.residual = true\n      // goodsType.get(row.id).then(response => {\n      //   this.update.models = response.data\n      //   this.loading.update = false\n      // }).catch(reject => {\n      //   this.loading.update = false\n      // })\n      this.goodTemp = row\n      this.$nextTick(() => {\n        this.$refs['formUpdate'].clearValidate()\n      })\n      this.loading.scrap = false\n    },\n    scrapData() {\n      this.$refs['formUpdate'].validate((valid) => {\n        if (!valid) return false\n        this.scrap.models.applicationType = 1\n        const tempData = Object.assign({}, this.scrap.models)\n        application.create(tempData, this.goodTemp).then(response => {\n          // 重新获取数据\n          this.getDatas()\n          this.scrap.dialog.visible = false\n        }).catch(reject => {\n        })\n      })\n    },\n    selectResidualGood(row) {\n      this.goodTemp = row\n      this.residual.models.goodsCode = row.goodsBarCode !== null ? row.goodsBarCode : row.goodsRfidCode\n      this.select.dialog.visible = false\n    },\n    selectScrapGood(row) {\n      this.goodTemp = row\n      this.scrap.models.goodsCode = row.goodsBarCode !== null ? row.goodsBarCode : row.goodsRfidCode\n      this.select.dialog.visible = false\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n  .search-unit {\n    position: relative;\n    top: 10px;\n    left: 4px;\n    /*right: 1000px;*/\n  }\n</style>\n\n"]}]}